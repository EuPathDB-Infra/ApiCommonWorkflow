# default configuration for a genomeAnalysis flow


######### Splign ##############################

 ApiCommonData/Load/Workflow/Steps/RunSplign.splignPath = /files/cbil/software/splign


######### Psipred ######################################

ApiCommonData/Load/Workflow/Steps/CreatePsipredDirWithFormattedDb.psipredPath = /files/software/psipred/bin
ApiCommonData/Load/Workflow/Steps/CreatePsipredDirWithFormattedDb.ncbiBlastPath = $NCBI_BLAST_PATH

ApiCommonData/Load/Workflow/Steps/CreatePsipredTaskDir.taskSize = 1
ApiCommonData/Load/Workflow/Steps/CreatePsipredTaskDir.clusterpath = /genomics/share/src/bio/psipred/current
ApiCommonData/Load/Workflow/Steps/CreatePsipredTaskDir.ncbibin = /genomics/share/src/bio/ncbi/bin

ApiCommonData/Load/Workflow/Steps/MakeAlgInv.psipredVersion = 2.5


####### TMHMM ######################################

ApiCommonData/Load/Workflow/Steps/RunTmhmm.path = /files/cbil/software/TMHMM/current/bin/tmhmm
ApiCommonData/Load/Workflow/Steps/RunTmhmm.version = 2.0a 

####### BLAST ######################################

ApiCommonData/Load/Workflow/Steps/CreateSimilarityDir.taskSize=1
ApiCommonData/Load/Workflow/Steps/CreateSimilarityDir.wuBlastBinPathCluster = /genomics/share/pkg/bio/wu-blast/current


