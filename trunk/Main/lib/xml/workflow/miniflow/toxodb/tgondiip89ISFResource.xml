<workflowGraph name="tgondiip89GenomeISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/toxoGenomeResourceISFResource</constant>

    <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="Tgondiip89_genome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tgondiip89_genome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

<subgraph name="Tgondiip89_contigs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tgondiip89_contigs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Tgondiip89_genome_RSRC"/>
  </subgraph>

</workflowGraph>



