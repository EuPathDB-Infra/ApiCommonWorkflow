<workflowGraph name="lmajorRnaSeqAndChipSeq">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="ssaGeneModelFile"/>
  <param name="genomicSeqsFileForSsa"/>
  <param name="transcriptSeqsFileForSsa"/>
  <param name="samHeaderFile"/>
  <param name="downloadSiteTranscriptExpressionDataDir"/>
  <param name="genomeExtDbRlsSpec"/>

  <constant name="dataDir">$$parentDataDir$$/lmajorRnaSeqAndChipSeq</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="getAndAnalyzeMylerSplicedLeaderAndPolyASites" xmlFile="tritrypdb/lmajorMylerSplicedLeaderAndPolyASites.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>

