<workflowGraph name="lmajorFriedlinISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/lmajorFriedlinISFResource</constant>

    <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="LmajorFriedlin_chromosomes_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">LmajorFriedlin_chromosomes_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="LmajorFriedlin_annotations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">LmajorFriedlin_annotations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="LmajorFriedlin_chromosomes_RSRC"/>
  </subgraph>

</workflowGraph>
