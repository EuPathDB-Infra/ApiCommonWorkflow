<workflowGraph name="lmajor_sanger_PACEnds">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/lmajor_sanger_PACEnds</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="Lmajor_sanger_PACEnds_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Lmajor_sanger_PACEnds_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Lmajor_sanger_PACEnds_additional_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Lmajor_sanger_PACEnds_additional_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="Lmajor_sanger_PACEnds_RSRC"/>
  </subgraph>

</workflowGraph>
