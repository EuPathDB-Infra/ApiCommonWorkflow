<workflowGraph name="lbraziliensisDbxRefsResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>
      <param name="projectVersion"/>
      <param name="projectName"/>
      <param name="downloadSiteDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/lbraziliensisDbxRefsResources</constant>
  <constant name="geneAliasesMappingDownloadFile">$$downloadSiteDataDir$$/Lbraziliensis/txt/LbraziliensisGeneAlias_$$projectName$$-$$projectVersion$$.txt</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <step name="makeGeneAliasesMappingFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeGeneAliasesMappingFile">
     <paramValue name="genomeExtDbRlsSpec">Lbraziliensis_chromosomes_RSRC|@@Lbraziliensis_chromosomes_VER@@</paramValue>
     <paramValue name="dbRefNAFeatureExtDbSpec">Lbraziliensis_previds_dbxref_RSRC|@@Lbraziliensis_chromosomes_VER@@</paramValue>
     <paramValue name="outputFile">$$geneAliasesMappingDownloadFile$$</paramValue>
  </step>

  <subgraph name="Lbraziliensis_Entrez_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Lbraziliensis_Entrez_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Lbraziliensis_Uniprot_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Lbraziliensis_Uniprot_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
