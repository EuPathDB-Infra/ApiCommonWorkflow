<workflowGraph name="tbruceigambienseSpecificAnalysis">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>

  <constant name="dataDir">$$parentDataDir$$/TbruceigambienseSpecific</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/TbruceigambienseLoadGenome/genomicSeqs.fsa</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="GOAssociations_Tbruceigambiense_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_Tbruceigambiense_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GO_RSRC"/>
      <dependsGlobal name="GO_evidence_codes_RSRC"/>
    </subgraph>

    <subgraph name="Tbruceigambiense_Comment_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tbruceigambiense_Comment_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Tbruceigambiense_GeneName_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tbruceigambiense_GeneName_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Tbruceigambiense_ECNumbers_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tbruceigambiense_ECNumbers_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDB_RSRC"/>
    </subgraph>

    <subgraph name="Tbruceigambiense_pmid_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tbruceigambiense_pmid_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>


</workflowGraph>



