<workflowGraph name="pchabaudiDbxRefsResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>
      <param name="projectVersion"/>
      <param name="projectName"/>
      <param name="downloadSiteDir"/>


  <constant name="dataDir">$$parentDataDir$$/pchabaudiDbxRefsResources</constant>
  <constant name="euPathUniProtLinkDownloadFile">$$downloadSiteDir$$/PchabaudiEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</constant>
  <constant name="geneAliasesMappingDownloadFile">$$downloadSiteDir$$/txt/PchabaudiGeneAlias_$$projectName$$-$$projectVersion$$.txt</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <step name="makeGeneAliasesMappingFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeGeneAliasesMappingFile">
     <paramValue name="genomeExtDbRlsSpec">Pchabaudi_chromosomes_RSRC|@@Pchabaudi_chromosomes_VER@@</paramValue>
     <paramValue name="outputFile">$$geneAliasesMappingDownloadFile$$</paramValue>
  </step>

  <subgraph name="Pc_Phenotype_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Phenotype_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pc_Pubmed_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Pubmed_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pc_Entrez_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Entrez_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pc_Uniprot_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Uniprot_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <step name="makeEuPathUniProtLinkFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeEuPathUniProtLinkFile">
      <paramValue name="dbrefExtDbRlsSpec">Pc_Uniprot_Gene_dbxref_RSRC|@@Pc_Uniprot_Gene_dbxref_VER@@</paramValue>
      <paramValue name="outputFile">$$euPathUniProtLinkDownloadFile$$</paramValue>
      <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="Pc_Uniprot_Gene_dbxref_RSRC"/>
    </step>

  <subgraph name="Pc_Winzeler_Westenberger_Dbrefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Winzeler_Westenberger_Dbrefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pc_Winzeler_Literature_References_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Winzeler_Literature_References_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


  <subgraph name="Pc_ApiLoc_EupathDB_Dbrefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_ApiLoc_EupathDB_Dbrefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>
</workflowGraph>
