<workflowGraph name="pbergheiDbxRefsResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>
      <param name="projectVersion"/>
      <param name="projectName"/>
      <param name="downloadSiteDir"/>

  <constant name="dataDir">$$parentDataDir$$/pbergheiDbxRefsResources</constant>
  <constant name="euPathUniProtLinkDownloadFile">$$downloadSiteDir$$/PbergheiEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</constant>
  <constant name="geneAliasesMappingDownloadFile">$$downloadSiteDir$$/txt/PbergheiGeneAlias_$$projectName$$-$$projectVersion$$.txt</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <step name="makeGeneAliasesMappingFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeGeneAliasesMappingFile">
     <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@@Pberghei_chromosomes_VER@@</paramValue>
     <paramValue name="outputFile">$$geneAliasesMappingDownloadFile$$</paramValue>
  </step>

  <subgraph name="Pb_RMgm_Phenotype_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_RMgm_Phenotype_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pb_Pubmed_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_Pubmed_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pb_Entrez_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_Entrez_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pb_Uniprot_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_Uniprot_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <step name="makeEuPathUniProtLinkFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeEuPathUniProtLinkFile">
      <paramValue name="dbrefExtDbRlsSpec">Pb_Uniprot_Gene_dbxref_RSRC|@@Pb_Uniprot_Gene_dbxref_VER@@</paramValue>
      <paramValue name="outputFile">$$euPathUniProtLinkDownloadFile$$</paramValue>
      <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="Pb_Uniprot_Gene_dbxref_RSRC"/>
    </step>

  <subgraph name="Pb_Winzeler_Westenberger_Dbrefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_Winzeler_Westenberger_Dbrefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
  </subgraph>

  <subgraph name="Pb_Winzeler_Literature_References_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_Winzeler_Literature_References_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


   <subgraph name="Pb_ApiLoc_EupathDB_Dbrefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_ApiLoc_EupathDB_Dbrefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>
</workflowGraph>
