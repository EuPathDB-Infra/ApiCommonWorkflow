<workflowGraph name="pyoeliiGeneExpressionResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>
      <param name="oligoExtDbRlsSpec"/>
      <param name="genomeExtDbRlsSpec"/>
      <param name="genomicSeqsFile"/>

  <subgraph name="VaidyaBergman_oligoResource" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Vaidya_Bergman_oligos</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
  </subgraph>

  <subgraph name="mapDeRisiOligosToGenomicSeqs" xmlFile="mapOligos.xml">
    <paramValue name="parentDataDir">$$parentDataDir$$/Vaidya_Bergman_oligos</paramValue>
    <paramValue name="oligoExtDbRlsSpec">$$oligoExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="idRegex">(\\S+)</paramValue>
    <paramValue name="loadOptions">--hspMatchLength 59 --noHsps --hspPctIdent 95 --hspsLimit 1</paramValue>
    <depends name="VaidyaBergman_oligoResource"/>
  </subgraph>

  <subgraph name="Kappe_expressionProfilesResource" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Kappe_expressionProfiles</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
  </subgraph>
</workflowGraph>
