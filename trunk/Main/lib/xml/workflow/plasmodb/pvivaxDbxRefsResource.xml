<workflowGraph name="pvivaxDbxRefsResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>
      <param name="projectVersion"/>
      <param name="projectName"/>
      <param name="downloadSiteDir"/>

  <constant name="dataDir">$$parentDataDir$$/pvivaxDbxRefsResources</constant>
  <constant name="euPathUniProtLinkDownloadFile">$$downloadSiteDir$$/txt/PvivaxEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</constant>
  <constant name="geneAliasesMappingDownloadFile">$$downloadSiteDir$$/txt/PvivaxGeneAlias_$$projectName$$-$$projectVersion$$.txt</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <step name="makeGeneAliasesMappingFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeGeneAliasesMappingFile">
     <paramValue name="genomeExtDbRlsSpec">Pvivax_contigsGB_RSRC|@@Pvivax_contigsGB_VER@@</paramValue>
     <paramValue name="outputFile">$$geneAliasesMappingDownloadFile$$</paramValue>
  </step>

  <subgraph name="Pv_Phenotype_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_Phenotype_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pv_Pubmed_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_Pubmed_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pv_Entrez_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_Entrez_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pv_Uniprot_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_Uniprot_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <step name="makeEuPathUniProtLinkFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeEuPathUniProtLinkFile">
      <paramValue name="dbrefExtDbRlsSpec">Pv_Uniprot_Gene_dbxref_RSRC|@@Pv_Uniprot_Gene_dbxref_VER@@</paramValue>
      <paramValue name="outputFile">$$euPathUniProtLinkDownloadFile$$</paramValue>
      <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="Pv_Uniprot_Gene_dbxref_RSRC"/>
    </step>

  <subgraph name="Pvivax_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pvivax_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pvivax_sequenceGB_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pvivax_sequenceGB_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pv_ApiLoc_EupathDB_Dbrefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_ApiLoc_EupathDB_Dbrefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pv_RMgm_Phenotype_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_RMgm_Phenotype_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
