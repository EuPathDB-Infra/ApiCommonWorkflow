<workflowGraph name="pchabaudiISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/pchabaudiISFResource</constant>

    <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="Pchabaudi_chromosomes_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pchabaudi_chromosomes_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pchabaudi_annotations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pchabaudi_annotations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Pchabaudi_chromosomes_RSRC"/>
  </subgraph>

</workflowGraph>
