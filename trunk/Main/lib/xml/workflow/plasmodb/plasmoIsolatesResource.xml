<workflowGraph name="plasmoIsolatesResource">
      <param name="commonResourceXmlFile"/>
      <param name="globalResourceXmlFile"/>
      <param name="parentDir"/>

  <constant name="dataDir">$$parentDir$$/plasmoIsolatesResources</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="PlasmodiumGenbankIsolates_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">PlasmodiumGenbankIsolates_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$commonResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="isolateVocabulary_RSRC" xmlFile="loadIsolateVocabularyResource.xml">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
    <paramValue name="commonResourceXmlFile">$$commonResourceXmlFile$$</paramValue>
    <paramValue name="globalResourceXmlFile">$$globalResourceXmlFile$$</paramValue>
    <depends name="PlasmodiumGenbankIsolates_RSRC"/>
  </subgraph>

</workflowGraph>
