      <paramValue name="sampleName">**NAME**</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/**rnaSeqExtDbName**/final/**NAME**.fsq</paramValue>
