      <paramValue name="outputIntensityFile">$$samplesDataDir$$/**NAME**.int</paramValue>
      <paramValue name="makeIntensityFile">**makeIntensityFile**</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">**rnaSeqExtDbRlsSpec**</paramValue>
      <paramValue name="genomeExtDbSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="pairedReadFilePath">**pairedReadFilePath**</paramValue>
      <paramValue name="numInsertions">**numInsertions**</paramValue>
      <paramValue name="limitNU">**limitNU**</paramValue>
      <paramValue name="samHeaderFile">$$samHeaderFile$$</paramValue>
      <depends name="mirrorToCluster"/>
