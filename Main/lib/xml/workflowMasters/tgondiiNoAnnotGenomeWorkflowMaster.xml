   <templateInstance name="Gniphandrodes" xmlFile="genomeUnannotatedTemplate.xml" rtcFile="toxoGenomeNoAnnot.rtc"/>

   <templateInstance name="TgondiiFOU" xmlFile="genomeUnannotatedTemplate.xml" rtcFile="toxoGenomeNoAnnot.rtc"/>

   <templateInstance name="TgondiiMAS" xmlFile="genomeUnannotatedTemplate.xml" rtcFile="toxoGenomeNoAnnot.rtc"/>

   <templateInstance name="TgondiiRUB" xmlFile="genomeUnannotatedTemplate.xml" rtcFile="toxoGenomeNoAnnot.rtc"/>

   <templateInstance name="TgondiiVAND" xmlFile="genomeUnannotatedTemplate.xml" rtcFile="toxoGenomeNoAnnot.rtc"/>

   <templateInstance name="Tgondiip89" xmlFile="genomeUnannotatedTemplate.xml" rtcFile="toxoGenomeNoAnnot.rtc"/>


</workflowGraph>

