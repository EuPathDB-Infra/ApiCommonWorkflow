<workflowGraph name="">
  <param name="parentDataDir"/>
  <param name="organismAbbrev"/>
  <param name="projectName"/>
  <param name="experimentResourceName"/>
  <param name="chipProbeExtDbRlsSpec"/>
  <param name="genomeExtDbRlsSpec"/>
  <param name="peakFinderArgs"/>
  <param name="loadPeakCalls"/>
  <param name="configFilesDir"/>


  <datasetTemplate class="chipChipSample">
    <prop name="sampleName"/>
    <prop name="experimentName"/>
    <prop name="LifeCycleStage"/>
    <prop name="Antibody"/>
    <prop name="Genotype"/>
    <prop name="Replicate"/>
    <prop name="Strain"/>
    <prop name="Treatment"/>
    <prop name="CellType"/>

    <subgraph name="${sampleName}_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">$$organismAbbrev$$_${experimentName}_${sampleName}_chipChipSample_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$projectName$$/${organismAbbrev}/${experimentName}.xml</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>

    <subgraph name="${sampleName}" xmlFile="analyzeChipChipSample.xml">
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
      <paramValue name="experimentResourceName">$$experimentResourceName$$</paramValue>
      <paramValue name="chipProbeExtDbRlsSpec">$$chipProbeExtDbRlsSpec$$</paramValue>
      <paramValue name="inputSampleFile">${sampleName}.txt</paramValue>
      <paramValue name="sampleName">${sampleName}</paramValue>
      <paramValue name="experimentName">${experimentName}</paramValue>
      <paramValue name="LifeCycleStage">${LifeCycleStage}</paramValue>
      <paramValue name="Antibody">${Antibody}</paramValue>
      <paramValue name="Genotype">${Genotype}</paramValue>
      <paramValue name="Replicate">${Replicate}</paramValue>
      <paramValue name="Strain">${Strain}</paramValue>
      <paramValue name="Treatment">${Treatment}</paramValue>
      <paramValue name="CellType">${CellType}</paramValue>
      <paramValue name="peakFinderArgs">$$peakFinderArgs$$</paramValue>
      <paramValue name="loadPeakCalls">$$loadPeakCalls$$</paramValue>
      <paramValue name="organismAbbrev">$$organismAbbrev$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="configFilesDir">$$configFilesDir$$</paramValue>
      <depends name="${sampleName}_RSRC"/>
    </subgraph>

  </datasetTemplate>


</workflowGraph>
