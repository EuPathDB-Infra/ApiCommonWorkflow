 <workflowGraph name="studyMetadata">
  <param name="parentDataDir"/>
  <param name="datasetLoaderXmlFileName"/>

  <datasetTemplate class="isolateQualifierToVocabMapping">
    <prop name="qualifierType"/>

    <subgraph name="isolateVocabularyMap_${qualifierType}_RSRC" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">isolateVocabularyMap_${qualifierType}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    </subgraph>
  </datasetTemplate>


  <datasetTemplate class="proteinMicroarrayMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>

    <subgraph name="proteinMicroarrayMetaData_${organismAbbrev}_${experimentName}" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>

  </datasetTemplate>

  <datasetTemplate class="SnpMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>

    <subgraph name="SNPMetaData_${organismAbbrev}_${experimentName}" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>
  </datasetTemplate>

  <datasetTemplate class="SNPChipMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>

    <subgraph name="SNPChipMetaData_${organismAbbrev}_${experimentName}" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>

  </datasetTemplate>

  <datasetTemplate class="chipChipMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>


    <subgraph name="ChipChipMetaData_${organismAbbrev}_${experimentName}" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>

  </datasetTemplate>

  <datasetTemplate class="copyNumberVariationMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>


    <subgraph name="CnvMetaData_${organismAbbrev}_${experimentName}" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>


  </datasetTemplate>

</workflowGraph>
