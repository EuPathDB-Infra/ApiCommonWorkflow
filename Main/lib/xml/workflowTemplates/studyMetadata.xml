 <workflowGraph name="studyMetadata">
  <param name="parentDataDir"/>
  <param name="datasetLoaderXmlFileName"/>

  <datasetTemplate class="isolateQualifierToVocabMapping">
    <subgraph name="isolateVocabularyMap_RSRC" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">isolateVocabularyMap_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    </subgraph>
  </datasetTemplate>


  <datasetTemplate class="proteinMicroarrayMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>

    <subgraph name="proteinMicroarrayMetaData_$$experimentName$$" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>

  </datasetTemplate>

  <datasetTemplate class="SnpMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>

    <subgraph name="SNPMetaData_$$experimentName$$" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>
  </datasetTemplate>

  <datasetTemplate class="SNPChipMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>

    <subgraph name="SNPChipMetaData_$$experimentName$$" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>

  </datasetTemplate>

  <datasetTemplate class="chipChipMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>


    <subgraph name="ChipChipMetaData_$$experimentName$$" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>

  </datasetTemplate>

  <datasetTemplate class="copyNumberVariationMetaData">
    <prop name="organismAbbrev"/>
    <prop name="experimentName"/>
    <prop name="experimentVersion"/>


    <subgraph name="CnvMetaData_$$experimentName$$" xmlFile="loadDataset.xml">
      <paramValue name="datasetName">${organismAbbrev}_metadata_${experimentName}_RSRC</paramValue>
      <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    </subgraph>


  </datasetTemplate>

</workflowGraph>
