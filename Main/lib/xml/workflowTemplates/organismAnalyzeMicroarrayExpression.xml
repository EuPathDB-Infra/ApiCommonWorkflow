<workflowGraph name="organismAnalyzeMicroarrayExpressionTemplate">


  <subgraph name="**NAME**_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">**NAME**_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="**probeSet**_getAndMapProbes"/>
  </subgraph>

  <subgraph name="**NAME**_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">**NAME**</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">**probeSet**_RSRC|@@**probeSet**_VER@@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">**NAME**_RSRC|@@**NAME**_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="insertAnalysisResult">**insertAnalysisResult**</paramValue>
    <paramValue name="probeGenesMappingFile">$$dataDir$$/getAndMap_**probeSet**_RSRC/geneProbeMapping.txt</paramValue>
    <paramValue name="profileSetNames">**profileSetNames**</paramValue>
    <paramValue name="profileSetSpecs">**profileSetSpecs**</paramValue>
    <paramValue name="isLogged">**isLogged**</paramValue>
    <paramValue name="mappingFileForExpressionStats">**mappingFileForExpressionStats**</paramValue>
    <paramValue name="calculateExpressionStats">**calculateExpressionStats**</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteTranscriptExpressionDataDir$$/**NAME**</paramValue>
    <depends name="**NAME**_RSRC"/>
  </subgraph>


</workflowGraph>
