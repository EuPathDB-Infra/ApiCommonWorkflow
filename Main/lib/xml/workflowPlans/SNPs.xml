<workflowGraph name="">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="organismFullName"/>
  <param name="genomicSeqsFile"/>
  <param name="genomeExtDbRlsSpec"/>

  <constant name="dataDir">$$parentDataDir$$/SNPs</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <datasetTemplate class="SNPs_GFF">
    <prop name="name"/>
    <prop name="version"/>
    <prop name="strainAbbrev"/>
    <subgraph name="$$orgAbbrev$$_SNP_$$name$$_gff_RSRC" xmlFile="getAndAnalyzeSNPs.xml">
      <paramValue name="snpExtDbRlsSpec">$$orgAbbrev$$_SNP_$$name$$_gff_RSRC|${version}</paramValue>
      <paramValue name="snpResourceName">$$orgAbbrev$$_SNP_$$name$$_gff_RSRC</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismFullName">$$organismFullName$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="referenceStrainAbbrev">${strainAbbrev}</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
  </datasetTemplate>


</workflowGraph>
