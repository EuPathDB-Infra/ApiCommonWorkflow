<workflowGraph name="tgondiiME49SpecificAnalysis">
  <param name="parentDataDir"/>
  <param name="organismName"/>
  <param name="organismFullName"/>
  <param name="genomeExtDbRlsSpec"/>
  <param name="maxIntronSize"/>
  <param name="ncbiTaxonId"/>
  <param name="BACEndsExtDbRlsSpec"/>
  <param name="cosmidEndsExtDbRlsSpec"/>
  <param name="repeatMaskSpecies"/>

  <constant name="dataDir">$$parentDataDir$$/$$organismName$$/SpecificAnalysis</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/$$organismName$$/genome/genomicSeqs.fsa</constant>
  <constant name="BACEndsDir">$$dataDir$$/extractAndBlockDnaSeq</constant>
  <constant name="cosmidEndsDir">$$dataDir$$/extractAndBlockCosmidEnds</constant>


  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <datasetClass name="isolates">
    <subgraph name="mapIsolatesToGenome" xmlFile="mapIsolatesToGenome.xml">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
      <paramValue name="organismName">$$organismName$$</paramValue>
      <paramValue name="isolatesExtDbRlsSpec">$$genbankIsolateExtDbRlsSpec$$</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    </subgraph>
  </datasetClass>

  <subgraph name="arrayResources" xmlFile="plasmodb/pfalciparumArrayStudies.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


  <subgraph name="extractAndBlockDnaSeq" xmlFile="extractAndBlockDnaSeq.xml">
    <paramValue name="dataDir">$$BACEndsDir$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="BACEndsExtDbRlsSpec">$$BACEndsExtDbRlsSpec$$</paramValue>
    <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
    <paramValue name="repeatMaskSpecies">$$repeatMaskSpecies$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


  <subgraph name="extractAndBlockCosmidEnds" xmlFile="extractAndBlockDnaSeq.xml">
    <paramValue name="dataDir">$$cosmidEndsDir$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="BACEndsExtDbRlsSpec">$$cosmidEndsExtDbRlsSpec$$</paramValue>
    <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
    <paramValue name="repeatMaskSpecies">$$repeatMaskSpecies$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


  <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
    <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
    <depends name="extractGenbankIsolateSeqs"/>
    <depends name="extractSibleyIsolateSeqs"/>
  </step>

  <subgraph name="mapBACEndsSeqsToGenome" xmlFile="blatDnaSeqAgainstGenome.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
    <paramValue name="targetNcbiTaxonId">$$ncbiTaxonId$$</paramValue>
    <paramValue name="targetExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="targetSeqsDir">$$parentDataDir$$/$$organismName$$/genome/mapAssemblySeqs/genomicSeqs</paramValue>
    <paramValue name="queryNcbiTaxonId">$$ncbiTaxonId$$</paramValue>
    <paramValue name="queryFile">$$BACEndsDir$$/master/mainresult/blocked.seq</paramValue>
    <paramValue name="queryTable">ExternalNASequence</paramValue>
    <paramValue name="queryExtDbRlsSpec">$$BACEndsExtDbRlsSpec$$</paramValue>
    <depends name="extractAndBlockDnaSeq"/>
  </subgraph>

  <subgraph name="mapCosmidEndsSeqsToGenome" xmlFile="blatDnaSeqAgainstGenome.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
    <paramValue name="targetNcbiTaxonId">$$ncbiTaxonId$$</paramValue>
    <paramValue name="targetExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="targetSeqsDir">$$parentDataDir$$/$$organismName$$/genome/mapAssemblySeqs/genomicSeqs</paramValue>
    <paramValue name="queryNcbiTaxonId">$$ncbiTaxonId$$</paramValue>
    <paramValue name="queryFile">$$cosmidEndsDir$$/master/mainresult/blocked.seq</paramValue>
    <paramValue name="queryTable">ExternalNASequence</paramValue>
    <paramValue name="queryExtDbRlsSpec">$$cosmidEndsExtDbRlsSpec$$</paramValue>
    <depends name="extractAndBlockCosmidEnds"/>
  </subgraph>

  <subgraph name="stanfordSnps_Mummer" xmlFile="runMummer.xml">
    <paramValue name="snpExtDbRlsSpec">$$stanfordSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpNickName">StanfordSnps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="resourcesFile">ME49_SNPs_fixed/Tg10x_31-StanfordSNPs.final.gff</paramValue>
    <paramValue name="strain">Me49</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="sibleySnps_Mummer" xmlFile="runMummer.xml">
    <paramValue name="snpExtDbRlsSpec">$$sibleySnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpNickName">SibleySnps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="resourcesFile">GeneticMarkers_Sibley_fixed/Tg10x_31-SibleySNPs.fixed.gff</paramValue>
    <paramValue name="strain">ME49</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="nucmerSnps_Mummer" xmlFile="runMummer.xml">
    <paramValue name="snpExtDbRlsSpec">$$nucmerSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpNickName">nucmerSnps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="resourcesFile">AmitAlignmentSnps/nucmerSnps.gff</paramValue>
    <paramValue name="strain">ME49</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="lindstrom454Snps_Mummer" xmlFile="runMummer.xml">
    <paramValue name="snpExtDbRlsSpec">$$lindstrom454SnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpNickName">lindstrom454Snps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="resourcesFile">Lindstrom454Snps/Lindstrom454Snps.gff</paramValue>
    <paramValue name="strain">ME49</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="map3pSageTags" xmlFile="mapSageTags.xml">
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="studyName">T. gondii 3p SAGE tag frequencies</paramValue>
    <paramValue name="sageTagExtDbRlsSpec">T. gondii 3p SAGE tags from M. White lab|1</paramValue>
    <paramValue name="paramValue">290062</paramValue>
    <paramValue name="nickName">map3pSageTags</paramValue>
    <paramValue name="calFreq">true</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="map5pSageTags" xmlFile="mapSageTags.xml">
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="studyName">T. gondii 5p SAGE tag frequencies</paramValue>
    <paramValue name="sageTagExtDbRlsSpec">T. gondii 5p SAGE tags from M. White lab|1</paramValue>
    <paramValue name="paramValue">290062</paramValue>
    <paramValue name="nickName">map5pSageTags</paramValue>
    <paramValue name="calFreq">false</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="einsteinChIP" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">einsteinChIP-chip</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFileRelativeToManualDeliveryDir">$$einsteinChipProbeFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec">$$einsteinChipExtDbRlsSpec$$</paramValue>
    <paramValue name="inputDir">$$einsteinChipRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <paramValue name="insertSimpleRadAnalysisFromConfig">false</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


  <subgraph name="einsteinChIPPeaks" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">einsteinChIP-chipdPeaks</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFileRelativeToManualDeliveryDir">$$einsteinChipPeaksProbeFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec">$$einsteinChipPeaksExtDbRlsSpec$$</paramValue>
    <paramValue name="inputDir">$$einsteinChipRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <paramValue name="insertSimpleRadAnalysisFromConfig">true</paramValue>
    <depends name="makeDataDir"/>
    <depends name="einsteinChIP"/>
  </subgraph>


  <subgraph name="hakimiChIP" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">hakimiChIP-chip</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFileRelativeToManualDeliveryDir">$$hakimiChipProbeFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec">$$hakimiChipExtDbRlsSpec$$</paramValue>
    <paramValue name="inputDir">$$hakimiChipRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <paramValue name="insertSimpleRadAnalysisFromConfig">false</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="hakimiChIPPeaks" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">hakimiChIP-chipPeaks</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFileRelativeToManualDeliveryDir">$$hakimiChipPeaksProbeFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec">$$hakimiChipPeaksExtDbRlsSpec$$</paramValue>
    <paramValue name="inputDir">$$hakimiChipRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <paramValue name="insertSimpleRadAnalysisFromConfig">true</paramValue>
    <depends name="makeDataDir"/>
    <depends name="hakimiChIP"/>
  </subgraph>

  <subgraph name="einsteinCentromereChIP" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">einsteinCentromereChIP-chip</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFileRelativeToManualDeliveryDir"></paramValue>
    <paramValue name="chipExtDbRlsSpec"></paramValue>
    <paramValue name="inputDir">$$einsteinCentromereRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">false</paramValue>
    <depends name="makeDataDir"/>
    <depends name="einsteinChIP"/>
  </subgraph>

</workflowGraph>



