<workflowGraph name="">
  <param name="organismResourceXmlFile"/>
  <param name="parentDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/productNames</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <datasetTemplate class="productNames">
    <prop name="name"/>
    <subgraph name="$$orgAbbrev$$_function_$$name$$_product_names_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">$$orgAbbrev$$_function_$$name$$_product_names_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
  </datasetTemplate>

</workflowGraph>
