<workflowGraph name="">
  <param name="organismResourceXmlFile"/>
  <param name="organismAbbrev"/>
  <param name="projectName"/>
  <param name="relativeDownloadSiteDir"/>
  <param name="parentDataDir"/>
  <param name="proteinsFile"/>
  <param name="projectVersionForWebsiteFiles"/>

  <constant name="dataDir">$$parentDataDir$$/dbXRefs</constant>
  <constant name="UniProtDescripString">Mapping between Gene IDs and UniProt Protein IDs</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <datasetTemplate class="dbxref_NRDB">
    <step name="findNrdbXrefs" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::FindNrdbXrefs">
      <paramValue name="proteinsFile">$$proteinsFile$$</paramValue>
      <paramValue name="nrdbFile">$$global_NRDBOutputFile$$</paramValue>
      <paramValue name="proteinsFileRegex">^([\\w\\.]+)</paramValue>
      <paramValue name="nrdbFileRegex">^\\|\\d+\\|(?:emb|gb|dbj|ref|sp|pdb)\\|([\\w\\.]+)\\|</paramValue>
      <paramValue name="outputFile">$$dataDir$$/nrdbXrefs.txt</paramValue>
       <depends name="makeDataDir"/>
      <dependsGlobal name="NRDB_RSRC"/>
    </step>

    <step name="loadNrdbXrefs" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::LoadNrdbXrefs" stepLoadTypes="plugin">
      <paramValue name="xrefsFile">$$dataDir$$/nrdbXrefs.txt</paramValue>
      <depends name="findNrdbXrefs"/>
    </step>
  </datasetTemplate>

  <datasetTemplate class="dbxref_simple">
    <prop name="name"/>
    <prop name="organismAbbrev"/>
    <subgraph name="${organismAbbrev}_dbxref_${name}_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">${organismAbbrev}_dbxref_${name}_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
  </datasetTemplate>
  
  <datasetTemplate class="dbxref_ApiLoc">
    <prop name="organismAbbrev"/>
    <subgraph name="${organismAbbrev}_dbxref_ApiLoc_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">${organismAbbrev}_dbxref_ApiLoc_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
  </datasetTemplate>
  
  <datasetTemplate class="dbxref_gene2Entrez">
    <prop name="organismAbbrev"/>
    <subgraph name="${organismAbbrev}_dbxref_gene2Entrez_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">${organismAbbrev}_dbxref_gene2Entrez_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
  </datasetTemplate>
  
  <datasetTemplate class="dbxref_gene2Uniprot">
    <prop name="name"/>
    <prop name="organismAbbrev"/>
    <subgraph name="${organismAbbrev}_dbxref_gene2Uniprot_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">${organismAbbrev}_dbxref_gene2Uniprot_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="${organismAbbrev}_dbxref_gene2Entrez_RSRC"/>
    </subgraph>

    <step name="makeEuPathUniProtLinkFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeEuPathUniProtLinkFile">
      <paramValue name="dbrefExtDbName">${organismAbbrev}_dbxref_gene2Uniprot_RSRC</paramValue>
    <paramValue name="organismAbbrev">${organismAbbrev}</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="relativeDir">$$relativeDownloadSiteDir$$</paramValue>
    <paramValue name="fileType">txt</paramValue>
    <paramValue name="dataName">UniProtMapping</paramValue>
    <paramValue name="descripString">$$UniProtDescripString$$</paramValue>
    <paramValue name="projectVersionForWebsiteFiles">$$projectVersionForWebsiteFiles$$</paramValue>
    <paramValue name="organelle"></paramValue>
    <depends name="${organismAbbrev}_dbxref_gene2Uniprot_RSRC"/>
    </step>
  </datasetTemplate>

  <datasetTemplate class="IEDB_dbxrefs">
    <prop name="organismAbbrev"/>
    <subgraph name="IEDBDBxRefResource" xmlFile="loadResource.xml">
      <paramValue name="resourceName">${organismAbbrev}_dbxref_epitope_IEDB_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
  </datasetTemplate>
  
</workflowGraph>
