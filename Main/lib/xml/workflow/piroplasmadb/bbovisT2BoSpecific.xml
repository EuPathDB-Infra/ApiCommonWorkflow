<workflowGraph name="bbovisT2BoSpecificAnalysis">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/BbovisT2BoSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/BbovisT2Bo</constant>


  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <subgraph name="GOAssociations_BbovisT2Bo_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_BbovisT2Bo_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GO_RSRC"/>
      <dependsGlobal name="GO_evidence_codes_RSRC"/>
    </subgraph>

    <subgraph name="BbovisT2Bo_Comment_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">BbovisT2Bo_Comment_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="BbovisT2Bo_GeneName_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">BbovisT2Bo_GeneName_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="BbovisT2Bo_ECNumbers_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">BbovisT2Bo_ECNumbers_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDB_RSRC"/>
    </subgraph>

    <subgraph name="BbovisT2Bo_pmid_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">BbovisT2Bo_pmid_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="BbovisT2Bo_uniprot_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">BbovisT2Bo_uniprot_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="BbovisT2Bo_vsgdb_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">BbovisT2Bo_vsgdb_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="BbovisT2Bo_embl_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">BbovisT2Bo_embl_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
