<workflowGraph name="tparvaMugugaSpecificAnalysis">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/TparvaMugugaSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/TparvaMuguga</constant>


  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <subgraph name="GOAssociations_TparvaMuguga_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_TparvaMuguga_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GO_RSRC"/>
      <dependsGlobal name="GO_evidence_codes_RSRC"/>
    </subgraph>

    <subgraph name="TparvaMuguga_Comment_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_Comment_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TparvaMuguga_GeneName_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_GeneName_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TparvaMuguga_ECNumbers_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_ECNumbers_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDB_RSRC"/>
    </subgraph>

    <subgraph name="TparvaMuguga_pmid_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_pmid_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TparvaMuguga_uniprot_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_uniprot_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TparvaMuguga_vsgdb_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_vsgdb_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TparvaMuguga_embl_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_embl_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TparvaMuguga_ctp_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_ctp_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TparvaMuguga_pdb_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_pdb_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TparvaMuguga_interpro_dbxref_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TparvaMuguga_interpro_dbxref_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

</workflowGraph>



