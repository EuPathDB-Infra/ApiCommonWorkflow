<workflowGraph name="saccharomyces_cerevisiae_s288cSpecificAnalysis">

  <constant name="dataDir">saccharomyces_cerevisiae_s288cArrayStudies</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <subgraph name="arrayResources" xmlFile="miniflow/fungidb/saccharomyces_cerevisiae_s288cArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">fungidb/saccharomyces_cerevisiae_s288c.xml</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir"></paramValue>
      <paramValue name="genomeExtDbRlsSpec">saccharomyces_cerevisiae_s288c_genome_sequence_RSRC|@@saccharomyces_cerevisiae_s288c_genome_sequence_VER@@</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

</workflowGraph>



