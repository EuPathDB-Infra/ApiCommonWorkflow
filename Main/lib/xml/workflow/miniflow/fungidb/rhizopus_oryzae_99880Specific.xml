<workflowGraph name="rhizopus_oryzae_99880SpecificAnalysis">

  <constant name="dataDir">rhizopus_oryzae_99880_RNASeq</constant>
  <constant name="genomicSeqsFile">$$dataDir$$/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$dataDir$$/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$dataDir$$/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$dataDir$$/geneModelForSsa.txt</constant>
  <constant name="samHeaderFile">$$dataDir$$/samHeader</constant>


  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <step name="extractGenomicSeqsForSsa"
          stepClass="ApiCommonWorkflow::Main::WorkflowSteps::ExtractNaSeqsForSsa">
      <paramValue name="ncbiTaxonId">@@rhizopus_oryzae_99880_NCBI_TAXONOMY_ID@@</paramValue>
      <paramValue name="outputFile">$$genomicSeqsFileForSsa$$</paramValue>
      <depends name="makeDataDir"/> 
    </step>

  <step name="extractTranscriptSeqsForSsa"
          stepClass="ApiCommonWorkflow::Main::WorkflowSteps::ExtractTranscriptSeqsForSsa">
      <paramValue name="ncbiTaxonId">@@rhizopus_oryzae_99880_NCBI_TAXONOMY_ID@@</paramValue>
      <paramValue name="outputFile">$$transcriptSeqsFileForSsa$$</paramValue>
      <depends name="makeDataDir"/> 
    </step>

  <step name="extractGeneModelForSsa"
          stepClass="ApiCommonWorkflow::Main::WorkflowSteps::ExtractGeneModelForSsa">
      <paramValue name="ncbiTaxonId">@@rhizopus_oryzae_99880_NCBI_TAXONOMY_ID@@</paramValue>
      <paramValue name="outputFile">$$ssaGeneModelFile$$</paramValue>
      <depends name="makeDataDir"/> 
    </step>

  <step name="makeSamHeader"
          stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeSamHeader">
      <paramValue name="ncbiTaxonId">@@rhizopus_oryzae_99880_NCBI_TAXONOMY_ID@@</paramValue>
      <paramValue name="outputFile">$$samHeaderFile$$</paramValue>
      <depends name="makeDataDir"/> 
    </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="extractGenomicSeqsForSsa"/>
      <depends name="extractTranscriptSeqsForSsa"/>
      <depends name="extractGeneModelForSsa"/>
    </step>


  <subgraph name="rnaSeqAndChipSeq" xmlFile="miniflow/fungidb/rhizopus_oryzae_99880RnaSeqAndChipSeq.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/rhizopus_oryzae_99880.xml</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="samHeaderFile">$$samHeaderFile$$</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir"></paramValue>
    <paramValue name="genomeExtDbRlsSpec">rhizopus_oryzae_99880_genome_sequence_RSRC|@@rhizopus_oryzae_99880_GENOME_SEQUENCE_VER@@</paramValue>
    <depends name="mirrorToCluster"/>
  </subgraph>

</workflowGraph>



