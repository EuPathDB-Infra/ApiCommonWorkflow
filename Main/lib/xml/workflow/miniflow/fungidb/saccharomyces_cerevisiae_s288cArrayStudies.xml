<workflowGraph name="accharomyces_cerevisiae_s288cArrayResources">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="ssaGeneModelFile"/>
  <param name="genomicSeqsFileForSsa"/>
  <param name="transcriptSeqsFileForSsa"/>
  <param name="downloadSiteTranscriptExpressionDataDir"/>
  <param name="genomeExtDbRlsSpec"/>

  <constant name="dataDir">$$parentDataDir$$/accharomyces_cerevisiae_s288cResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

<!--Spellman CellCycle No microarrayPlatform -->
  <subgraph name="accharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">accharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="mirrorToCluster"/>
  </subgraph>

  <subgraph name="accharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">accharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec"></paramValue>
    <paramValue name="experimentExtDbRlsSpec">accharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC|@@accharomyces_cerevisiae_s288c_Spellman_CellCycle_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="insertAnalysisResult">false</paramValue>
    <paramValue name="probeGenesMappingFile"></paramValue>
    <paramValue name="profileSetNames"></paramValue>
    <paramValue name="profileSetSpecs"></paramValue>
    <paramValue name="isLogged">true</paramValue>
    <paramValue name="mappingFileForExpressionStats"></paramValue>
    <paramValue name="calculateExpressionStats">false</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteTranscriptExpressionDataDir$$/accharomyces_cerevisiae_s288c_Spellman_CellCycle</paramValue>
    <depends name="accharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC"/>
  </subgraph>

</workflowGraph>
