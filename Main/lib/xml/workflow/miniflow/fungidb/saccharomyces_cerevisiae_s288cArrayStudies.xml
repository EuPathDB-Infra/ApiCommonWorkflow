<workflowGraph name="saccharomyces_cerevisiae_s288cArrayResources">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="ssaGeneModelFile"/>
  <param name="genomicSeqsFileForSsa"/>
  <param name="transcriptSeqsFileForSsa"/>
  <param name="downloadSiteTranscriptExpressionDataDir"/>
  <param name="genomeExtDbRlsSpec"/>

  <constant name="dataDir">$$parentDataDir$$/saccharomyces_cerevisiae_s288cResources</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ReFlow::StepClasses::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

<!--Spellman CellCycle No microarrayPlatform -->
  <subgraph name="saccharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="mirrorToCluster"/>
  </subgraph>

  <subgraph name="saccharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">saccharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec"></paramValue>
    <paramValue name="experimentExtDbRlsSpec">saccharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC|@@saccharomyces_cerevisiae_s288c_Spellman_CellCycle_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="insertAnalysisResult">false</paramValue>
    <paramValue name="probeGenesMappingFile"></paramValue>
    <paramValue name="profileSetNames"></paramValue>
    <paramValue name="profileSetSpecs"></paramValue>
    <paramValue name="isLogged">true</paramValue>
    <paramValue name="mappingFileForExpressionStats"></paramValue>
    <paramValue name="calculateExpressionStats">false</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteTranscriptExpressionDataDir$$/saccharomyces_cerevisiae_s288c_Spellman_CellCycle</paramValue>
    <depends name="saccharomyces_cerevisiae_s288c_Spellman_CellCycle_RSRC"/>
  </subgraph>

</workflowGraph>
