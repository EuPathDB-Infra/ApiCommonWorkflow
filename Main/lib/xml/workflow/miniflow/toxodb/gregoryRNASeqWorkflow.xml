<workflowGraph name="gregoryRnaSeqWorkflow">
  <param name="projectName"/>
  <param name="projectVersion"/>

  <subgraph name="miniGlobal" xmlFile="miniflow/miniGlobal.xml" > </subgraph>

   <subgraph name="tgME49LoadGenome" xmlFile="miniflow/miniLoadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">tgME49</paramValue>
    <paramValue name="genomeExtDbRlsSpec">T.gondii ME49 contigs from Genbank|@@Tgondii_ME49_contigs_from_Genbank_VER@@</paramValue>
    <paramValue name="ncbiTaxonId">508771</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="miniGlobal"/>
  </subgraph>

  <subgraph name="tgGT1LoadGenome" xmlFile="miniflow/miniLoadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">tgGT1</paramValue>
    <paramValue name="genomeExtDbRlsSpec">T.gondii GT1 contigs from Genbank|@@T.gondii GT1 contigs from Genbank_VER@@</paramValue>
    <paramValue name="ncbiTaxonId">507601</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="miniGlobal"/> 
  </subgraph>

   <subgraph name="tgVEGLoadGenome" xmlFile="miniflow/miniLoadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">tgVEG</paramValue>
    <paramValue name="genomeExtDbRlsSpec">T.gondii VEG contigs from Genbank|@@Tgondii_VEG_contigs_from_Genbank_VER@@</paramValue>
    <paramValue name="ncbiTaxonId">432359</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="miniGlobal"/> 
  </subgraph>

  <subgraph name="ncLoadGenome" xmlFile="miniflow/miniLoadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">nc</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Sanger N. caninum chromosomes|@@Sanger_Ncaninum_chromosomes_VER@@</paramValue>
    <paramValue name="ncbiTaxonId">29176</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="miniGlobal"/>
  </subgraph>

  <subgraph name="tgME49Specific" xmlFile="miniflow/toxodb/tgME49GregorySpecific.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="resourceXmlFileName">toxodb/gregoryRNAResources.xml</paramValue>
    <depends name="tgME49LoadGenome"/> 
  </subgraph>

  <subgraph name="tgGT1Specific" xmlFile="miniflow/toxodb/tgGT1GregorySpecific.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="resourceXmlFileName">toxodb/gregoryRNAResources.xml</paramValue>
    <depends name="tgGT1LoadGenome"/> 
  </subgraph>

  <subgraph name="tgVEGSpecific" xmlFile="miniflow/toxodb/tgVEGGregorySpecific.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="resourceXmlFileName">toxodb/gregoryRNAResources.xml</paramValue>
    <depends name="tgVEGLoadGenome"/> 
  </subgraph>

  <subgraph name="ncSpecific" xmlFile="miniflow/toxodb/ncGregorySpecific.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="resourceXmlFileName">toxodb/gregoryRNAResources.xml</paramValue>
    <depends name="ncLoadGenome"/> 
  </subgraph>


</workflowGraph>

