<workflowGraph name="tgME49BoothroydArrayResources">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="ssaGeneModelFile"/>
  <param name="genomicSeqsFileForSsa"/>
  <param name="transcriptSeqsFileForSsa"/>
  <param name="downloadSiteTranscriptExpressionDataDir"/>
  <param name="genomeExtDbRlsSpec"/>

  <constant name="dataDir">$$parentDataDir$$/tgME49BoothroydResources</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>



<!--Boothroyd -->
  <subgraph name="tgME49Boothroyd_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">tgME49Boothroyd_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
  </subgraph>

  <subgraph name="tgME49Boothroyd_RSRC_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">tgME49Boothroyd_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec"></paramValue>
    <paramValue name="experimentExtDbRlsSpec">tgME49Boothroyd_RSRC|@@tgME49Boothroyd_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="insertAnalysisResult">false</paramValue>
    <paramValue name="probeGenesMappingFile">$$dataDir$$/tgME49Boothroyd_RSRC/final/Tgondiia520372.cdf</paramValue>
    <paramValue name="profileSetNames"></paramValue>
    <paramValue name="profileSetSpecs">Expression profiles of Tgondii ME49 Boothroyd experiments|Expression percentile profiles of Tgondii ME49 Boothroyd experiments</paramValue>
    <paramValue name="isLogged">true</paramValue>
    <paramValue name="mappingFileForExpressionStats"></paramValue>
    <paramValue name="calculateExpressionStats">true</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteTranscriptExpressionDataDir$$/tgME49Boothroyd</paramValue>
    <depends name="tgME49Boothroyd_RSRC"/>
  </subgraph>

</workflowGraph>
