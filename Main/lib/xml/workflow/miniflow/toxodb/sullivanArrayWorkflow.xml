<workflowGraph name="sullivanArrayWorkflow">
  <param name="projectName"/>
  <param name="projectVersion"/>


   <subgraph name="tgME49LoadGenome" xmlFile="miniflow/miniLoadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">tgME49</paramValue>
    <paramValue name="genomeExtDbRlsSpec">T.gondii ME49 contigs from Genbank|@@Tgondii_ME49_contigs_from_Genbank_VER@@</paramValue>
    <paramValue name="ncbiTaxonId">508771</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
  </subgraph>

  <subgraph name="Affy-Amit_NuclearCoding_getAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="probeResourceName">Tg_array_Affy-Amit_NuclearCoding_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Tg_array_Affy-Amit_NuclearCoding_RSRC|@@Tg_array_Affy-Amit_NuclearCoding_VER@@</paramValue>
    <paramValue name="isOneChannel">true</paramValue>
    <paramValue name="organismResourceXmlFile">toxodb/tgSullivanArrayResources.xml</paramValue>
    <paramValue name="ssaGeneModelFile">tgME49LoadGenome/geneModelForSsa.txt</paramValue>
    <paramValue name="genomeExtDbRlsSpec">T.gondii ME49 contigs from Genbank|@@Tgondii_ME49_contigs_from_Genbank_VER@@</paramValue>
    <paramValue name="genomicSeqsFileForSsa">tgME49LoadGenome/genomicSeqsForSsa.fsa</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">tgME49LoadGenome/transcriptSeqsForSsa.fsa</paramValue>
    <paramValue name="limitNU">30</paramValue>
    <paramValue name="numInsertions">1</paramValue>
    <paramValue name="dnaHybridazation">false</paramValue>
    <paramValue name="pairedReadFilePath"></paramValue>
    <paramValue name="countMismatches">true</paramValue>
    <paramValue name="cdfFileName">Tgondiia520372.cdf</paramValue>
    <paramValue name="isNimbleGen">false</paramValue>
    <paramValue name="ndfFileName"></paramValue>
    <paramValue name="SNPs"></paramValue>
    <paramValue name="reference_strain"></paramValue>
    <paramValue name="organismFullName"></paramValue>
    <paramValue name="strandSpecific"></paramValue>
    <paramValue name="variableLengthReads"></paramValue>
    <depends name="tgME49LoadGenome"/>
  </subgraph>


</workflowGraph>

