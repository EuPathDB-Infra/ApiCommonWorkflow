<workflowGraph name="tgondiiVANDGenomeISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/toxoGenomeResourceISFResource</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="TgondiiVAND_genome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">TgondiiVAND_genome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

<subgraph name="Tgondii_VAND_contigs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">TgondiiVAND_contigs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="TgondiiVAND_genome_RSRC"/>
  </subgraph>

</workflowGraph>



