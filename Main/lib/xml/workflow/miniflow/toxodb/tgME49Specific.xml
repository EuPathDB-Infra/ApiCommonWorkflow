<workflowGraph name="tgME49Specific">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="resourceXmlFileName"/>

  <constant name="dataDir">$$parentDataDir$$/tgME49Specific</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/tgME49LoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/tgME49LoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/tgME49LoadGenome/geneModelForSsa.txt</constant>
  <constant name="samHeaderFile">$$parentDataDir$$/tgME49LoadGenome/samHeader</constant>
  <constant name="downloadSiteDir">@@downloadSiteDir@@/$$projectName$$/release-$$projectVersion$$/</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>


  <subgraph name="tgME49RnaSeqAndChipSeq" xmlFile="miniflow/toxodb/tgME49RnaSeqAndChipSeq.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="resourceXmlFileName">$$resourceXmlFileName$$</paramValue>
    <paramValue name="samHeaderFile">$$samHeaderFile$$</paramValue>
    <depends name="mirrorToCluster"/>
  </subgraph>

    <subgraph name="tgME49BoothroydArray" xmlFile="miniflow/toxodb/tgME49BoothroydArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$resourceXmlFileName$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
      <paramValue name="genomeExtDbRlsSpec">@@Tgondii_ME49_genome_RSRC@@|@@Tgondii_ME49_contigs_from_Genbank_VER@@</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

</workflowGraph>
