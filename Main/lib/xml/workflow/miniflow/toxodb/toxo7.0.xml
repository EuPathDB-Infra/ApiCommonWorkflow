<workflowGraph name="toxoWorkflow">
  <param name="projectName"/>
  <param name="projectVersion"/>


  <constant name="comparativeGenomicsFastaDir">common/comparativeGenomics/Fasta/</constant>
  <constant name="comparativeGenomicsGffDir">common/comparativeGenomics/Gff/</constant>
  <constant name="webServicesDataDir">@@webServicesDir@@/$$projectName$$/release-$$projectVersion$$/</constant>
  <constant name="downloadSiteDir">@@downloadSite@@/$$projectName$$/release-$$projectVersion$$/</constant>

  <globalSubgraph name="miniglobal" xmlFile="miniflow/miniGlobal.xml">
    <paramValue name="globalResourceXmlFile">miniGlobalResources.xml</paramValue>
  </globalSubgraph>

  <subgraph name="common" xmlFile="common.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="comparativeGenomicsFastaDir">$$comparativeGenomicsFastaDir$$</paramValue>
    <paramValue name="comparativeGenomicsGffDir">$$comparativeGenomicsGffDir$$</paramValue>
    <paramValue name="webServicesDataDir">@@webServicesDir@@/$$projectName$$/release-$$projectVersion$$/</paramValue>
  </subgraph>





  <subgraph name="EtenellaHoughtonLoadGenome" xmlFile="loadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">EtenellaHoughton</paramValue>
    <paramValue name="genomeExtDbRlsSpec">EtenellaHoughton_genome_RSRC|@@EtenellaHoughton_genome_VER@@</paramValue>
    <paramValue name="organismResourceXmlFile">toxodb/etenellaHoughton.xml</paramValue>
    <paramValue name="organismSpecificISFReference">toxodb/etenellaHoughtonISFResource.xml</paramValue>
    <paramValue name="ncbiTaxonId">413949</paramValue>
    <paramValue name="downloadSiteDir">$$downloadSiteDir$$</paramValue>
    <paramValue name="extractProteins">true</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="haveOrganelleGenomeSequence">false</paramValue>
    <paramValue name="organelleInitDownloadDirSubgraph">initMitoAndApicoplastDownloadDirs.xml</paramValue>
    <paramValue name="downloadSiteDataDir">@@downloadSiteDir@@/$$projectName$$/release-$$projectVersion$$</paramValue>
    <depends name="common"/>
    <dependsGlobal name="taxonomy_RSRC"/> 
  </subgraph>

  <subgraph name="EtenellaHoughtonMakeAndMaskTopLevelGenome" xmlFile="makeAndMaskTopLevelGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">EtenellaHoughton</paramValue>
    <paramValue name="organismFullName">@@EtenellaHoughtonOrganismFullName@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">EtenellaHoughton_genome_RSRC|@@EtenellaHoughton_genome_VER@@</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFileResourceName"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFile">false</paramValue>
    <paramValue name="repeatMaskSpecies">-species Eimeria</paramValue>
    <paramValue name="organismResourceXmlFile">toxodb/etenellaHoughton.xml</paramValue>
    <paramValue name="comparativeGenomicsFastaDir">$$comparativeGenomicsFastaDir$$</paramValue>
    <paramValue name="comparativeGenomicsGffDir">$$comparativeGenomicsGffDir$$</paramValue>
    <depends name="EtenellaHoughtonLoadGenome"/>
  </subgraph>

  <subgraph name="EtenellaHoughtonPostLoadGenome" xmlFile="postLoadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">EtenellaHoughton</paramValue>
    <paramValue name="organismFullName">@@EtenellaHoughtonOrganismFullName@@</paramValue>
    <paramValue name="organismAbbrev">EtenellaHoughton</paramValue>
    <paramValue name="genomeExtDbRlsSpec">EtenellaHoughton_genome_RSRC|@@EtenellaHoughton_genome_VER@@</paramValue>
    <paramValue name="dbESTResourceName">EtenellaHoughton_dbEST_RSRC</paramValue>
    <paramValue name="dbESTVersion">@@EtenellaHoughton_dbEST_VER@@</paramValue>
    <paramValue name="loadDbest">true</paramValue>
    <paramValue name="ncbiTaxonId">413949</paramValue>
    <paramValue name="parentNcbiTaxonId">5802</paramValue>
    <paramValue name="usePredictedTranscripts">false</paramValue>
    <paramValue name="predictedTranscriptsExtDbRlsSpec"></paramValue>
    <paramValue name="runtRNAScan">true</paramValue>
    <paramValue name="runExportPred">false</paramValue>
    <paramValue name="maxIntronSize">20000</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota, Apicomplexa, Eimeria</paramValue>
    <paramValue name="repeatMaskSpecies">-species Eimeria</paramValue>
    <paramValue name="genomeSize">45000927</paramValue>
    <paramValue name="skipProteomeAnalysis">false</paramValue>
    <paramValue name="skipGenomeAnalysis">false</paramValue>
    <paramValue name="loadEpitope">true</paramValue>
    <paramValue name="iedbVersion">@@EtenellaHoughton_IEDB_Epitopes_VER@@</paramValue>
    <paramValue name="iedbDbRefResourceName">EtenellaHoughton_IEDB_Epitopes_DBxRefs_RSRC</paramValue>
    <paramValue name="iedbResourceName">EtenellaHoughton_IEDB_Epitopes_RSRC</paramValue>
    <paramValue name="makeAssemblySeqs">false</paramValue>
    <paramValue name="makeAssemblies">false</paramValue>
    <paramValue name="mapAssemblySeqsAndAssemblies">false</paramValue>
    <paramValue name="loadOrganismSpecificESTResource">false</paramValue>
    <paramValue name="loadOrganismSpecificMassSpecResource">false</paramValue>
    <paramValue name="distanceBetweenStartsForAlignmentsInSameCluster">20000</paramValue>
    <paramValue name="organismSpecificESTResourcesReference"></paramValue>
    <paramValue name="organismSpecificMassSpecResourcesReference"></paramValue>
    <paramValue name="organismSpecificProductNameResourcesReference">toxodb/etenellaHoughtonProductNameResource.xml</paramValue>
    <paramValue name="organismResourceXmlFile">toxodb/etenellaHoughton.xml</paramValue>
    <depends name="EtenellaHoughtonLoadGenome"/>
  </subgraph>

  <subgraph name="EtenellaHoughtonSpecificWorkflow" xmlFile="toxodb/etenellaHoughtonSpecific.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismResourceXmlFile">toxodb/etenellaHoughton.xml</paramValue>
    <paramValue name="downloadSiteDataDir">@@downloadSiteDir@@/$$projectName$$/release-$$projectVersion$$</paramValue>
    <depends name="EtenellaHoughtonLoadGenome"/>
  </subgraph>
 
  <subgraph name="EtenellaHoughtonLoadGeneAliases" xmlFile="loadGeneAliases.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">EtenellaHoughton</paramValue>
    <paramValue name="organismSpecificGeneAliasesReference">toxodb/etenellaHoughtonGeneAliasesResource.xml</paramValue>
    <paramValue name="organismResourceXmlFile">toxodb/etenellaHoughton.xml</paramValue>
    <depends name="EtenellaHoughtonLoadGenome"/>
  </subgraph>

  <subgraph name="EtenellaHoughtonMakeAndFormatDownloadFiles" xmlFile="makeAndFormatDownloadFiles.xml">
    <paramValue name="downloadSiteDataDir">@@downloadSiteDir@@/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="organismName">EtenellaHoughton</paramValue>
    <paramValue name="organismFullName">@@EtenellaHoughtonOrganismFullName@@</paramValue>
    <paramValue name="ncbiTaxonId">413949</paramValue>
    <paramValue name="parentNcbiTaxonId">5802</paramValue>
    <paramValue name="genomeSource">@@EtenellaHoughtonGenomeSource@@</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="dbESTExtDbRlsSpec">EtenellaHoughton_dbEST_RSRC|@@EtenellaHoughton_dbEST_VER@@</paramValue>
    <paramValue name="webServicesDataDir">@@webServicesDir@@/$$projectName$$/release-$$projectVersion$$/</paramValue>
    <paramValue name="genomeExtDbRlsSpec">EtenellaHoughton_genome_RSRC|@@EtenellaHoughton_genome_VER@@</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="minOrfLength">50</paramValue>
    <paramValue name="deprecated">false</paramValue>
    <paramValue name="soTermIdsOrNames">contig</paramValue>
    <paramValue name="makeESTDownloadFile">false</paramValue>
    <paramValue name="makeProteomeDownloadFile">true</paramValue>
    <paramValue name="makeGenomicDownloadFile">true</paramValue>
    <depends name="EtenellaHoughtonPostLoadGenome"/>
    <depends name="EtenellaHoughtonMakeAndMaskTopLevelGenome"/>
  </subgraph>

</workflowGraph>

