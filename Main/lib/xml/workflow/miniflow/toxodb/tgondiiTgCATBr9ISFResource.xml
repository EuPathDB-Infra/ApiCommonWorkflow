<workflowGraph name="tgondiiTgCATBr9GenomeISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/toxoGenomeResourceISFResource</constant>

    <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="TgondiiTgCATBr9_genome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">TgondiiTgCATBr9_genome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="TgondiiTgCATBr9_apicoplast_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">TgondiiTgCATBr9_apicoplast_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="TgondiiTgCATBr9_genome_RSRC"/>
  </subgraph>


</workflowGraph>



