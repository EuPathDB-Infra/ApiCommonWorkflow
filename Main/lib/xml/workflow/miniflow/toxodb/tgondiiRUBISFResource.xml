<workflowGraph name="tgondiiRUBGenomeISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/toxoGenomeResourceISFResource</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="TgondiiRUB_genome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">TgondiiRUB_genome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

 <subgraph name="TgondiiRUB_contigs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">TgondiiRUB_contigs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="TgondiiRUB_genome_RSRC"/>
  </subgraph>

</workflowGraph>



