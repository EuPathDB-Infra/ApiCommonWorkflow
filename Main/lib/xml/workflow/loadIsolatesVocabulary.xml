<workflowGraph name="">
      <param name="parentDataDir"/>
      <param name="resourceXmlFile"/>

    <constant name="dataDir">$$parentDataDir$$/isolatesVocabulary</constant>

    <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

    <step name="manuallyImproveIsolatesVocabulary" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::DoNothingStep">
       <depends name="makeDataDir"/>
    </step>

  <subgraph name="isolateVocabulary_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">isolateVocabulary_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$resourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="manuallyImproveIsolatesVocabulary"/>
  </subgraph>

  <subgraph name="Continents_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Continents_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$resourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="manuallyImproveIsolatesVocabulary"/>
  </subgraph>

  <subgraph name="isolateVocabularyMap_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">isolateVocabularyMap_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$resourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="isolateVocabulary_RSRC"/>
    <depends name="Continents_RSRC"/>
  </subgraph>


</workflowGraph>

