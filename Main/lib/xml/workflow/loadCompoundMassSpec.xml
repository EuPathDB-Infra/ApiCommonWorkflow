<workflowGraph name="loadCompoundMassSpec">
    <param name="datasetName"/>
    <param name="datasetLoaderXmlFileName"/>
    <param name="parentDataDir"/>

    <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
        <paramValue name="dataDir">$$parentDataDir$$</paramValue>
    </step>

    <subgraph name="compoundMassSpec_${experimentName}_RSRC" xmlFile="loadDataset.xml">
        <paramValue name="datasetName">$$datasetName$$</paramValue>
        <paramValue name="datasetLoaderXmlFileName">$$datasetLoaderXmlFileName$$</paramValue>
        <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
        <depends name="makeDataDir"/>
    </subgraph>

</workflowGraph>
