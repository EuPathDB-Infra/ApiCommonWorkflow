<workflowGraph name="ncTachyRnaSeq">
    <param name="parentDataDir"/>
    <param name="resourceXmlFileName"/>
    <param name="ssaGeneModelFile"/>
    <param name="genomicSeqsFileForSsa"/>
    <param name="transcriptSeqsFileForSsa"/>

    <constant name="dataDir">$$parentDataDir$$/ncTachyRnaSeq</constant>
    <constant name="samplesDataDir">$$dataDir$$/samples</constant>
    <constant name="seqCoverageAndIntensityDataDir">$$dataDir$$/seqCoverageAndIntensity</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

    <step name="makeSeqCoverageAndIntensityDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$seqCoverageAndIntensityDataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <step name="makeSamplesDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$samplesDataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeSamplesDataDir"/>
    </step>

    <subgraph name="Nc_RNASeq_Tachyzoite_Reid_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Nc_RNASeq_Tachyzoite_Reid_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$resourceXmlFileName$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
     </subgraph>
 
    <subgraph name="analyzeDay3SampleA" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Day3A</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/3250_5_1.fastq</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeDay3SampleA/Day3A.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Day3A.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>     
      <paramValue name="rnaSeqExtDbRlsSpec">Nc_RNASeq_Tachyzoite_Reid_RSRC|@@Nc_RNASeq_Tachyzoite_Reid_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Sanger N. caninum chromosomes|@@Sanger_Ncaninum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="pairedReadFilePath">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/3250_5_2.fastq</paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Nc_RNASeq_Tachyzoite_Reid_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="analyzeDay3SampleB" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Day3B</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/3563_2_1.fastq</paramValue>
      <paramValue name="pairedReadFilePath">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/3563_2_2.fastq</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeDay3SampleB/Day3B.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Day3B.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Nc_RNASeq_Tachyzoite_Reid_RSRC|@@Nc_RNASeq_Tachyzoite_Reid_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Sanger N. caninum chromosomes|@@Sanger_Ncaninum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Nc_RNASeq_Tachyzoite_Reid_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="analyzeDay4SampleA" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Day4A</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/3250_6_1.fastq</paramValue>
      <paramValue name="pairedReadFilePath">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/3250_6_2.fastq</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeDay4SampleA/Day4A.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Day4A.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Nc_RNASeq_Tachyzoite_Reid_RSRC|@@Nc_RNASeq_Tachyzoite_Reid_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Sanger N. caninum chromosomes|@@Sanger_Ncaninum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Nc_RNASeq_Tachyzoite_Reid_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="analyzeDay4SampleB" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Day4B</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/3563_3_1.fastq</paramValue>
      <paramValue name="pairedReadFilePath">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/3563_3_2.fastq</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeDay4SampleB/Day4B.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Day4B.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Nc_RNASeq_Tachyzoite_Reid_RSRC|@@Nc_RNASeq_Tachyzoite_Reid_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Sanger N. caninum chromosomes|@@Sanger_Ncaninum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Nc_RNASeq_Tachyzoite_Reid_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <step name="mergeIntensityFiles" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MergeIntensityFiles">
      <paramValue name="inputFilesDir">$$samplesDataDir$$</paramValue>
      <paramValue name="outputFile">$$seqCoverageAndIntensityDataDir$$/profiles.txt</paramValue>
      <paramValue name="profileElementOrder">Day3A.int,Day3B.int,Day4A.int,Day4B.int</paramValue>
      <depends name="analyzeDay3SampleA"/>
      <depends name="analyzeDay3SampleB"/>
      <depends name="analyzeDay4SampleA"/>
      <depends name="analyzeDay4SampleB"/>
    </step>

    <step name="makePercentileProfile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakePercentileProfile">
      <paramValue name="inputFile">$$seqCoverageAndIntensityDataDir$$/profiles.txt</paramValue>
      <paramValue name="outputFile">$$seqCoverageAndIntensityDataDir$$/percents.txt</paramValue>
      <paramValue name="ties">min</paramValue>
      <paramValue name="hasHeader">true</paramValue>
      <depends name="mergeIntensityFiles"/>
    </step>

    <step name="loadIntensityProfile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::InsertExpressionProfiles" stepLoadTypes="plugin">
      <paramValue name="configFile">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/config.txt</paramValue>
      <paramValue name="inputDir">$$seqCoverageAndIntensityDataDir$$</paramValue>
      <paramValue name="extDbRlsSpec">Nc_RNASeq_Tachyzoite_Reid_RSRC|@@Nc_RNASeq_Tachyzoite_Reid_VER@@</paramValue>
      <depends name="makePercentileProfile"/>
    </step>
<!--
    <step name="copyExpressionProfilesToDownloadDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::CopyExpressionProfilesToDownloadDir">
      <paramValue name="copyFromDir">$$seqCoverageAndIntensityDataDir$$</paramValue>
      <paramValue name="configFile">$$dataDir$$/Nc_RNASeq_Tachyzoite_Reid_RSRC/config.txt</paramValue>
      <paramValue name="copyToDir">$$downloadSiteTranscriptExpressionDataDir$$</paramValue>
      <depends name="mergeIntensityFiles"/>
    </step>
-->

</workflowGraph>
