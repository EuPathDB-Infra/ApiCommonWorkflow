<workflowGraph name="tgondiiArrayStudies">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="ssaGeneModelFile"/>
  <param name="genomicSeqsFileForSsa"/>
  <param name="transcriptSeqsFileForSsa"/>
  <param name="downloadSiteTranscriptExpressionDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/tgondiiArrayResources</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="ChIP_Chip_Einstein_ME1_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">ChIP_Chip_Einstein_ME1_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <subgraph name="Einstein_ME1_AnalyzeChipChip_RH_H3K4me1_Rep1_sample" xmlFile="analyzeChipChip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="ChipChipResourceName">ChIP_Chip_Einstein_ME1_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">ChIP Chip Einstein|2008-08</paramValue>
    <paramValue name="experimentExtDbRlsSpec">ChIP_Chip_Einstein_ME1_RSRC|@@ChIP_Chip_Einstein_ME1_VER@@</paramValue>
    <paramValue name="inputSampleFile">RH_H3K4me1_Rep1.txt</paramValue>
    <paramValue name="sampleName">RH_H3K4me1_Rep1</paramValue>
    <paramValue name="sampleOrder">Einstein_ME11</paramValue>
    <paramValue name="peakFinderArgs">0.5 4 600 600</paramValue>
    <paramValue name="runPeakCalls">true</paramValue>
    <paramValue name="organismFullName">@@TgME49OrganismFullName@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">T.gondii ME49 contigs from Genbank|2008-07-23</paramValue>
    <depends name="ChIP_Chip_Einstein_ME1_RSRC"/>
  </subgraph>

  <subgraph name="Einstein_ME1_AnalyzeChipChip_RH_H3K4me1_Rep2_sample" xmlFile="analyzeChipChip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="ChipChipResourceName">ChIP_Chip_Einstein_ME1_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">ChIP Chip Einstein|2008-08</paramValue>
    <paramValue name="experimentExtDbRlsSpec">ChIP_Chip_Einstein_ME1_RSRC|@@ChIP_Chip_Einstein_ME1_VER@@</paramValue>
    <paramValue name="inputSampleFile">RH_H3K4me1_Rep2.txt</paramValue>
    <paramValue name="sampleName">RH_H3K4me1_Rep2</paramValue>
    <paramValue name="sampleOrder">Einstein_ME12</paramValue>
    <paramValue name="peakFinderArgs">0.5 4 600 600</paramValue>
    <paramValue name="runPeakCalls">true</paramValue>
    <paramValue name="organismFullName">@@TgME49OrganismFullName@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">T.gondii ME49 contigs from Genbank|2008-07-23</paramValue>
    <depends name="ChIP_Chip_Einstein_ME1_RSRC"/>
  </subgraph>

</workflowGraph>
