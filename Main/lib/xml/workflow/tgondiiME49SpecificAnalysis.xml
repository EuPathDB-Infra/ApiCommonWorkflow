<workflowGraph name="tgondiiME49SpecificAnalysis">
  <param name="parentDataDir"/>
  <param name="organismName"/>
  <param name="organismFullName"/>
  <param name="genomeExtDbRlsSpec"/>
  <param name="stanfordSnpExtDbRlsSpec"/>
  <param name="sibleySnpExtDbRlsSpec"/>
  <param name="nucmerSnpExtDbRlsSpec"/>
  <param name="sageTagExtDbRlsSpec"/>
  <param name="einsteinChipProbeFile"/>
  <param name="einsteinChipExtDbRlsSpec"/>
  <param name="einsteinChipPeaksProbeFile"/>
  <param name="einsteinChipPeaksExtDbRlsSpec"/>
  <param name="hakimiChipProbeFile"/>
  <param name="hakimiChipExtDbRlsSpec"/>
  <param name="hakimiChipPeaksProbeFile"/>
  <param name="hakimiChipPeaksExtDbRlsSpec"/>
  <param name="einsteinChipRadAnalysisWorkingDir"/>
  <param name="hakimiChipRadAnalysisWorkingDir"/>
  <param name="einsteinCentromereRadAnalysisWorkingDir"/>
  <param name="genbankIsolateExtDbRlsSpec"/>
  <param name="sibleyIsolateExtDbRlsSpec"/>
  <param name="maxIntronSize"/>
  <param name="ncbiTaxonId"/>
  <param name="BACEndsExtDbRlsSpec"/>
  <param name="cosmidEndsExtDbRlsSpec"/>
  <param name="repeatMaskSpecies"/>

  <constant name="dataDir">$$parentDataDir$$/$$organismName$$/SpecificAnalysis</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/$$organismName$$/genome/genomicSeqs.fsa</constant>
  <constant name="genbankIsolateSeqFile">$$dataDir$$/genbankIsolatesSeqs.fsa</constant>
  <constant name="sibleyIsolateSeqFile">$$dataDir$$/sibleyIsolatesSeqs.fsa</constant>
  <constant name="BACEndsDir">$$dataDir$$/makeBACEnds</constant>
  <constant name="cosmidEndsDir">$$dataDir$$/makeBACEnds</constant>


  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <step name="extractGenbankIsolateSeqs"
          stepClass="ApiCommonWorkflow::Main::WorkflowSteps::ExtractNaSeqs">
      <paramValue name="extDbRlsSpec">$$genbankIsolateExtDbRlsSpec$$</paramValue>
      <paramValue name="table">ExternalNASequence</paramValue>
      <paramValue name="alternateDefline"></paramValue>
      <paramValue name="outputFile">$$genbankIsolateSeqFile$$</paramValue>
      <paramValue name="separateFastaFiles">false</paramValue>
      <paramValue name="outputDirForSeparateFiles"></paramValue>
      <depends name="makeDataDir"/> 
    </step>

  <step name="extractSibleyIsolateSeqs"
          stepClass="ApiCommonWorkflow::Main::WorkflowSteps::ExtractNaSeqs">
      <paramValue name="extDbRlsSpec">$$sibleyIsolateExtDbRlsSpec$$</paramValue>
      <paramValue name="table">ExternalNASequence</paramValue>
      <paramValue name="alternateDefline"></paramValue>
      <paramValue name="outputFile">$$sibleyIsolateSeqFile$$</paramValue>
      <paramValue name="separateFastaFiles">false</paramValue>
      <paramValue name="outputDirForSeparateFiles"></paramValue>
      <depends name="makeDataDir"/> 
    </step>

    <subgraph name="extractAndBlockBACEnds" xmlFile="extractAndBlockBACEnds.xml">
      <paramValue name="dataDir">$$BACEndsDir$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">$$BACEndsExtDbRlsSpec$$</paramValue>
      <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
      <paramValue name="repeatMaskSpecies">$$repeatMaskSpecies$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>


    <subgraph name="extractAndBlockCosmidEnds" xmlFile="extractAndBlockBACEnds.xml">
      <paramValue name="dataDir">$$cosmidEndsDir$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">$$cosmidEndsExtDbRlsSpec$$</paramValue>
      <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
      <paramValue name="repeatMaskSpecies">$$repeatMaskSpecies$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>


    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="extractGenbankIsolateSeqs"/>
      <depends name="extractSibleyIsolateSeqs"/>
    </step>

    <subgraph name="blastnGenbankIsolateSeqGenomicSeqs" xmlFile="blastWorkflow.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="nickName">blastnGenbankIsolates</paramValue>
      <paramValue name="queryFile">$$genbankIsolateSeqFile$$</paramValue>
      <paramValue name="queryTable">DoTS::ExternalNASequence</paramValue>
      <paramValue name="queryExtDbRlsSpec">$$genbankIsolateExtDbRlsSpec$$</paramValue>
      <paramValue name="subjectFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="subjectTable">DoTS::ExternalNASequence</paramValue>
      <paramValue name="subjectExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="blastArgs">V=1 B=1 hspsepQmax=50000 hspsepSmax=50000 -span1 -topcomboN=1 E=0.1 -wordmask=seg</paramValue>
      <paramValue name="idRegex">(\\S+)</paramValue>
      <paramValue name="blastType">blastn</paramValue>
      <paramValue name="vendor">wu</paramValue>
      <paramValue name="loadSubjectSubset">false</paramValue>
      <paramValue name="filterByTaxon">false</paramValue>
      <paramValue name="taxonHierarchyForFilter"></paramValue>
      <paramValue name="loadOptions"></paramValue>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="blastnSibleyIsolateSeqGenomicSeqs" xmlFile="blastWorkflow.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="nickName">blastnSibleyIsolates</paramValue>
      <paramValue name="queryFile">$$sibleyIsolateSeqFile$$</paramValue>
      <paramValue name="queryTable">DoTS::ExternalNASequence</paramValue>
      <paramValue name="queryExtDbRlsSpec">$$sibleyIsolateExtDbRlsSpec$$</paramValue>
      <paramValue name="subjectFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="subjectTable">DoTS::ExternalNASequence</paramValue>
      <paramValue name="subjectExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="blastArgs">V=1 B=1 hspsepQmax=50000 hspsepSmax=50000 -span1 -topcomboN=1 E=0.1 -wordmask=seg</paramValue>
      <paramValue name="idRegex">(\\S+)</paramValue>
      <paramValue name="blastType">blastn</paramValue>
      <paramValue name="vendor">wu</paramValue>
      <paramValue name="loadSubjectSubset">false</paramValue>
      <paramValue name="filterByTaxon">false</paramValue>
      <paramValue name="taxonHierarchyForFilter"></paramValue>
      <paramValue name="loadOptions"></paramValue>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="mapBACEndsSeqsToGenome" xmlFile="mapAssembliesToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="ncbiTaxonId">$$ncbiTaxonId$$</paramValue>
      <paramValue name="assembliesDataDir">$$parentDataDir$$/$$organismName$$/genome/makeAssemblies</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/$$organismName$$/genome/makeAssemblySeqs/genomicSeqs</paramValue>
      <paramValue name="organismTwoLetterAbbrev"></paramValue>
      <paramValue name="nickName">BACEnds</paramValue>
      <paramValue name="queryTable">ExternalNASequence</paramValue>
      <paramValue name="updateAssemblySourceId">false</paramValue>
      <depends name="extractAndBlockBACEnds"/>
    </subgraph>

    <subgraph name="mapCosmidEndsSeqsToGenome" xmlFile="mapAssembliesToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="ncbiTaxonId">$$ncbiTaxonId$$</paramValue>
      <paramValue name="assembliesDataDir">$$parentDataDir$$/$$organismName$$/genome/makeAssemblies</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/$$organismName$$/genome/makeAssemblySeqs/genomicSeqs</paramValue>
      <paramValue name="organismTwoLetterAbbrev"></paramValue>
      <paramValue name="nickName">cosmidEnds</paramValue>
      <paramValue name="queryTable">ExternalNASequence</paramValue>
      <paramValue name="updateAssemblySourceId">false</paramValue>
      <depends name="extractAndBlockCosmidEnds"/>
    </subgraph>

  <subgraph name="stanfordSnps_Mummer" xmlFile="runMummer.xml">
    <paramValue name="snpExtDbRlsSpec">$$stanfordSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpNickName">Stanford_SNPs</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="sibleySnps_Mummer" xmlFile="runMummer.xml">
    <paramValue name="snpExtDbRlsSpec">$$sibleySnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpNickName">Sibley_SNPs</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="nucmerSnps_Mummer" xmlFile="runMummer.xml">
    <paramValue name="snpExtDbRlsSpec">$$nucmerSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpNickName">nucmerSnps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="mapSageTags" xmlFile="mapSageTags.xml">
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="studyName">$$sageTagExtDbRlsSpec$$</paramValue>
    <paramValue name="sageTagExtDbRlsSpec">T. gondii 3p SAGE tag frequencies</paramValue>
    <paramValue name="paramValue">290062</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


  <subgraph name="einsteinChIP" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">einsteinChIP-chip</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFile">$$einsteinChipProbeFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec">$$einsteinChipExtDbRlsSpec$$</paramValue>
    <paramValue name="inputDir">$$einsteinChipRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


  <subgraph name="einsteinChIPPeaks" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">einsteinChIP-chipdPeaks</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFile">$$einsteinChipPeaksProbeFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec">$$einsteinChipPeaksExtDbRlsSpec$$</paramValue>
    <paramValue name="inputDir">$$einsteinChipRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


  <subgraph name="hakimiChIP" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">hakimiChIP-chip</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFile">$$hakimiChipProbeFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec">$$hakimiChipExtDbRlsSpec$$</paramValue>
    <paramValue name="inputDir">$$hakimiChipRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="hakimiChIPPeaks" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">hakimiChIP-chipPeaks</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFile">$$hakimiChipPeaksProbeFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec">$$hakimiChipPeaksExtDbRlsSpec$$</paramValue>
    <paramValue name="inputDir">$$hakimiChipRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="einsteinCentromereChIP" xmlFile="ChIP-chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipNickName">einsteinCentromereChIP-chip</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="probeFile"></paramValue>
    <paramValue name="chipExtDbRlsSpec"></paramValue>
    <paramValue name="inputDir">$$einsteinCentromereRadAnalysisWorkingDir$$</paramValue>
    <paramValue name="mapProbesToNaSequences">false</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>



