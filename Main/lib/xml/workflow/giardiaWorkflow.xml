<workflowGraph name="giardiaWorkflow">
  <param name="interproVersion"/>
  <param name="iedbVersion"/>
  <param name="nrdbVersion"/>
  <param name="dbESTVersion"/>
  <param name="soVersion"/>
  <param name="goVersion"/>
  <param name="ncbiTaxonVersion"/>
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="pdbExtDbRlsSpec"/>
  <param name="genbankIsolateExtDbRlsSpec"/>
  <param name="GlambliaExtDbRlsSpec"/>
  <param name="GlambliaGenomeSource"/>
  <param name="GlambliaVirtualSeqsExtDbRlsSpec"/>
  <param name="GlambliaSageTagExtDbRlsSpec"/>
  <param name="GlambliaDeprecatedGenesExtDbRlsSpec"/>

  <constant name="pdbFile">common/pdb.fsa</constant>
  <constant name="nrdbFile">common/nrdb.fsa</constant>
  <constant name="nrdbShortDeflineFile">common/nrdb-short-defline.fsa</constant>
  <constant name="mercatorDir">common/mercator/</constant>

  <subgraph name="common" xmlFile="commonAnalysis.xml">
    <paramValue name="nrdbExtDbRlsSpec">NRDB|$$nrdbVersion$$</paramValue>
    <paramValue name="nrdbOutputFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineOutputFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="pdbOutputFile">$$pdbFile$$</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
  </subgraph>

  <subgraph name="GlambliaAnalysis" xmlFile="organismAnalysis.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Glamblia</paramValue>
    <paramValue name="organismFullName">Giardia lamblia ATCC 50803</paramValue>
    <paramValue name="organismTwoLetterAbbrev">Gl</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$GlambliaExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec">$$GlambliaVirtualSeqsExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeSource">$$GlambliaGenomeSource$$</paramValue>
    <paramValue name="interproVersion">$$interproVersion$$</paramValue>
    <paramValue name="iedbVersion">$$iedbVersion$$</paramValue>
    <paramValue name="soVersion">$$soVersion$$</paramValue>
    <paramValue name="goVersion">$$goVersion$$</paramValue>
    <paramValue name="dbESTVersion">$$dbESTVersion$$</paramValue>
    <paramValue name="ncbiTaxonVersion">$$ncbiTaxonVersion$$</paramValue>
    <paramValue name="ncbiTaxonId">184922</paramValue>
    <paramValue name="parentNcbiTaxonId">5741</paramValue>
    <paramValue name="usePredictedTranscripts">false</paramValue>
    <paramValue name="predictedTranscriptsExtDbRlsSpec"></paramValue>
    <paramValue name="runtRNAScan">false</paramValue>
    <paramValue name="runExportPred">false</paramValue>
    <paramValue name="pdbFile">$$pdbFile$$</paramValue>
    <paramValue name="nrdbFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="maxIntronSize">500</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota, Hexamitidae, Giardia</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">NRDB|$$nrdbVersion$$</paramValue>
    <paramValue name="repeatMaskSpecies">-species Giardia</paramValue>
    <paramValue name="pdbExtDbRlsSpec">$$pdbExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeSize">11192216</paramValue>
    <paramValue name="skipProteomeAnalysis">false</paramValue>
    <paramValue name="skipGenomeAnalysis">false</paramValue>
    <paramValue name="mapEpitope">true</paramValue>
    <paramValue name="makeAssemblySeqs">true</paramValue>
    <paramValue name="makeAssemblies">true</paramValue>
    <paramValue name="mapAssemblySeqsAndAssemblies">true</paramValue>
    <depends name="common"/>
  </subgraph>

  <subgraph name="DeprecatedGenesAnalysis" xmlFile="organismAnalysis.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">GlambliaDeprecatedGenes</paramValue>
    <paramValue name="organismFullName">Giardia lamblia ATCC 50803</paramValue>
    <paramValue name="organismTwoLetterAbbrev"></paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$GlambliaDeprecatedGenesExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="genomeSource"></paramValue>
    <paramValue name="interproVersion">$$interproVersion$$</paramValue>
    <paramValue name="iedbVersion">$$iedbVersion$$</paramValue>
    <paramValue name="soVersion">$$soVersion$$</paramValue>
    <paramValue name="goVersion">$$goVersion$$</paramValue>
    <paramValue name="dbESTVersion">$$dbESTVersion$$</paramValue>
    <paramValue name="ncbiTaxonVersion">$$ncbiTaxonVersion$$</paramValue>
    <paramValue name="ncbiTaxonId">184922</paramValue>
    <paramValue name="parentNcbiTaxonId">5741</paramValue>
    <paramValue name="usePredictedTranscripts">false</paramValue>
    <paramValue name="predictedTranscriptsExtDbRlsSpec"></paramValue>
    <paramValue name="runtRNAScan">false</paramValue>
    <paramValue name="runExportPred">false</paramValue>
    <paramValue name="pdbFile">$$pdbFile$$</paramValue>
    <paramValue name="nrdbFile">$$nrdbFile$$</paramValue>
    <paramValue name="nrdbShortDeflineFile">$$nrdbShortDeflineFile$$</paramValue>
    <paramValue name="maxIntronSize">500</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota, Hexamitidae, Giardia</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">NRDB|$$nrdbVersion$$</paramValue>
    <paramValue name="repeatMaskSpecies">-species Giardia</paramValue>
    <paramValue name="pdbExtDbRlsSpec">$$pdbExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeSize">11192216</paramValue>
    <paramValue name="skipProteomeAnalysis">false</paramValue>
    <paramValue name="skipGenomeAnalysis">true</paramValue>
    <paramValue name="mapEpitope">true</paramValue>
    <paramValue name="makeAssemblySeqs">true</paramValue>
    <paramValue name="makeAssemblies">true</paramValue>
    <paramValue name="mapAssemblySeqsAndAssemblies">true</paramValue>
    <depends name="common"/>
  </subgraph>

  <subgraph name="GlambliaSpecificAnalysis" xmlFile="glambliaSpecificAnalysis.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Glamblia</paramValue>
    <paramValue name="organismFullName">Giardia lamblia ATCC 50803</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$GlambliaExtDbRlsSpec$$</paramValue>
    <paramValue name="sageTagExtDbRlsSpec">$$GlambliaSageTagExtDbRlsSpec$$</paramValue>
    <paramValue name="genbankIsolateExtDbRlsSpec">$$genbankIsolateExtDbRlsSpec$$</paramValue>
    <paramValue name="ncbiTaxonId">184922</paramValue>
    <depends name="GlambliaAnalysis"/>
  </subgraph>

</workflowGraph>









 
