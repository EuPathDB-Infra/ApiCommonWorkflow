<workflowGraph name="pfalciparumChIP">
  <param name="parentDataDir"/>
  <param name="genomicSeqsFile"/>
  <param name="organismResourceXmlFile"/>

  <constant name="dataDir">$$parentDataDir$$</constant>

  <subgraph name="stunnenberChIP" xmlFile="getAndAnalyzeChIPs.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipResourceName">ChIP_Chip_Henk_Stunnenberg_RSRC</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec"></paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <paramValue name="insertSimpleRadAnalysisFromConfig">true</paramValue>
  </subgraph>

  <subgraph name="scherfChIP" xmlFile="getAndAnalyzeChIPs.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipResourceName">ChIP_Chip_Artur_Scherf_RSRC</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec"></paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <paramValue name="insertSimpleRadAnalysisFromConfig">true</paramValue>
  </subgraph>

  <subgraph name="vossChIP" xmlFile="getAndAnalyzeChIPs.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipResourceName">ChIP_Chip_Till_Voss_RSRC</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="chipExtDbRlsSpec"></paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="mapProbesToNaSequences">true</paramValue>
    <paramValue name="insertSimpleRadAnalysisFromConfig">true</paramValue>
  </subgraph>

  <subgraph name="Pf_temp_called_peaks_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pf_temp_called_peaks_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="scherfChIP"/>
    <depends name="vossChIP"/>
  </subgraph>


  <subgraph name="Pfalciparum_ChIP_PEAKs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pfalciparum_ChIP_PEAKs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="Pf_temp_called_peaks_RSRC"/>
  </subgraph>

  <subgraph name="Pfalciparum_ChIP_SMOOTHED_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pfalciparum_ChIP_SMOOTHED_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="Pf_temp_called_peaks_RSRC"/>
  </subgraph>

</workflowGraph>
