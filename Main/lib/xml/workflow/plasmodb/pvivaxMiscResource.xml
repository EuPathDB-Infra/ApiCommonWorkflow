<workflowGraph name="pvivaxMiscResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/pvivaxMiscResource</constant>

  <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="Pv_TransFactorBindingSites_Llinas_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_TransFactorBindingSites_Llinas_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$parentDataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
