<workflowGraph name="pbergheiMassSpecResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/pbergheiMassSpecResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="Waters_female_gametes_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Waters_female_gametes_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Waters_male_gametes_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Waters_male_gametes_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Waters_mixed_gametes_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Waters_mixed_gametes_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
