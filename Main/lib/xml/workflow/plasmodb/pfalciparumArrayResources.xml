<workflowGraph name="pfalciparumArrayResources">
  <param name="parentDataDir"/>
  <param name="genomicSeqsFile"/>
  <param name="organismResourceXmlFile"/>

  <constant name="dataDir">$$parentDataDir$$/pfalciparumArrayResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="A-MEXP-1407GetAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">A-MEXP-1407_RSRC</paramValue>
    <paramValue name="indexFileDirForShortSeqAligner"></paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">A-MEXP-1407_RSRC|@A-MEXP-1407_RSRC_VER@</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <subgraph name="ChIP_Chip_Artur_Scherf_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">ChIP_Chip_Artur_Scherf_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="A-MEXP-1407GetAndMapProbes"/>
    </subgraph>

  <subgraph name="scherfAnalyzeChIP-Chip" xmlFile="analyzeChIP-Chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="ChIP-ChipResourceName">ChIP_Chip_Artur_Scherf_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">A-MEXP-1407_RSRC|@A-MEXP-1407_VER@</paramValue>
    <paramValue name="peakFinderArgs">0.5 4 100 200 true</paramValue>
    <paramValue name="runPeakCalls">true</paramValue>
    <paramValue name="organismFullName">@PfOrganismFullName@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <depends name="ChIP_Chip_Artur_Scherf_RSRC"/>
  </subgraph>

  <subgraph name="GPL8808GetAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">Pf_Probes_Till_Voss_RSRC</paramValue>
    <paramValue name="indexFileDirForShortSeqAligner"></paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pf_array_GPL8808_RSRC|@Pf_array_GPL8808_VER@</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <subgraph name="ChIP_Chip_Till_Voss_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">ChIP_Chip_Till_Voss_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="GPL8808GetAndMapProbes"/>
    </subgraph>

  <subgraph name="vossAnalyzeChIP-Chip" xmlFile="analyzeChIP-Chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="ChIP-ChipResourceName">ChIP_Chip_Till_Voss_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pf_array_GPL8808_RSRC|@Pf_array_GPL8808_VER@</paramValue>
    <paramValue name="peakFinderArgs">0.5 4 100 200 true</paramValue>
    <paramValue name="runPeakCalls">true</paramValue>
    <paramValue name="organismFullName">@PfOrganismFullName@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <depends name="ChIP_Chip_Till_Voss_RSRC"/>
  </subgraph>


  <subgraph name="winzeler_WestenbergerGetAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">Pf_Probes_Winzeler_Westenberger_RSRC</paramValue>
    <paramValue name="indexFileDirForShortSeqAligner"></paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Affy-Winzeler-Westenberger_RSRC|@Affy-Winzeler-Westenberger_VER@</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <subgraph name="ChIP_Chip_Winzeler_Westenberger_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">ChIP_Chip_Winzeler_Westenberger_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="winzeler_WestenbergerGetAndMapProbes"/>
    </subgraph>

  <subgraph name="winzelerAnalyzeChIP-Chip" xmlFile="analyzeChIP-Chip.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="ChIP-ChipResourceName">ChIP_Chip_Winzeler_Westenberger_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pf_Probes_Winzeler_Westenberger_RSRC|@ChIP_Chip_Winzeler_Westenberger_VER@</paramValue>
    <paramValue name="peakFinderArgs">0.5 4 100 200 true</paramValue>
    <paramValue name="runPeakCalls">false</paramValue>
    <paramValue name="organismFullName">@PfOrganismFullName@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <depends name="ChIP_Chip_Winzeler_Westenberger_RSRC"/>
  </subgraph>


</workflowGraph>
