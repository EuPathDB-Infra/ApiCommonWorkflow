<workflowGraph name="plasmoWorkflow">
  <param name="interproVersion"/>
  <param name="iedbVersion"/>
  <param name="nrdbVersion"/>
  <param name="dbESTVersion"/>
  <param name="soVersion"/>
  <param name="goVersion"/>
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="pdbExtDbRlsSpec"/>
  <param name="genbankIsolateExtDbRlsSpec"/>
  <param name="barcodeIsolateExtDbRlsSpec"/>
  <param name="PfalciparumExtDbRlsSpec"/>
  <param name="PfalciparumGenomeSource"/>
  <param name="PfalciparumPredictedTranscriptsExtDbRlsSpec"/>
  <param name="PvivaxExtDbRlsSpec"/>
  <param name="PvivaxGenomeSource"/>
  <param name="PvivaxPredictedTranscriptsExtDbRlsSpec"/>
  <param name="PvivaxVirtualSeqsExtDbRlsSpec"/>
  <param name="PchabaudiExtDbRlsSpec"/>
  <param name="PchabaudiGenomeSource"/>
  <param name="PknowlesiExtDbRlsSpec"/>
  <param name="PknowlesiGenomeSource"/>
  <param name="PbergheiExtDbRlsSpec"/>
  <param name="PbergheiGenomeSource"/>
  <param name="PbergheiPredictedTranscriptsExtDbRlsSpec"/>
  <param name="PyoeliiExtDbRlsSpec"/>
  <param name="PyoeliiGenomeSource"/>
  <param name="PyoeliiPredictedTranscriptsExtDbRlsSpec"/>
  <param name="PreichenowiExtDbRlsSpec"/>
  <param name="PreichenowiGenomeSource"/>
  <param name="PgallinaceumExtDbRlsSpec"/>
  <param name="PgallinaceumGenomeSource"/>
  <param name="suSnpExtDbRlsSpec"/>
  <param name="broadSnpExtDbRlsSpec"/>
  <param name="winzelerSnpExtDbRlsSpec"/>
  <param name="sangerItGhanaSnpExtDbRlsSpec"/>
  <param name="sangerReichenowiSnpExtDbRlsSpec"/>
  <param name="combinedSnpExtDbRlsSpec"/>
  <param name="pfalciparumSageTagExtDbRlsSpec"/>
  <param name="pfalciparumDeRisiOligosExtDbRlsSpec"/>
  <param name="pyoeliiVaidyaBergmanOligosExtDbRlsSpec"/>
  <param name="stunnenberChipExtDbRlsSpec"/>
  <param name="scherfChipExtDbRlsSpec"/>
  <param name="vossChipExtDbRlsSpec"/>

  <constant name="comparativeGenomicsFastaDir">common/comparativeGenomicsFasta/</constant>

  <subgraph name="common" xmlFile="../common.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="comparativeGenomicsFastaDir">$$comparativeGenomicsFastaDir$$</paramValue>
  </subgraph>

  <globalSubgraph name="global" xmlFile="../global.xml">
    <paramValue name="isolatesResourceReference">plasmoIsolatesResource.xml</paramValue>
    <paramValue name="commonResourceXmlFile">plasmodb/plasmodbCommon.xml</paramValue>
    <paramValue name="globalResourceXmlFile">global.xml</paramValue>
  </subgraph>

  <subgraph name="PfalciparumLoadGenome" xmlFile="../loadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Pfalciparum</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$PfalciparumExtDbRlsSpec$$</paramValue>
    <paramValue name="organismResourceXmlFile">plasmodb/pfalciparum.xml</paramValue>
    <paramValue name="organismSpecificISFReference">plasmodb/pfalciparumISFResource.xml</paramValue>
    <paramValue name="ncbiTaxonId">36329</paramValue>
    <depends name="common"/>
    <dependsGlobal name="taxonomyResource"/>
    <dependsGlobal name="SOResource"/>
  </subgraph>

  <subgraph name="PfalciparumMakeAndMaskTopLevelGenome" xmlFile="../makeAndMaskTopLevelGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Pfalciparum</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$PfalciparumExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFileResourceName"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFile">false</paramValue>
    <paramValue name="repeatMaskSpecies">-species Plasmodium</paramValue>
    <paramValue name="organismResourceXmlFile">plasmodb/pfalciparum.xml</paramValue>
    <paramValue name="comparativeGenomicsFastaDir">$$comparativeGenomicsFastaDir$$</paramValue>
    <depends name="PfalciparumLoadGenome"/>
  </subgraph>

  <subgraph name="PfalciparumPostLoadGenome" xmlFile="../postLoadGenome.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Pfalciparum</paramValue>
    <paramValue name="organismFullName">Plasmodium falciparum 3D7</paramValue>
    <paramValue name="organismTwoLetterAbbrev">Pf</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$PfalciparumExtDbRlsSpec$$</paramValue>
    <paramValue name="interproVersion">$$interproVersion$$</paramValue>
    <paramValue name="iedbVersion">$$iedbVersion$$</paramValue>
    <paramValue name="soVersion">$$soVersion$$</paramValue>
    <paramValue name="goVersion">$$goVersion$$</paramValue>
    <paramValue name="dbESTVersion">$$dbESTVersion$$</paramValue>
    <paramValue name="ncbiTaxonId">36329</paramValue>
    <paramValue name="parentNcbiTaxonId">5833</paramValue>
    <paramValue name="usePredictedTranscripts">true</paramValue>
    <paramValue name="predictedTranscriptsExtDbRlsSpec">$$PfalciparumPredictedTranscriptsExtDbRlsSpec$$</paramValue>
    <paramValue name="runtRNAScan">false</paramValue>
    <paramValue name="runExportPred">true</paramValue>
    <paramValue name="maxIntronSize">20000</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota Apicomplexa Plasmodium</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">NRDB|$$nrdbVersion$$</paramValue>
    <paramValue name="repeatMaskSpecies">-species Plasmodium</paramValue>
    <paramValue name="pdbExtDbRlsSpec">$$pdbExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeSize">23304547</paramValue>
    <paramValue name="skipProteomeAnalysis">false</paramValue>
    <paramValue name="skipGenomeAnalysis">false</paramValue>
    <paramValue name="mapEpitope">true</paramValue>
    <paramValue name="makeAssemblySeqs">true</paramValue>
    <paramValue name="makeAssemblies">true</paramValue>
    <paramValue name="mapAssemblySeqsAndAssemblies">true</paramValue>
    <paramValue name="distanceBetweenStartsForAlignmentsInSameCluster">30000</paramValue>
    <paramValue name="organismSpecificESTResourcesReference">plasmodb/pfalciparumESTResource.xml</paramValue>
    <paramValue name="organismSpecificMassSpecResourcesReference">plasmodb/pfalciparumMassSpecResource.xml</paramValue>
    <paramValue name="organismResourceXmlFile">plasmodb/pfalciparum.xml</paramValue>
    <depends name="PfalciparumLoadGenome"/>
  </subgraph>

  <subgraph name="PfalciparumSpecificWorkflow" xmlFile="pfalciparumSpecific.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Pfalciparum</paramValue>
    <paramValue name="organismFullName">Plasmodium falciparum</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$PfalciparumExtDbRlsSpec$$</paramValue>
    <paramValue name="suSnpExtDbRlsSpec">$$suSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="broadSnpExtDbRlsSpec">$$broadSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="winzelerSnpExtDbRlsSpec">$$winzelerSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="sangerItGhanaSnpExtDbRlsSpec">$$sangerItGhanaSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="sangerReichenowiSnpExtDbRlsSpec">$$sangerReichenowiSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="combinedSnpExtDbRlsSpec">$$combinedSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="sageTagExtDbRlsSpec">$$pfalciparumSageTagExtDbRlsSpec$$</paramValue>
    <paramValue name="oligoExtDbRlsSpec">$$pfalciparumDeRisiOligosExtDbRlsSpec$$</paramValue>
    <paramValue name="genbankIsolateExtDbRlsSpec">$$genbankIsolateExtDbRlsSpec$$</paramValue>
    <paramValue name="stunnenberChipExtDbRlsSpec">$$stunnenberChipExtDbRlsSpec$$</paramValue>
    <paramValue name="scherfChipExtDbRlsSpec">$$scherfChipExtDbRlsSpec$$</paramValue>
    <paramValue name="vossChipExtDbRlsSpec">$$vossChipExtDbRlsSpec$$</paramValue>
    <paramValue name="organismResourceXmlFile">plasmodb/pfalciparum.xml</paramValue>
    <depends name="PfalciparumLoadGenome"/>
  </subgraph>
</workflowGraph>
