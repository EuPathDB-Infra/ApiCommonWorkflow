<workflowGraph name="pvivaxArrayStudies">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="ssaGeneModelFile"/>
  <param name="genomicSeqsFileForSsa"/>
  <param name="transcriptSeqsFileForSsa"/>

  <constant name="dataDir">$$parentDataDir$$/pvivaxArrayResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

  <subgraph name="ZB_Oligos_getAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">Pvivax_ZB_oligos_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pvivax_ZB_oligos_RSRC|@@Pvivax_ZB_oligos_VER@@</paramValue>
    <paramValue name="isOneChannel">false</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pvivax_contigsGB_RSRC|@@Pvivax_contigsGB_VER@@</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="limitNU">30</paramValue>
    <paramValue name="numInsertions">1</paramValue>
    <paramValue name="dnaHybridazation">false</paramValue>
    <paramValue name="pairedReadFilePath"></paramValue>
    <paramValue name="countMismatches">true</paramValue>
    <paramValue name="cdfFileName"></paramValue>
    <depends name="mirrorToCluster"/>
  </subgraph>

  <subgraph name="Pv_ZB_time_series_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_ZB_time_series_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="ZB_Oligos_getAndMapProbes"/>
  </subgraph>

  <subgraph name="ZB_time_series_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">Pv_ZB_time_series_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pvivax_ZB_oligos_RSRC|@@Pvivax_ZB_oligos_VER@@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">Pv_ZB_time_series_RSRC|@@Pv_ZB_time_series_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pvivax_contigsGB_RSRC|@@Pvivax_contigsGB_VER@@</paramValue>
    <paramValue name="insertAnalysisResult">false</paramValue>
    <paramValue name="probeGenesMappingFile">$$dataDir$$/getAndMap_Pvivax_ZB_oligos_RSRC/geneProbeMapping.txt</paramValue>
    <paramValue name="profileSetNames"></paramValue>
    <paramValue name="profileSetSpecs">ZB Pvivax time series normalized_averaged 1|ZB Pvivax time series percentiles 1 Red,ZB Pvivax time series normalized_averaged 2|ZB Pvivax time series percentiles 2 Red,ZB Pvivax time series normalized_averaged 3|ZB Pvivax time series percentiles 3 Red</paramValue>
    <paramValue name="isLogged">true</paramValue>
    <paramValue name="mappingFileForExpressionStats"></paramValue>
    <paramValue name="calculateExpressionStats">true</paramValue>
    <depends name="Pv_ZB_time_series_RSRC"/>
  </subgraph>

  <subgraph name="pvivax_westenberger_lib520507_getAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">pvivax_westenberger_lib520507_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">pvivax_westenberger_lib520507_RSRC|@@pvivax_westenberger_lib520507_VER@@</paramValue>
    <paramValue name="isOneChannel">true</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pvivax_contigsGB_RSRC|@@Pvivax_contigsGB_VER@@</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="limitNU">30</paramValue>
    <paramValue name="numInsertions">0</paramValue>
    <paramValue name="dnaHybridazation">false</paramValue>
    <paramValue name="pairedReadFilePath"></paramValue>
    <paramValue name="countMismatches">true</paramValue>
    <paramValue name="cdfFileName">PvivaxLib520507.cdf</paramValue>
    <depends name="mirrorToCluster"/>
  </subgraph>

  <subgraph name="Pv_Westenberger_array_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pv_Westenberger_array_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="pvivax_westenberger_lib520507_getAndMapProbes"/>
  </subgraph>

  <subgraph name="Westenberger_array_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">Pv_Westenberger_array_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">pvivax_westenberger_lib520507_RSRC|@@pvivax_westenberger_lib520507_VER@@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">Pv_Westenberger_array_RSRC|@@Pv_Westenberger_array_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pvivax_contigsGB_RSRC|@@Pvivax_contigsGB_VER@@</paramValue>
    <paramValue name="insertAnalysisResult">false</paramValue>
    <paramValue name="probeGenesMappingFile">$$dataDir$$/getAndMap_pvivax_westenberger_lib520507_RSRC/PvivaxLib520507.cdf</paramValue>
    <paramValue name="profileSetNames"></paramValue>
    <paramValue name="profileSetSpecs">westenberger vivax expression profile|westenberger vivax expression profile percentiles</paramValue>
    <paramValue name="isLogged">true</paramValue>
    <paramValue name="mappingFileForExpressionStats"></paramValue>
    <paramValue name="calculateExpressionStats">true</paramValue>
    <depends name="Pv_Westenberger_array_RSRC"/>
  </subgraph>

</workflowGraph>
