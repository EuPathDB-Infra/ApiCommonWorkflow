<workflowGraph name="pfalciparumChipSeq">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>

  <constant name="dataDir">$$parentDataDir$$/pfalciparumChipSeqResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="getAndAnalyzeLeRochChipSeq" xmlFile="getAndAnalyzeChipSeq.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="chipSeqResourceName">Pf_ChipSeq_Le_Roch_RSRC</paramValue>
    <paramValue name="indexFileDirForShortSeqAligner"></paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="chipSeqExtDbRlsSpec">Pf_ChipSeq_Le_Roch_RSRC|@Pf_ChipSeq_Le_Roch_VER@</paramValue>
    <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@P.falciparum_chromosomes_VER@</paramValue>
    <paramValue name="generatePeakCalls">false</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
