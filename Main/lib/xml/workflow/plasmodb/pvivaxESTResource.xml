<workflowGraph name="pvivaxESTResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/pfalciparumESTResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="WatanabePv_PredictedTranscripts_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">WatanabePv_PredictedTranscripts_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="WatanabePv_EstLibrary_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">WatanabePv_EstLibrary_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
