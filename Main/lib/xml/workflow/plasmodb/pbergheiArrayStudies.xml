<workflowGraph name="pbergheiArrayStudies">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="bowtieGenomicIndexDir"/>
  <param name="bowtieTranscriptsIndexDir"/>
  <param name="ssaGeneModelFile"/>
  <param name="genomicSeqsFile"/>
  <param name="genomicSeqsDir"/>

  <constant name="dataDir">$$parentDataDir$$/pbergheiArrayResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="GSScDNAArray_getAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">Pberghei_gss_oligos_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_gss_oligos_RSRC|@Pberghei_gss_oligos_VER@</paramValue>
    <paramValue name="isOneChannel">false</paramValue>
    <paramValue name="loadGeneProbeMapping">false</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="bowtieGenomicIndexDir">$$bowtieGenomicIndexDir$$</paramValue>
    <paramValue name="bowtieTranscriptIndexDir">$$bowtieTranscriptIndexDir$$</paramValue>
    <paramValue name="geneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="genomicSeqsDir">$$genomicSeqsDir$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@Pberghei_chromosomes_VER@</paramValue>
    <paramValue name="organismFullName">@PbOrganismFullName@</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Waters_gss_time_series_HP_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Waters_gss_time_series_HP_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="GSScDNAArray_getAndMapProbes"/>
  </subgraph>

  <subgraph name="Waters_gss_time_series_HP_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">Waters_gss_time_series_HP_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_gss_oligos_RSRC|@Pberghei_gss_oligos_VER@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">Waters_gss_time_series_HP_RSRC|@Waters_gss_time_series_HP_VER@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@Pberghei_chromosomes_VER@</paramValue>
    <paramValue name="insertAnalysisResult"></paramValue>
    <depends name="Waters_gss_time_series_HP_RSRC"/>
  </subgraph>

  <subgraph name="Waters_gss_time_series_HPE_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Waters_gss_time_series_HPE_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="GSScDNAArray_getAndMapProbes"/>
  </subgraph>

  <subgraph name="Waters_gss_time_series_HPE_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">Waters_gss_time_series_HPE_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_gss_oligos_RSRC|@Pberghei_gss_oligos_VER@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">Waters_gss_time_series_HPE_RSRC|@Waters_gss_time_series_HPE_VER@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@Pberghei_chromosomes_VER@</paramValue>
    <paramValue name="insertAnalysisResult"></paramValue>
    <depends name="Waters_gss_time_series_HP_RSRC"/>
  </subgraph>

  <subgraph name="DOZILongOligoArray_getAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">Pberghei_DOZI_oligos_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_DOZI_oligos_RSRC|@Pberghei_DOZI_oligos_VER@</paramValue>
    <paramValue name="isOneChannel">false</paramValue>
    <paramValue name="loadGeneProbeMapping">false</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="bowtieGenomicIndexDir">$$bowtieGenomicIndexDir$$</paramValue>
    <paramValue name="bowtieTranscriptIndexDir">$$bowtieTranscriptIndexDir$$</paramValue>
    <paramValue name="geneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="genomicSeqsDir">$$genomicSeqsDir$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@Pberghei_chromosomes_VER@</paramValue>
    <paramValue name="organismFullName">@PbOrganismFullName@</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Waters_DOZI_array_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Waters_DOZI_array_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="DOZILongOligoArray_getAndMapProbes"/>
  </subgraph>

  <subgraph name="Waters_DOZI_array_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">Pberghei_DOZI_oligos_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_DOZI_oligos_RSRC|@Pberghei_DOZI_oligos_VER@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">Waters_DOZI_array_RSRC|@Waters_DOZI_array_VER@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@Pberghei_chromosomes_VER@</paramValue>
    <paramValue name="insertAnalysisResult"></paramValue>
    <depends name="Waters_DOZI_array_RSRC"/>
  </subgraph>

</workflowGraph>
