<workflowGraph name="pbergheiArrayStudies">

  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="ssaGeneModelFile"/>
  <param name="genomicSeqsFileForSsa"/>
  <param name="transcriptSeqsFileForSsa"/>
  <param name="downloadSiteTranscriptExpressionDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/pbergheiArrayResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

  <subgraph name="GSScDNAArray_getAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">Pberghei_gss_oligos_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_gss_oligos_RSRC|@@Pberghei_gss_oligos_VER@@</paramValue>
    <paramValue name="isOneChannel">false</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@@Pberghei_chromosomes_VER@@</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="limitNU">30</paramValue>
    <paramValue name="numInsertions">10</paramValue>
    <paramValue name="dnaHybridazation">false</paramValue>
    <paramValue name="pairedReadFilePath"></paramValue>
    <paramValue name="countMismatches">true</paramValue>
    <paramValue name="cdfFileName"></paramValue>
    <depends name="mirrorToCluster"/>
  </subgraph>

  <subgraph name="Pb_Waters_gss_time_series_HP_HPE_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_Waters_gss_time_series_HP_HPE_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="GSScDNAArray_getAndMapProbes"/>
  </subgraph>

  <subgraph name="Waters_gss_time_series_HP_HPE_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">Pb_Waters_gss_time_series_HP_HPE_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_gss_oligos_RSRC|@@Pberghei_gss_oligos_VER@@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">Pb_Waters_gss_time_series_HP_HPE_RSRC|@@Pb_Waters_gss_time_series_HP_HPE_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@@Pberghei_chromosomes_VER@@</paramValue>
    <paramValue name="insertAnalysisResult">false</paramValue>
    <paramValue name="probeGenesMappingFile">$$dataDir$$/getAndMap_Pberghei_gss_oligos_RSRC/geneProbeMapping.txt</paramValue>
    <paramValue name="profileSetNames"></paramValue>
    <paramValue name="profileSetSpecs"></paramValue>
    <paramValue name="isLogged">true</paramValue>
    <paramValue name="mappingFileForExpressionStats"></paramValue>
    <paramValue name="calculateExpressionStats">false</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteTranscriptExpressionDataDir$$/Pb_Waters_gss_time_series_HP_HPE</paramValue>
    <depends name="Pb_Waters_gss_time_series_HP_HPE_RSRC"/>
  </subgraph>

  <subgraph name="DOZILongOligoArray_getAndMapProbes" xmlFile="getAndMapProbes.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="probeResourceName">Pberghei_DOZI_oligos_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_DOZI_oligos_RSRC|@@Pberghei_DOZI_oligos_VER@@</paramValue>
    <paramValue name="isOneChannel">false</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@@Pberghei_chromosomes_VER@@</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="limitNU">30</paramValue>
    <paramValue name="numInsertions">1</paramValue>
    <paramValue name="dnaHybridazation">false</paramValue>
    <paramValue name="pairedReadFilePath"></paramValue>
    <paramValue name="countMismatches">true</paramValue>
    <paramValue name="cdfFileName"></paramValue>
    <depends name="mirrorToCluster"/>
  </subgraph>

  <subgraph name="Pb_Waters_DOZI_array_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pb_Waters_DOZI_array_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="DOZILongOligoArray_getAndMapProbes"/>
  </subgraph>

  <subgraph name="Waters_DOZI_array_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="experimentResourceName">Pb_Waters_DOZI_array_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pberghei_DOZI_oligos_RSRC|@@Pberghei_DOZI_oligos_VER@@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">Pb_Waters_DOZI_array_RSRC|@@Pb_Waters_DOZI_array_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pberghei_chromosomes_RSRC|@@Pberghei_chromosomes_VER@@</paramValue>
    <paramValue name="insertAnalysisResult">true</paramValue>
    <paramValue name="probeGenesMappingFile">$$dataDir$$/getAndMap_Pberghei_DOZI_oligos_RSRC/geneProbeMapping.txt</paramValue>
    <paramValue name="profileSetNames"></paramValue>
    <paramValue name="profileSetSpecs"></paramValue>
    <paramValue name="isLogged">true</paramValue>
    <paramValue name="mappingFileForExpressionStats"></paramValue>
    <paramValue name="calculateExpressionStats">false</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteTranscriptExpressionDataDir$$/Pb_Waters_DOZI_array</paramValue>
    <depends name="Pb_Waters_DOZI_array_RSRC"/>
  </subgraph>
</workflowGraph>
