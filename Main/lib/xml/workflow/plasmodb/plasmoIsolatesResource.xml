<workflowGraph name="plasmoIsolatesResource">
      <param name="commonResourceXmlFile"/>
      <param name="globalResourceXmlFile"/>
      <param name="parentDir"/>

  <constant name="dataDir">$$parentDir$$/plasmoIsolatesResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="PlasmodiumGenbankIsolates_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">PlasmodiumGenbankIsolates_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$commonResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="isolateVocabulary_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">isolateVocabulary_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$globalResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Continents_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Continents_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$globalResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="isolateVocabularyMap_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">isolateVocabularyMap_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$commonResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="PlasmodiumGenbankIsolates_RSRC"/>
    <depends name="isolateVocabulary_RSRC"/>
  </subgraph>

</workflowGraph>
