<workflowGraph name="pfalciparumRnaSeqResources">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>

  <constant name="dataDir">$$parentDataDir$$/pfalciparumRnaSeqResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="getAndAnalyzeDuffyRnaSeq" xmlFile="getAndAnalyzeRnaSeq.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="rnaSeqResourceName">Pf_RNASeq_Patrick_Duffy_RSRC</paramValue>
    <paramValue name="rnaSeqIntensityResourceName">Pf_RNASeq_Intensity_Profiles_Patrick_Duffy_RSRC</paramValue>
    <paramValue name="indexFileDirForShortSeqAligner"></paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Patrick_Duffy_RSRC|@Pf_RNASeq_Patrick_Duffy_VER@</paramValue>
    <paramValue name="rnaSeqIntensityExtDbRlsSpec">Pf_RNASeq_Patrick_Duffy_RSRC|@Pf_RNASeq_Patrick_Duffy_VER@</paramValue>
    <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@P.falciparum_chromosomes_VER@</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <subgraph name="Pfalciparum_Newbold_RNASeq_Intensity_Profiles_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pfalciparum_Newbold_RNASeq_Intensity_Profiles_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
</workflowGraph>
