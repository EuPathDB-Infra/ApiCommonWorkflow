<workflowGraph name="pchabaudiDbxRefsResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>
      <param name="euPathUniProtLinkDownloadFile"/>
      <param name="projectName"/>

  <constant name="dataDir">$$parentDataDir$$/pchabaudiDbxRefsResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="Pc_Entrez_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Entrez_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pc_Phenotype_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Phenotype_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pc_Pubmed_DBRefs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Pubmed_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pc_Entrez_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Entrez_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
  </subgraph>

  <subgraph name="Pc_Uniprot_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_Uniprot_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <step name="makeEuPathUniProtLinkFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeEuPathUniProtLinkFile">
      <paramValue name="dbrefExtDbRlsSpec">Pc_Uniprot_Gene_dbxref_RSRC|@Pc_Uniprot_Gene_dbxref_VER@</paramValue>
      <paramValue name="outputFile">$$euPathUniProtLinkDownloadFile$$</paramValue>
      <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="Pc_Uniprot_Gene_dbxref_RSRC"/>
    </step>

</workflowGraph>
