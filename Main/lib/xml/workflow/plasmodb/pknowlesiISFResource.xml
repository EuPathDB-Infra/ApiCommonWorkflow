<workflowGraph name="pknowlesiISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/pknowlesiISFResource</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="Pknowlesi_chromosomes_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pknowlesi_chromosomes_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Pknowlesi_chromosomes_annotations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pknowlesi_chromosomes_annotations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Pknowlesi_chromosomes_RSRC"/>
  </subgraph>

  <subgraph name="Pknowlesi_contigs_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pknowlesi_contigs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Pknowlesi_chromosomes_RSRC"/>
  </subgraph>

  <subgraph name="Pknowlesi_contigs_annotations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pknowlesi_contigs_annotations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Pknowlesi_contigs_RSRC"/>
  </subgraph>

  <subgraph name="Pk_previous_aliases_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pk_previous_aliases_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="Pknowlesi_chromosomes_annotations_RSRC"/>
    <depends name="Pknowlesi_contigs_annotations_RSRC"/>
  </subgraph>
</workflowGraph>
