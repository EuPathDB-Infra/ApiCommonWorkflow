<workflowGraph name="pchabaudiGeneAliasesResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/pchabaudiGeneAliasesResource</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="Pc_geneDB_Ids_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pc_geneDB_Ids_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <subgraph name="Pc_previous_aliases_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pc_previous_aliases_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>


    <subgraph name="Pc_CorrectGeneAliases_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pc_CorrectGeneAliases_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Pc_geneDB_Ids_RSRC"/>
      <depends name="Pc_previous_aliases_RSRC"/>
    </subgraph>


</workflowGraph>
