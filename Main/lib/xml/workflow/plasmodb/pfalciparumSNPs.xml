<workflowGraph name="pfalciparumSNPs">
  <param name="parentDataDir"/>
  <param name="organismName"/>
  <param name="organismFullName"/>
  <param name="organismResourceXmlFile"/>
  <param name="genomicSeqsFile"/>

  <constant name="dataDir">$$parentDataDir$$</constant>

  <subgraph name="SuSnps" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">Pf_Su_SNPs_RSRC|@Pf_Su_SNPs_VER@</paramValue>
    <paramValue name="snpResourceName">Pf_Su_SNPs_RSRC</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
  </subgraph>

  <subgraph name="broadSnps" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">Pf_Broad_SNPs_RSRC|@Pf_Broad_SNPs_VER@</paramValue>
    <paramValue name="snpResourceName">Pf_Broad_SNPs_RSRC</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
  </subgraph>

  <subgraph name="Broad_Isolate_Barcode_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Broad_Isolate_Barcode_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="broadSnps"/>
  </subgraph>

  <subgraph name="Broad_3K_Genotyping_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Broad_3K_Genotyping_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="broadSnps"/>
  </subgraph>

  <subgraph name="Broad_75K_Genotyping_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Broad_75K_Genotyping_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="broadSnps"/>
  </subgraph>

  <subgraph name="sangerItGhanaSnps" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">$$sangerItGhanaSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpResourceName">sangerItGhanaSnps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
  </subgraph>

  <subgraph name="sangerReichenowiSnps" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">sangerPReichenowiSnps_RSRC|@sangerPReichenowiSnps_VER@</paramValue>
    <paramValue name="snpResourceName">sangerPReichenowiSnps_RSRC</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
  </subgraph>

  <subgraph name="WinzelerGeneticVariationArray_RSRC" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">WinzelerGeneticVariationArray_RSRC|@WinzelerGeneticVariationArray_VER@</paramValue>
    <paramValue name="snpResourceName">WinzelerGeneticVariationArray_RSRC</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
  </subgraph>

  <subgraph name="combinedSnps" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">Pf_plasmoDbCombinedSnps_RSRC|@Pf_plasmoDbCombinedSnps_VER@</paramValue>
    <paramValue name="snpResourceName">Pf_plasmoDbCombinedSnps_RSRC</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@Pfalciparum_chromosomes_VER@</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
  </subgraph>

</workflowGraph>
