<workflowGraph name="pfalciparumSNPs">
  <param name="parentDataDir"/>
  <param name="organismName"/>
  <param name="organismFullName"/>
  <param name="genomeExtDbRlsSpec"/>
  <param name="suSnpExtDbRlsSpec"/>
  <param name="broadSnpExtDbRlsSpec"/>
  <param name="winzelerSnpExtDbRlsSpec"/>
  <param name="sangerItGhanaSnpExtDbRlsSpec"/>
  <param name="sangerReichenowiSnpExtDbRlsSpec"/>
  <param name="combinedSnpExtDbRlsSpec"/>
  <param name="organismResourceXmlFile"/>
  <param name="genomicSeqsFile"/>

  <constant name="dataDir">$$parentDataDir$$</constant>

  <subgraph name="SuSNPs" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">$$suSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpResourceName">Su_SNPs</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
  </subgraph>

  <subgraph name="broadSnps_Mummer" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">$$broadSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpResourceName">Broad_SNPs</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Broad_Isolate_BarcodeResource" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Broad_Isolate_Barcode</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="dataDir">$$dataDir$$/Broad_Isolate_Barcode</paramValue>
    <depends name="broadSnps_Mummer"/>
  </subgraph>

  <subgraph name="Broad_3K_GenotypingResource" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Broad_3K_Genotyping</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="dataDir">$$dataDir$$/Broad_3K_Genotyping</paramValue>
    <depends name="broadSnps_Mummer"/>
  </subgraph>

  <subgraph name="Broad_75K_GenotypingResource" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Broad_75K_Genotyping</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="dataDir">$$dataDir$$/Broad_75K_Genotyping</paramValue>
    <depends name="broadSnps_Mummer"/>
  </subgraph>

  <subgraph name="sangerItGhanaSnps_Mummer" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">$$sangerItGhanaSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpResourceName">sangerItGhanaSnps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="sangerReichenowiSnps_Mummer" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">$$sangerReichenowiSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpResourceName">sangerReichenowiSnps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="winzelerSnps_Mummer" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">$$winzelerSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpResourceName">WinzelerGeneticVariationArray</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="combinedSnps_Mummer" xmlFile="getAndAnalyzeSNPs.xml">
    <paramValue name="snpExtDbRlsSpec">$$combinedSnpExtDbRlsSpec$$</paramValue>
    <paramValue name="snpResourceName">plasmoDbCombinedSnps</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismFullName">$$organismFullName$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="strain">3D7</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
