<workflowGraph name="pfalciparumSpecificAnalysis">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismName"/>
  <param name="organismFullName"/>
  <param name="genomeExtDbRlsSpec"/>
  <param name="suSnpExtDbRlsSpec"/>
  <param name="broadSnpExtDbRlsSpec"/>
  <param name="winzelerSnpExtDbRlsSpec"/>
  <param name="sangerItGhanaSnpExtDbRlsSpec"/>
  <param name="sangerReichenowiSnpExtDbRlsSpec"/>
  <param name="combinedSnpExtDbRlsSpec"/>
  <param name="sageTagExtDbRlsSpec"/>
  <param name="oligoExtDbRlsSpec"/>
  <param name="genbankIsolateExtDbRlsSpec"/>
  <param name="stunnenberChipExtDbRlsSpec"/>
  <param name="scherfChipExtDbRlsSpec"/>
  <param name="vossChipExtDbRlsSpec"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/$$organismName$$Specific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/$$organismName$$</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/$$organismName$$LoadGenome/genomicSeqs.fsa</constant>


  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="mapIsolatesToGenome" xmlFile="plasmodb/pfalciparumMapIsolatesToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismName">$$organismName$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="genbankIsolateExtDbRlsSpec">$$genbankIsolateExtDbRlsSpec$$</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="isolatesResource"/>
    </subgraph>

    <subgraph name="snps" xmlFile="plasmodb/pfalciparumSNPs.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismName">$$organismName$$</paramValue>
      <paramValue name="organismFullName">$$organismFullName$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <paramValue name="suSnpExtDbRlsSpec">$$suSnpExtDbRlsSpec$$</paramValue>
      <paramValue name="broadSnpExtDbRlsSpec">$$broadSnpExtDbRlsSpec$$</paramValue>
      <paramValue name="winzelerSnpExtDbRlsSpec">$$winzelerSnpExtDbRlsSpec$$</paramValue>
      <paramValue name="sangerItGhanaSnpExtDbRlsSpec">$$sangerItGhanaSnpExtDbRlsSpec$$</paramValue>
      <paramValue name="sangerReichenowiSnpExtDbRlsSpec">$$sangerReichenowiSnpExtDbRlsSpec$$</paramValue>
      <paramValue name="combinedSnpExtDbRlsSpec">$$combinedSnpExtDbRlsSpec$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="chIP-chips" xmlFile="plasmodb/pfalciparumChIP.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="stunnenberChipExtDbRlsSpec">$$stunnenberChipExtDbRlsSpec$$</paramValue>
      <paramValue name="scherfChipExtDbRlsSpec">$$scherfChipExtDbRlsSpec$$</paramValue>
      <paramValue name="vossChipExtDbRlsSpec">$$vossChipExtDbRlsSpec$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="sageTagDesignResource" xmlFile="loadResource.xml">
      <paramValue name="resourceName">WirthSageTagArrayDesign</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <subgraph name="mapSageTags" xmlFile="mapSageTags.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="sageTagExtDbRlsSpec">$$sageTagExtDbRlsSpec$$</paramValue>
    <paramValue name="studyName">Transcriptional profile of P.falciparum by Serial analysis of gene expression</paramValue>
    <paramValue name="paramValue">25372</paramValue>  
    <paramValue name="nickName">mapSageTags</paramValue>   
    <paramValue name="calFreq">true</paramValue>
    <depends name="sageTagDesignResource"/>
  </subgraph>

    <subgraph name="sageTagFreqsResource" xmlFile="loadResource.xml">
      <paramValue name="resourceName">WirthSageTagFreqs</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="sageTagDesignResource"/>
    </subgraph>

    <subgraph name="correctGeneAliasesResource" xmlFile="loadResource.xml">
      <paramValue name="resourceName">CorrectGeneAliases</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <step name="makeGeneAliasesMappingFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeGeneAliasesMappingFile">
     <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
     <paramValue name="outputFile">$$downloadSiteDataDir$$/$$organismName$$GeneAlias_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <depends name="correctGeneAliasesResource"/>
  </step>

  <subgraph name="dbxRefsResources" xmlFile="plasmodb/pfalciparumDbxRefsResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="euPathUniProtLinkDownloadFile">$$downloadSiteDataDir$$/$$organismName$$EuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="correctGeneAliasesResource"/>
  </subgraph>

  <subgraph name="miscResources" xmlFile="plasmodb/pfalciparumMiscResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="geneExpressionResources" xmlFile="plasmodb/pfalciparumGeneExpressionResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="oligoExtDbRlsSpec">$$oligoExtDbRlsSpec$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <depends name="correctGeneAliasesResource"/>
    <dependsGlobal name="MGEDOntologyEntryResource"/>
  </subgraph>

    <subgraph name="GOAssociations_PfalciparumResource" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_Pfalciparum</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GOResource"/>
      <dependsGlobal name="GOEvidenceCodesResource"/>
    </subgraph>

    <subgraph name="ECAssociationsResource" xmlFile="loadResource.xml">
      <paramValue name="resourceName"></paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDBResource"/>
    </subgraph>

    <subgraph name="loadAnticodonsResource" xmlFile="loadResource.xml">
      <paramValue name="resourceName">loadAnticodons</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
</workflowGraph>
