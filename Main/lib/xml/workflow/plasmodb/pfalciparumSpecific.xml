<workflowGraph name="pfalciparumSpecific">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/PfalciparumSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/Pfalciparum</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/PfalciparumLoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/PfalciparumLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/PfalciparumLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/PfalciparumLoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <subgraph name="mapIsolatesToGenome" xmlFile="plasmodb/plasmoMapIsolatesToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismName">Pfalciparum</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <depends name="mirrorToCluster"/>
      <dependsGlobal name="isolatesResource"/>
    </subgraph>

    <subgraph name="snps" xmlFile="plasmodb/pfalciparumSNPs.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Pfalciparum_geneDB_Ids_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pfalciparum_geneDB_Ids_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Pf_CorrectGeneAliases_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pf_CorrectGeneAliases_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Pfalciparum_geneDB_Ids_RSRC"/>
    </subgraph>

    <subgraph name="arrayResources" xmlFile="plasmodb/pfalciparumArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <depends name="Pf_CorrectGeneAliases_RSRC"/>
    </subgraph>


  <subgraph name="rnaSeqAndChipSeq" xmlFile="plasmodb/pfalciparumRnaSeqAndChipSeq.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <subgraph name="WirthSageTagArrayDesign_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">WirthSageTagArrayDesign_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <subgraph name="mapSageTags" xmlFile="mapSageTags.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="sageTagExtDbRlsSpec">WirthSageTagArrayDesign_RSRC|@@WirthSageTagArrayDesign_VER@@</paramValue>
    <paramValue name="studyName">Transcriptional profile of P.falciparum by Serial analysis of gene expression</paramValue>
    <paramValue name="paramValue">25372</paramValue>  
    <paramValue name="nickName">mapSageTags</paramValue>   
    <paramValue name="calFreq">true</paramValue>
    <depends name="WirthSageTagArrayDesign_RSRC"/>
  </subgraph>

    <subgraph name="WirthSageTagFreqs_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">WirthSageTagFreqs_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="WirthSageTagArrayDesign_RSRC"/>
    </subgraph>

  <step name="makeGeneAliasesMappingFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeGeneAliasesMappingFile">
     <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
     <paramValue name="outputFile">$$downloadSiteDataDir$$/PfalciparumGeneAlias_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <depends name="Pf_CorrectGeneAliases_RSRC"/>
  </step>

  <subgraph name="dbxRefsResources" xmlFile="plasmodb/pfalciparumDbxRefsResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="euPathUniProtLinkDownloadFile">$$downloadSiteDataDir$$/PfalciparumEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="Pf_CorrectGeneAliases_RSRC"/>
  </subgraph>

  <subgraph name="miscResources" xmlFile="plasmodb/pfalciparumMiscResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <subgraph name="GOAssociations_Pfalciparum_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_Pfalciparum_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GO_RSRC"/>
      <dependsGlobal name="GO_evidence_codes_RSRC"/>
    </subgraph>

    <subgraph name="Pfalciparum_ECNumbers_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pfalciparum_ECNumbers_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDB_RSRC"/>
    </subgraph>

    <subgraph name="Pfalciparum_anticodons_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pfalciparum_anticodons_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Pf_PiggyBac_Clones_Adams_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pf_PiggyBac_Clones_Adams_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <step name="mirrorPf_PiggyBac_Clones_Adams_RSRCToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$/Pf_PiggyBac_Clones_Adams_RSRC</paramValue>
      <depends name="Pf_PiggyBac_Clones_Adams_RSRC"/>
    </step>

    <subgraph name="PfMapPiggyBacClonesToGenomicSeqs" xmlFile="runBlastAndLoadFeatures.xml">
      <paramValue name="dataDir">$$dataDir$$/Pf_PiggyBac_Clones_Adams_RSRC/PfMapPiggyBacClonesToGenomicSeqs</paramValue>
      <paramValue name="queryFile">$$dataDir$$/Pf_PiggyBac_Clones_Adams_RSRC/PiggyBac_Clones_Adams.fsa</paramValue>
      <paramValue name="subjectFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="blastArgs">-nogaps V=1 E=1e-10 B=1 hspsepQmax=50000 hspsepSmax=50000 -span1 -topcomboN=1</paramValue>
      <paramValue name="idRegex">(\\S+)</paramValue>
      <paramValue name="blastType">blastn</paramValue>
      <paramValue name="vendor">wu</paramValue>
      <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="extDbRlsSpec">Pf_PiggyBac_Clones_Adams_RSRC|@@Pf_PiggyBac_Clones_Adams_VER@@</paramValue>
      <paramValue name="organismFullName">@@PfOrganismFullName@@</paramValue>
      <depends name="mirrorPf_PiggyBac_Clones_Adams_RSRCToCluster"/>
    </subgraph>

  <subgraph name="PlasmoSpecificMakeAndFormatDownloadFiles" xmlFile="plasmodb/plasmoSpecificMakeAndFormatDownloadFiles.xml">
    <paramValue name="downloadSiteDataDir">$$downloadSiteDataDir$$</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="webServicesDataDir">$$webServicesDataDir$$</paramValue>
    <paramValue name="genbankIsolateExtDbRlsSpec">PlasmodiumGenbankIsolates_RSRC|@@PlasmodiumGenbankIsolates_VER@@</paramValue>
    <paramValue name="barcodeIsolateExtDbRlsSpec">Broad_Isolate_Barcode_RSRC|@@Broad_Isolate_Barcode_VER@@</paramValue>
    <depends name="snps"/>
  </subgraph>

</workflowGraph>
