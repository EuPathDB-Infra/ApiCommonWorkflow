<workflowGraph name="pfNewboldRnaSeq">
    <param name="parentDataDir"/>
    <param name="organismResourceXmlFile"/>
    <param name="ssaGeneModelFile"/>
    <param name="genomicSeqsFileForSsa"/>
    <param name="transcriptSeqsFileForSsa"/>
    <param name="downloadSiteTranscriptExpressionDataDir"/>

    <constant name="dataDir">$$parentDataDir$$/pfNewboldRnaSeq</constant>
    <constant name="samplesDataDir">$$dataDir$$/samples</constant>
    <constant name="seqCoverageAndIntensityDataDir">$$dataDir$$/seqCoverageAndIntensity</constant>


    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

    <step name="makeSeqCoverageAndIntensityDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$seqCoverageAndIntensityDataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <step name="makeSamplesDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$samplesDataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeSamplesDataDir"/>
    </step>

    <subgraph name="Pf_RNASeq_Newbold_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pf_RNASeq_Newbold_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="analyzeHour0Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Hour0</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/hour0.fsa</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeHour0/Hour0.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Hour0.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">transcriptSeqsFileForSsa</paramValue>
      <paramValue name="pairedReadFilePath"></paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Pf_RNASeq_Newbold_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="analyzeHour8Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Hour8</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/hour8.fsa</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeHour8/Hour8.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Hour8.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">transcriptSeqsFileForSsa</paramValue>
      <paramValue name="pairedReadFilePath"></paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Pf_RNASeq_Newbold_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>


    <subgraph name="analyzeHour16Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Hour16</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/hour16.fsa</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeHour16/Hour16.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Hour16.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">transcriptSeqsFileForSsa</paramValue>
      <paramValue name="pairedReadFilePath"></paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Pf_RNASeq_Newbold_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>


    <subgraph name="analyzeHour24Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Hour24</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/hour24.fsa</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeHour24/Hour24.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Hour24.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">transcriptSeqsFileForSsa</paramValue>
      <paramValue name="pairedReadFilePath"></paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Pf_RNASeq_Newbold_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>


    <subgraph name="analyzeHour32Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Hour32</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/hour32.fsa</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeHour32/Hour32.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Hour32.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">transcriptSeqsFileForSsa</paramValue>
      <paramValue name="pairedReadFilePath"></paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Pf_RNASeq_Newbold_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>


    <subgraph name="analyzeHour40Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Hour40</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/hour40.fsa</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeHour40/Hour40.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Hour40.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">transcriptSeqsFileForSsa</paramValue>
      <paramValue name="pairedReadFilePath"></paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Pf_RNASeq_Newbold_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="analyzeHour48Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Hour48</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/hour48.fsa</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeHour48/Hour48.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Hour48.int</paramValue>
      <paramValue name="makeIntensityFile">true</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">transcriptSeqsFileForSsa</paramValue>
      <paramValue name="pairedReadFilePath"></paramValue>
      <paramValue name="numInsertions">1</paramValue>
      <paramValue name="limitNU">30</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Pf_RNASeq_Newbold_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>


    <step name="mergeMinIntensityFiles" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MergeIntensityFiles">
      <paramValue name="inputFilesDir">$$samplesDataDir$$</paramValue>
      <paramValue name="outputFile">$$seqCoverageAndIntensityDataDir$$/minProfiles.txt</paramValue>
      <paramValue name="profileElementOrder">Hour0.int.min,Hour8.int.min,Hour16.int.min,Hour24.int.min,Hour32.int.min,Hour40.int.min,Hour48.int.min</paramValue>
      <depends name="makeSeqCoverageAndIntensityDataDir"/>
      <depends name="analyzeHour0Sample"/>
      <depends name="analyzeHour8Sample"/>
      <depends name="analyzeHour16Sample"/>
      <depends name="analyzeHour24Sample"/>
      <depends name="analyzeHour32Sample"/>
      <depends name="analyzeHour40Sample"/>
      <depends name="analyzeHour48Sample"/>
    </step>

    <step name="mergeMaxIntensityFiles" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MergeIntensityFiles">
      <paramValue name="inputFilesDir">$$samplesDataDir$$</paramValue>
      <paramValue name="outputFile">$$seqCoverageAndIntensityDataDir$$/maxProfiles.txt</paramValue>
      <paramValue name="profileElementOrder">Hour0.int.max,Hour8.int.max,Hour16.int.max,Hour24.int.max,Hour32.int.max,Hour40.int.max,Hour48.int.max</paramValue>
      <depends name="makeSeqCoverageAndIntensityDataDir"/>
      <depends name="analyzeHour0Sample"/>
      <depends name="analyzeHour8Sample"/>
      <depends name="analyzeHour16Sample"/>
      <depends name="analyzeHour24Sample"/>
      <depends name="analyzeHour32Sample"/>
      <depends name="analyzeHour40Sample"/>
      <depends name="analyzeHour48Sample"/>
    </step>

    <step name="makeMinPercentileProfile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakePercentileProfile">
      <paramValue name="inputFile">$$seqCoverageAndIntensityDataDir$$/minProfiles.txt</paramValue>
      <paramValue name="outputFile">$$seqCoverageAndIntensityDataDir$$/minProfiless.txt.pct</paramValue>
      <paramValue name="ties">min</paramValue>
      <paramValue name="hasHeader">true</paramValue>
      <depends name="mergeMinIntensityFiles"/>
    </step>

    <step name="makeMaxPercentileProfile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakePercentileProfile">
      <paramValue name="inputFile">$$seqCoverageAndIntensityDataDir$$/maxProfiles.txt</paramValue>
      <paramValue name="outputFile">$$seqCoverageAndIntensityDataDir$$/maxProfiles.txt.pct</paramValue>
      <paramValue name="ties">min</paramValue>
      <paramValue name="hasHeader">true</paramValue>
      <depends name="mergeMaxIntensityFiles"/>
    </step>

    <step name="makeDiffProfile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeProfileDiff">
      <paramValue name="maxInputFile">$$seqCoverageAndIntensityDataDir$$/maxProfiles.txt</paramValue>
      <paramValue name="minInputFile">$$seqCoverageAndIntensityDataDir$$/minProfiles.txt</paramValue>     
      <paramValue name="outputFile">$$seqCoverageAndIntensityDataDir$$/diffProfiles.txt</paramValue>
      <paramValue name="hasHeader">true</paramValue>
      <depends name="mergeMinIntensityFiles"/>
      <depends name="mergeMaxIntensityFiles"/>
    </step>

    <step name="loadIntensityProfile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::InsertExpressionProfiles" stepLoadTypes="plugin">
      <paramValue name="configFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/config.txt</paramValue>
      <paramValue name="inputDir">$$seqCoverageAndIntensityDataDir$$</paramValue>
      <paramValue name="extDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <depends name="mergeMinIntensityFiles"/>
      <depends name="makeMinPercentileProfile"/>
      <depends name="makeDiffProfile"/>
    </step>

    <step name="calculateExpressionStats" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::CalculateExpressionStatsForTimeSeries" stepLoadTypes="plugin">
      <paramValue name="profileSetSpecs">Profiles of P.falciparum Newbold mRNA Seq data|Percents of P. falciparum Newbold mRNA Seq data</paramValue>
      <paramValue name="extDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="isLogged">false</paramValue>
      <paramValue name="mappingFile"></paramValue>
      <depends name="loadIntensityProfile"/>
    </step>

    <step name="copyExpressionProfilesToDownloadDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::CopyExpressionProfilesToDownloadDir">
      <paramValue name="copyFromDir">$$seqCoverageAndIntensityDataDir$$</paramValue>
      <paramValue name="configFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/config.txt</paramValue>
      <paramValue name="copyToDir">$$downloadSiteTranscriptExpressionDataDir$$</paramValue>
      <depends name="mergeMinIntensityFiles"/>
      <depends name="mergeMaxIntensityFiles"/>
    </step>
</workflowGraph>
