<workflowGraph name="pfNewboldRnaSeq">
    <param name="parentDataDir"/>
    <param name="organismResourceXmlFile"/>
    <param name="bowtieGenomicIndexDir"/>
    <param name="bowtieTranscriptIndexDir"/>
    <param name="genomicSeqsFile"/>
    <param name="genomicSeqsDir"/>
    <param name="ssaGeneModelFile"/>
    <param name="genomicSeqsFileForSsa"/>
    <param name="maxIntronSize"/>

    <constant name="dataDir">$$parentDataDir$$/pfNewboldRnaSeq</constant>
    <constant name="samplesDataDir">$$dataDir$$/samples</constant>
    <constant name="seqCoverageAndIntensityDataDir">$$dataDir$$/seqCoverageAndIntensity</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

    <step name="makeSeqCoverageAndIntensityDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$seqCoverageAndIntensityDataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <step name="makeSamplesDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$samplesDataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeSamplesDataDir"/>
    </step>

    <subgraph name="Pf_RNASeq_Newbold_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pf_RNASeq_Newbold_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="analyzeHour0Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
      <paramValue name="parentDataDir">$$samplesDataDir$$</paramValue>
      <paramValue name="sampleName">Hour0</paramValue>
      <paramValue name="bowtieGenomicIndexDir">$$bowtieGenomicIndexDir$$</paramValue>
      <paramValue name="bowtieTranscriptIndexDir">$$bowtieTranscriptIndexDir$$</paramValue>
      <paramValue name="genomicSeqsDir">$$genomicSeqsDir$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="shortSeqsFile">$$dataDir$$/Pf_RNASeq_Newbold_RSRC/final/hour0.fsa</paramValue>
      <paramValue name="outputCoverageFile">$$samplesDataDir$$/analyzeHour0/Hour0.cov</paramValue>
      <paramValue name="outputIntensityFile">$$samplesDataDir$$/Hour0.int</paramValue>
      <paramValue name="computePeaks">false</paramValue>
      <paramValue name="readType">single</paramValue>
      <paramValue name="isChipSeq">false</paramValue>
      <paramValue name="rnaSeqExtDbRlsSpec">Pf_RNASeq_Newbold_RSRC|@@Pf_RNASeq_Newbold_VER@@</paramValue>
      <paramValue name="genomeExtDbSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
      <paramValue name="readLength">64</paramValue>
      <paramValue name="maxIntronSize">$$maxIntronSize$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="nonUniqueMappingSuppressLimits">30</paramValue>
      <paramValue name="useTranscripts">true</paramValue>
      <depends name="makeSamplesDataDir"/>
      <depends name="Pf_RNASeq_Newbold_RSRC"/>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="analyzeHour8Sample" xmlFile="analyzeRnaSeqOrChipSeqSample.xml">
