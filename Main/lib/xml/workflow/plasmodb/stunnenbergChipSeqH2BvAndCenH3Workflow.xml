<workflowGraph name="stunnenbergChipSeqH2BvAndCenH3Workflow">
  <param name="projectName"/>
  <param name="projectVersion"/>

  <constant name="genomicSeqsFileForSsa">PfalciparumLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">PfalciparumLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">PfalciparumLoadGenome/geneModelForSsa.txt</constant>

   <subgraph name="pfLoadGenome" xmlFile="miniflow/miniLoadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Pfalciparum</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
    <paramValue name="ncbiTaxonId">36329</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
  </subgraph>

  <subgraph name="getAndAnalyzeStunnenbergChipSeqH2BvAndCenH3" xmlFile="plasmodb/pfStunnenbergChipSeqH2BvAndCenH3.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismResourceXmlFile">plasmodb/pfalciparum.xml</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa"></paramValue>
    <depends name="pfLoadGenome"/>
  </subgraph>

  <subgraph name="Pf_Boothroyd_Elias_Moritz_Phosphoroteome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pf_Boothroyd_Elias_Moritz_Phosphoproteome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">plasmodb/pfalciparum.xml</paramValue>
    <paramValue name="parentDataDir"></paramValue>
  </subgraph>

  <subgraph name="Pf_Boothroyd_Elias_Moritz_Totalproteome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Pf_Boothroyd_Elias_Moritz_Totalproteome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">plasmodb/pfalciparum.xml</paramValue>
    <paramValue name="parentDataDir"></paramValue>
  </subgraph>

  <subgraph name="Su_Pfalciparum_Genetic_Map_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Su_Pfalciparum_Genetic_Map_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">plasmodb/pfalciparum.xml</paramValue>
    <paramValue name="parentDataDir"></paramValue>
  </subgraph>

    <subgraph name="Pf_array_GPL7189_eQTL_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Pf_array_GPL7189_eQTL_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">plasmodb/pfalciparum.xml</paramValue>
      <paramValue name="parentDataDir"></paramValue>
    </subgraph>

  <subgraph name="Ferdig_Pfalciparum_eQTL_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Ferdig_Pfalciparum_eQTL_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">plasmodb/pfalciparum.xml</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <depends name="Pf_array_GPL7189_eQTL_RSRC"/>
  </subgraph>

  <subgraph name="Pf_Ferdig_Pfalciparum_eQTL_analyzeMicroarrayExpression" xmlFile="analyzeMicroarrayExpression.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="experimentResourceName">Ferdig_Pfalciparum_eQTL_RSRC</paramValue>
    <paramValue name="chipProbeExtDbRlsSpec">Pf_array_GPL7189_eQTL_RSRC|@@Pf_array_GPL7189_eQTL_VER@@</paramValue>
    <paramValue name="experimentExtDbRlsSpec">Ferdig_Pfalciparum_eQTL_RSRC|@@Ferdig_Pfalciparum_eQTL_VER@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Pfalciparum_chromosomes_RSRC|@@Pfalciparum_chromosomes_VER@@</paramValue>
    <paramValue name="insertAnalysisResult">false</paramValue>
    <paramValue name="probeGenesMappingFile">Pf_array_GPL7189_eQTL_RSRC/geneProbeMapping.txt</paramValue>
    <paramValue name="profileSetNames"></paramValue>
    <paramValue name="profileSetSpecs"></paramValue>
    <paramValue name="isLogged">true</paramValue>
    <paramValue name="mappingFileForExpressionStats"></paramValue>
    <paramValue name="calculateExpressionStats">false</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">Ferdig_Pfalciparum_eQTL_RSRC</paramValue>
    <depends name="Ferdig_Pfalciparum_eQTL_RSRC"/>
  </subgraph>

</workflowGraph>
