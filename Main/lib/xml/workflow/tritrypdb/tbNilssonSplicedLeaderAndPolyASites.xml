      <paramValue name="outputIndexDir">$$dataDir$$/indexes</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <subgraph name="Tbrucei_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tbrucei_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <step name="runBowtie_SRR058969" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
