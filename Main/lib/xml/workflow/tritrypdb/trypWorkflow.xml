<workflowGraph name="trypWorkflow">
  <param name="projectName"/>
  <param name="projectVersion"/>

  <constant name="comparativeGenomicsFastaDir">common/comparativeGenomics/Fasta/</constant>
  <constant name="comparativeGenomicsGffDir">common/comparativeGenomics/Gff/</constant>

  <globalSubgraph name="global" xmlFile="globalGraph.xml">
    <paramValue name="isolatesResourceReference"></paramValue>
    <paramValue name="loadIsolates">false</paramValue>
    <paramValue name="commonResourceXmlFile">tritrypdb/tritrypdbCommon.xml</paramValue>
    <paramValue name="globalResourceXmlFile">globalResources.xml</paramValue>
  </globalSubgraph>

  <subgraph name="common" xmlFile="common.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="comparativeGenomicsFastaDir">$$comparativeGenomicsFastaDir$$</paramValue>
    <paramValue name="comparativeGenomicsGffDir">$$comparativeGenomicsGffDir$$</paramValue>
    <paramValue name="webServicesDataDir">@@webServicesDir@@/$$projectName$$/release-$$projectVersion$$/</paramValue>
  </subgraph>

<!--Cfasci-->

  <subgraph name="CfasciLoadGenome" xmlFile="loadGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Cfasci</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Cfasci_contigs_RSRC|@@Cfasci_contigs_VER@@</paramValue>
    <paramValue name="organismResourceXmlFile">tritrypdb/cfasci.xml</paramValue>
    <paramValue name="organismSpecificISFReference">tritrypdb/cfasciISFResource.xml</paramValue>
    <paramValue name="ncbiTaxonId">5656</paramValue>
    <paramValue name="downloadSiteDataDir">@@downloadSiteDir@@/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="extractProteins">false</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="haveOrganelleGenomeSequence">false</paramValue>
    <paramValue name="organelleInitDownloadDirSubgraph">initMitoAndApicoplastDownloadDirs.xml</paramValue>
    <depends name="common"/>
    <dependsGlobal name="taxonomy_RSRC"/> 
    <dependsGlobal name="SO_RSRC"/>
  </subgraph>

  <subgraph name="CfasciMakeAndMaskTopLevelGenome" xmlFile="makeAndMaskTopLevelGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Cfasci</paramValue>
    <paramValue name="organismFullName">@@CfasciOrganismFullName@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Cfasci_contigs_RSRC|@@Cfasci_contigs_VER@@</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFileResourceName"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFile">false</paramValue>
    <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
    <paramValue name="organismResourceXmlFile">tritrypdb/cfasci.xml</paramValue>
    <paramValue name="comparativeGenomicsFastaDir">$$comparativeGenomicsFastaDir$$</paramValue>
    <paramValue name="comparativeGenomicsGffDir">$$comparativeGenomicsGffDir$$</paramValue>
    <depends name="CfasciLoadGenome"/>
  </subgraph>

  <subgraph name="CfasciPostLoadGenome" xmlFile="postLoadGenome.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">Cfasci</paramValue>
    <paramValue name="organismFullName">@@CfasciOrganismFullName@@</paramValue>
    <paramValue name="organismAbbrev">Cfasci</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Cfasci_contigs_RSRC|@@Cfasci_contigs_VER@@</paramValue>
    <paramValue name="iedbResourceName"></paramValue>
    <paramValue name="iedbVersion"></paramValue>
    <paramValue name="iedbDbRefResourceName"></paramValue>
    <paramValue name="dbESTResourceName"></paramValue>
    <paramValue name="dbESTVersion"></paramValue>
    <paramValue name="loadDbest">false</paramValue>
    <paramValue name="ncbiTaxonId">5656</paramValue>
    <paramValue name="parentNcbiTaxonId">5656</paramValue>
    <paramValue name="usePredictedTranscripts">false</paramValue>
    <paramValue name="predictedTranscriptsExtDbRlsSpec"></paramValue>
    <paramValue name="runtRNAScan">false</paramValue>
    <paramValue name="runExportPred">false</paramValue>
    <paramValue name="maxIntronSize">2000</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">Eukaryota, Kinetoplastida, Trypanosoma</paramValue>
    <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
    <paramValue name="genomeSize">35386209</paramValue>
    <paramValue name="skipProteomeAnalysis">true</paramValue>
    <paramValue name="skipGenomeAnalysis">false</paramValue>
    <paramValue name="loadEpitope">false</paramValue>
    <paramValue name="makeAssemblySeqs">false</paramValue>
    <paramValue name="makeAssemblies">false</paramValue>
    <paramValue name="mapAssemblySeqsAndAssemblies">false</paramValue>
    <paramValue name="loadOrganismSpecificESTResource">false</paramValue>
    <paramValue name="loadOrganismSpecificMassSpecResource">false</paramValue>
    <paramValue name="distanceBetweenStartsForAlignmentsInSameCluster">20000</paramValue>
    <paramValue name="organismSpecificESTResourcesReference"></paramValue>
    <paramValue name="organismSpecificMassSpecResourcesReference"></paramValue>
    <paramValue name="organismSpecificProductNameResourcesReference"></paramValue>
    <paramValue name="organismResourceXmlFile">tritrypdb/cfasci.xml</paramValue>
    <depends name="CfasciLoadGenome"/>
  </subgraph>

  <subgraph name="CfasciMakeAndFormatDownloadFiles" xmlFile="makeAndFormatDownloadFiles.xml">
    <paramValue name="downloadSiteDataDir">@@downloadSiteDir@@/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="organismName">Cfasci</paramValue>
    <paramValue name="organismFullName">@@CfasciOrganismFullName@@</paramValue>
    <paramValue name="ncbiTaxonId">5656</paramValue>
    <paramValue name="parentNcbiTaxonId">5656</paramValue>
    <paramValue name="genomeSource">@@CfasciGenomeSource@@</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="dbESTExtDbRlsSpec"></paramValue>
    <paramValue name="webServicesDataDir">@@webServicesDir@@/$$projectName$$/release-$$projectVersion$$/</paramValue>
    <paramValue name="genomeExtDbRlsSpec">Cfasci_contigs_RSRC|@@Cfasci_contigs_VER@@</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="minOrfLength">50</paramValue>
    <paramValue name="deprecated">false</paramValue>
    <paramValue name="soTermIdsOrNames">contig</paramValue>
    <paramValue name="makeESTDownloadFile">false</paramValue>
    <paramValue name="makeProteomeDownloadFile">false</paramValue>
    <paramValue name="makeGenomicDownloadFile">true</paramValue>
    <depends name="CfasciPostLoadGenome"/>
    <depends name="CfasciMakeAndMaskTopLevelGenome"/>
  </subgraph>

