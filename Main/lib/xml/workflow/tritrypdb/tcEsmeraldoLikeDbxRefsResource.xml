<workflowGraph name="tcEsmeraldoLikeDbxRefsResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>
      <param name="projectVersion"/>
      <param name="projectName"/>
      <param name="downloadSiteDir"/>

  <constant name="dataDir">$$parentDataDir$$/tcEsmeraldoLikeDbxRefsResources</constant>
  <constant name="euPathUniProtLinkDownloadFile">$$downloadSiteDir$$/TcEsmeraldoLikeEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</constant>
  <constant name="geneAliasesMappingDownloadFile">$$downloadSiteDir$$/txt/TcEsmeraldoLikeGeneAlias_$$projectName$$-$$projectVersion$$.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <step name="makeGeneAliasesMappingFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeGeneAliasesMappingFile">
     <paramValue name="genomeExtDbRlsSpec">TcEsmeraldoLike_chromosomes_RSRC|@@TcEsmeraldoLike_chromosomes_VER@@</paramValue>
     <paramValue name="outputFile">$$geneAliasesMappingDownloadFile$$</paramValue>
  </step>
  <subgraph name="Tcruzi_Aguero_SNP_DBRefs_RSRC" xmlFile="loadResource.xml">

    <paramValue name="resourceName">Tcruzi_Aguero_SNP_DBRefs_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Tcruzi_Entrez_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tcruzi_Entrez_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Tcruzi_Uniprot_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tcruzi_Uniprot_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

    <step name="makeEuPathUniProtLinkFile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeEuPathUniProtLinkFile">
      <paramValue name="dbrefExtDbRlsSpec">Tcruzi_Uniprot_Gene_dbxref_RSRC|@@Tcruzi_Uniprot_Gene_dbxref_VER@@</paramValue>
      <paramValue name="outputFile">$$euPathUniProtLinkDownloadFile$$</paramValue>
      <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="Tcruzi_Uniprot_Gene_dbxref_RSRC"/>
    </step>

</workflowGraph>
