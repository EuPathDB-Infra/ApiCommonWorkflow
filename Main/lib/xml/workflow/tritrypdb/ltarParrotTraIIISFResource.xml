<workflowGraph name="ltarParrotTraIIISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/ltarParrotTraIIISFResource</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="ltarParrotTraII_primary_genome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">ltarParrotTraII_primary_genome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="ltarParrotTraII_genome_annotation_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">ltarParrotTraII_genome_annotation_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="ltarParrotTraII_primary_genome_RSRC"/>
  </subgraph>

  <subgraph name="ltarParrotTraII_supercontig_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">ltarParrotTraII_supercontig_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="ltarParrotTraII_primary_genome_RSRC"/>
  </subgraph>

  <subgraph name="ltarParrotTraII_supercontig_annotation_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">ltarParrotTraII_supercontig_annotation_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="ltarParrotTraII_supercontig_RSRC"/>
		<!-- for annothation, the depends should be the related sequence -->
  </subgraph>

  <subgraph name="ltarParrotTraII_contig_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">ltarParrotTraII_contig_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="ltarParrotTraII_primary_genome_RSRC"/>
  </subgraph>

  <subgraph name="ltarParrotTraII_contig_annotation_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">ltarParrotTraII_contig_annotation_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="ltarParrotTraII_contig_RSRC"/>
		<!-- for annotation, the depends should be the related sequence -->
  </subgraph>

</workflowGraph>
