<workflowGraph name="lmajorSpecificAnalysis">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/LmajorSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/Lmajor</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/LmajorLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/LmajorLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/LmajorLoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="sangerCosmidEnds_loadAndAnalyze" xmlFile="loadAndAnalyzeDnaSeq.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">tritrypdb/lmajor.xml</paramValue>
      <paramValue name="organismName">Lmajor</paramValue>
      <paramValue name="genomeExtDbRlsSpec">Lmajor_chromosomes_RSRC|@@Lmajor_chromosomes_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">5664</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Lmajor_sanger_CosmidEnds_RSRC|@@Lmajor_sanger_CosmidEnds_VER@@</paramValue>
      <paramValue name="BACEndsResourceName">Lmajor_sanger_CosmidEnds_RSRC</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsFile">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs.fsa</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="sangerFV1Ends_loadAndAnalyze" xmlFile="loadAndAnalyzeDnaSeq.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">tritrypdb/lmajor.xml</paramValue>
      <paramValue name="organismName">Lmajor</paramValue>
      <paramValue name="genomeExtDbRlsSpec">Lmajor_chromosomes_RSRC|@@Lmajor_chromosomes_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">5664</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Lmajor_sanger_FV1Ends_RSRC|@@Lmajor_sanger_FV1Ends_VER@@</paramValue>
      <paramValue name="BACEndsResourceName">Lmajor_sanger_FV1Ends_RSRC</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsFile">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs.fsa</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="sangerPACEnds_loadAndAnalyze" xmlFile="loadAndAnalyzeDnaSeq.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">tritrypdb/lmajor.xml</paramValue>
      <paramValue name="organismName">Lmajor</paramValue>
      <paramValue name="genomeExtDbRlsSpec">Lmajor_chromosomes_RSRC|@@Lmajor_chromosomes_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">5664</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Lmajor_sanger_PACEnds_RSRC|@@Lmajor_sanger_PACEnds_VER@@</paramValue>
      <paramValue name="BACEndsResourceName">Lmajor_sanger_PACEnds_RSRC</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsFile">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs.fsa</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="sangerBACEnds_loadAndAnalyze" xmlFile="loadAndAnalyzeDnaSeq.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">tritrypdb/lmajor.xml</paramValue>
      <paramValue name="organismName">Lmajor</paramValue>
      <paramValue name="genomeExtDbRlsSpec">Lmajor_chromosomes_RSRC|@@Lmajor_chromosomes_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">5664</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Lmajor_sanger_BACEnds_RSRC|@@Lmajor_sanger_BACEnds_VER@@</paramValue>
      <paramValue name="BACEndsResourceName">Lmajor_sanger_BACEnds_RSRC</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsFile">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs.fsa</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/LmajorLoadGenome/genomicSeqs</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>


  <subgraph name="dbxRefsResources" xmlFile="tritrypdb/lmajorDbxRefsResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="euPathUniProtLinkDownloadFile">$$downloadSiteDataDir$$/LmajorEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="lmajor_CorrectGeneAliases_RSRC"/>
  </subgraph>

    <subgraph name="arrayResources" xmlFile="tritrypdb/lmajorArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
      <depends name=""/>
    </subgraph>

</workflowGraph>