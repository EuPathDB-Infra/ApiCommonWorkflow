<workflowGraph name="tbruceiMassSpecResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/tbruceiMassSpecResource</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="Tbrucei_Proteomics_PF_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tbrucei_Proteomics_PF_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Tbrucei_Ferguson_Phospho_Proteome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tbrucei_Ferguson_Phospho_Proteome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Tbrucei_Almeida_Palmitoylated_Proteome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tbrucei_Almeida_Palmitoylated_Proteome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
