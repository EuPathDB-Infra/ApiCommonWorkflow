<workflowGraph name="tcruziEsmeraldoSpecificAnalysis">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/TcEsmeraldoSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/TcEsmeraldo</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/TcEsmeraldoLoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/TcEsmeraldoLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/TcEsmeraldoLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/TcEsmeraldoLoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="Tcruzi_BACEnds_loadAndAnalyze" xmlFile="loadAndAnalyzeDnaSeq.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">tritrypdb/tcruzi.xml</paramValue>
      <paramValue name="organismName">TcEsmeraldo</paramValue>
      <paramValue name="genomeExtDbRlsSpec">TcEsmeraldo_chromosomes_RSRC|@@TcEsmeraldo_chromosomes_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">366581</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Tcruzi_BACEnds_RSRC|@@Tcruzi_BACEnds_VER@@</paramValue>
      <paramValue name="BACEndsResourceName">Tcruzi_BACEnds_RSRC</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsFile">$$parentDataDir$$/$$organismName$$LoadGenome/genomicSeqs.fsa</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/$$organismName$$LoadGenome/genomicSeqs</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <subgraph name="dbxRefsResources" xmlFile="tritrypdb/tcruziEsmeraldoDbxRefsResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="euPathUniProtLinkDownloadFile">$$downloadSiteDataDir$$/TcruziEsmeraldoEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name=""/>
  </subgraph>

    <subgraph name="arrayResources" xmlFile="tritrypdb/tcruziEsmeraldoArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
      <depends name=""/>
    </subgraph>

</workflowGraph>



