<workflowGraph name="lmajorFriedlinSpecificAnalysis">
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/LmajorFriedlinSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/LmajorFriedlin</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/LmajorFriedlinLoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/LmajorFriedlinLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/LmajorFriedlinLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/LmajorFriedlinLoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <subgraph name="sangerCosmidEnds_loadAndAnalyze" xmlFile="loadAndAnalyzeBACEnds.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="BACEndsResourceName">Lmajor_sanger_CosmidEnds_RSRC</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Lmajor_sanger_CosmidEnds_RSRC|@@Lmajor_sanger_CosmidEnds_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">5664</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">LmajorFriedlin_chromosomes_RSRC|@@LmajorFriedlin_chromosomes_VER@@</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/LmajorFriedlinLoadGenome/genomicSeqs</paramValue>
      <depends name="mirrorToCluster"/>
    </subgraph>


    <subgraph name="sangerFV1Ends_loadAndAnalyze" xmlFile="loadAndAnalyzeBACEnds.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="BACEndsResourceName">Lmajor_sanger_FV1Ends_RSRC</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Lmajor_sanger_FV1Ends_RSRC|@@Lmajor_sanger_FV1Ends_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">5664</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">LmajorFriedlin_chromosomes_RSRC|@@LmajorFriedlin_chromosomes_VER@@</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/LmajorFriedlinLoadGenome/genomicSeqs</paramValue>
      <depends name="mirrorToCluster"/>
    </subgraph>


    <subgraph name="sangerPACEnds_loadAndAnalyze" xmlFile="loadAndAnalyzeBACEnds.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="BACEndsResourceName">Lmajor_sanger_PACEnds_RSRC</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Lmajor_sanger_PACEnds_RSRC|@@Lmajor_sanger_PACEnds_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">5664</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">LmajorFriedlin_chromosomes_RSRC|@@LmajorFriedlin_chromosomes_VER@@</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/LmajorFriedlinLoadGenome/genomicSeqs</paramValue>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="sangerBACEnds_loadAndAnalyze" xmlFile="loadAndAnalyzeBACEnds.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="BACEndsResourceName">Lmajor_sanger_BACEnds_RSRC</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Lmajor_sanger_BACEnds_RSRC|@@Lmajor_sanger_BACEnds_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">5664</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">LmajorFriedlin_chromosomes_RSRC|@@LmajorFriedlin_chromosomes_VER@@</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/LmajorFriedlinLoadGenome/genomicSeqs</paramValue>
      <depends name="mirrorToCluster"/>
    </subgraph>


    <subgraph name="arrayResources" xmlFile="tritrypdb/lmajorArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
      <paramValue name="genomeExtDbRlsSpec">LmajorFriedlin_chromosomes_RSRC|@@LmajorFriedlin_chromosomes_VER@@</paramValue>
      <depends name="mirrorToCluster"/>
    </subgraph>

    <subgraph name="GOAssociations_LmajorFriedlin_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_LmajorFriedlin_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
