<workflowGraph name="linfantumSpecificAnalysis">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/LinfantumSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDir$$/Linfantum</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/LinfantumLoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/LinfantumLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/LinfantumLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/LinfantumLoadGenome/geneModelForSsa.txt</constant>


  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="dbxRefsResources" xmlFile="tritrypdb/linfantumDbxRefsResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="euPathUniProtLinkDownloadFile">$$downloadSiteDataDir$$/LinfantumEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name=""/>
  </subgraph>

    <subgraph name="arrayResources" xmlFile="tritrypdb/linfantumArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
      <depends name=""/>
    </subgraph>

    <subgraph name="GOAssociations_Linfantum_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_Linfantum_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GO_RSRC"/>
      <dependsGlobal name="GO_evidence_codes_RSRC"/>
    </subgraph>

    <subgraph name="Linfantum_Comments_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Linfantum_Comments_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Linfantum_GeneName_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Linfantum_GeneName_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Linfantum_ECNumbers_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Linfantum_ECNumbers_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDB_RSRC"/>
    </subgraph>


</workflowGraph>



