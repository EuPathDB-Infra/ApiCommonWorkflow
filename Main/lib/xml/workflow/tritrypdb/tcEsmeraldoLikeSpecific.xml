<workflowGraph name="tcEsmeraldoLikeSpecificAnalysis">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/TcEsmeraldoLikeSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/TcEsmeraldoLike</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/TcEsmeraldoLikeLoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/TcEsmeraldoLikeLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/TcEsmeraldoLikeLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/TcEsmeraldoLikeLoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="Tcruzi_BACEnds_loadAndAnalyze" xmlFile="loadDnaSeqAndAlignToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">tritrypdb/tcruzi.xml</paramValue>
      <paramValue name="organismName">TcEsmeraldoLike</paramValue>
      <paramValue name="genomeExtDbRlsSpec">TcEsmeraldoLike_chromosomes_RSRC|@@TcEsmeraldoLike_chromosomes_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">366581</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Tcruzi_BACEnds_RSRC|@@Tcruzi_BACEnds_VER@@</paramValue>
      <paramValue name="BACEndsResourceName">Tcruzi_BACEnds_RSRC</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsFile">$$parentDataDir$$/$$organismName$$LoadGenome/genomicSeqs.fsa</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/$$organismName$$LoadGenome/genomicSeqs</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="arrayResources" xmlFile="tritrypdb/tcEsmeraldoLikeArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
      <depends name=""/>
    </subgraph>

    <subgraph name="GOAssociations_TcEsmeraldoLike_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_TcEsmeraldoLike_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GO_RSRC"/>
      <dependsGlobal name="GO_evidence_codes_RSRC"/>
    </subgraph>

    <subgraph name="TcEsmeraldoLike_Comments_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TcEsmeraldoLike_Comments_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TcEsmeraldoLike_GeneName_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TcEsmeraldoLike_GeneName_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="TcEsmeraldoLike_ECNumbers_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">TcEsmeraldoLike_ECNumbers_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDB_RSRC"/>
    </subgraph>


</workflowGraph>



