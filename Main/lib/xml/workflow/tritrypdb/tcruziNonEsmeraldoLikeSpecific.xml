<workflowGraph name="tcruziNonEsmeraldoLikeSpecificAnalysis">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/TcNonEsmeraldoLikeSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/TcNonEsmeraldoLike</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/TcNonEsmeraldoLikeLoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/TcNonEsmeraldoLikeLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/TcNonEsmeraldoLikeLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/TcNonEsmeraldoLikeLoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="Tcruzi_BACEnds_loadAndAnalyze" xmlFile="loadDnaSeqAndAlignToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">tritrypdb/tcruzi.xml</paramValue>
      <paramValue name="organismName">TcNonEsmeraldoLike</paramValue>
      <paramValue name="genomeExtDbRlsSpec">TcNonEsmeraldoLike_chromosomes_RSRC|@@TcNonEsmeraldoLike_chromosomes_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId"></paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Tcruzi_BACEnds_RSRC|@@Tcruzi_BACEnds_VER@@</paramValue>
      <paramValue name="BACEndsResourceName">Tcruzi_BACEnds_RSRC</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsFile">$$parentDataDir$$/$$organismName$$LoadGenome/genomicSeqs.fsa</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/$$organismName$$LoadGenome/genomicSeqs</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <subgraph name="dbxRefsResources" xmlFile="tritrypdb/tcruziNonEsmeraldoLikeDbxRefsResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="euPathUniProtLinkDownloadFile">$$downloadSiteDataDir$$/TcruziNonEsmeraldoLikeEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name=""/>
  </subgraph>
</workflowGraph>



