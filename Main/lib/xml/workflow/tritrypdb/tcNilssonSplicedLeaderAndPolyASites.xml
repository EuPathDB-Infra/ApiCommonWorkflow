<workflowGraph name="tcNilssonSplicedLeaderAndPolyASites">
    <param name="parentDataDir"/>
    <param name="organismResourceXmlFile"/>
    <param name="genomicSeqsFile"/>
    <param name="genomeExtDbRlsSpec"/>
    <param name="downloadSiteTranscriptExpressionDataDir"/>

    <constant name="dataDir">$$parentDataDir$$/tcNilssonSplicedLeaderAndPolyASites</constant>
    <constant name="samplesDataDir">$$dataDir$$/samples</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

    <step name="makeSamplesDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$samplesDataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <step name="createBowtieIndices" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::CreateBowtieIndexes">
      <paramValue name="inputFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="outputIndexDir">$$dataDir$$/indexes</paramValue>
      <depends name="makeDataDir"/>
    </step>

    <subgraph name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <step name="runBowtie_SRR094782" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
      <paramValue name="inputShortSeqsFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/SRR094782.fasta</paramValue>
      <paramValue name="inputIndexesDir">$$dataDir$$/indexes</paramValue>
      <paramValue name="bowtieParam">-a -v 3 -m 14</paramValue>
      <paramValue name="inputShortSeqsFileType">-f</paramValue>
      <paramValue name="outputFile">$$dataDir$$/SRR094782.bt</paramValue>
      <depends name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC"/>
      <depends name="createBowtieIndices"/>
    </step>

    <step name="runBowtie_SRR094783" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
      <paramValue name="inputShortSeqsFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/SRR094783.fasta</paramValue>
      <paramValue name="inputIndexesDir">$$dataDir$$/indexes</paramValue>
      <paramValue name="bowtieParam">-a -v 3 -m 14</paramValue>
      <paramValue name="inputShortSeqsFileType">-f</paramValue>
      <paramValue name="outputFile">$$dataDir$$/SRR094783.bt</paramValue>
      <depends name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC"/>
      <depends name="createBowtieIndices"/>
    </step>

    <step name="runBowtie_SRR094784" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
      <paramValue name="inputShortSeqsFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/SRR094784.fasta</paramValue>
      <paramValue name="inputIndexesDir">$$dataDir$$/indexes</paramValue>
      <paramValue name="bowtieParam">-a -v 3 -m 14</paramValue>
      <paramValue name="inputShortSeqsFileType">-f</paramValue>
      <paramValue name="outputFile">$$dataDir$$/SRR094784.bt</paramValue>
      <depends name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC"/>
      <depends name="createBowtieIndices"/>
    </step>

    <step name="runBowtie_SRR094785" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
      <paramValue name="inputShortSeqsFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/SRR094785.fasta</paramValue>
      <paramValue name="inputIndexesDir">$$dataDir$$/indexes</paramValue>
      <paramValue name="bowtieParam">-a -v 3 -m 14</paramValue>
      <paramValue name="inputShortSeqsFileType">-f</paramValue>
      <paramValue name="outputFile">$$dataDir$$/SRR094785.bt</paramValue>
      <depends name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC"/>
      <depends name="createBowtieIndices"/>
    </step>

    <step name="runBowtie_SRR094786" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
      <paramValue name="inputShortSeqsFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/SRR094786.fasta</paramValue>
      <paramValue name="inputIndexesDir">$$dataDir$$/indexes</paramValue>
      <paramValue name="bowtieParam">-a -v 3 -m 14</paramValue>
      <paramValue name="inputShortSeqsFileType">-f</paramValue>
      <paramValue name="outputFile">$$dataDir$$/SRR094786.bt</paramValue>
      <depends name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC"/>
      <depends name="createBowtieIndices"/>
    </step>

    <step name="runBowtie_SRR094787" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
      <paramValue name="inputShortSeqsFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/SRR094787.fasta</paramValue>
      <paramValue name="inputIndexesDir">$$dataDir$$/indexes</paramValue>
      <paramValue name="bowtieParam">-a -v 3 -m 14</paramValue>
      <paramValue name="inputShortSeqsFileType">-f</paramValue>
      <paramValue name="outputFile">$$dataDir$$/SRR094787.bt</paramValue>
      <depends name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC"/>
      <depends name="createBowtieIndices"/>
    </step>

    <step name="runBowtie_SRR094788" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
      <paramValue name="inputShortSeqsFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/SRR094788.fasta</paramValue>
      <paramValue name="inputIndexesDir">$$dataDir$$/indexes</paramValue>
      <paramValue name="bowtieParam">-a -v 3 -m 14</paramValue>
      <paramValue name="inputShortSeqsFileType">-f</paramValue>
      <paramValue name="outputFile">$$dataDir$$/SRR094788.bt</paramValue>
      <depends name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC"/>
      <depends name="createBowtieIndices"/>
    </step>

    <step name="runBowtie_SRR094789" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::SsaRunBowtie">
      <paramValue name="inputShortSeqsFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/SRR094789.fasta</paramValue>
      <paramValue name="inputIndexesDir">$$dataDir$$/indexes</paramValue>
      <paramValue name="bowtieParam">-a -v 3 -m 14</paramValue>
      <paramValue name="inputShortSeqsFileType">-f</paramValue>
      <paramValue name="outputFile">$$dataDir$$/SRR094789.bt</paramValue>
      <depends name="Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC"/>
      <depends name="createBowtieIndices"/>
    </step>

     <step name="insertSpliceSiteFeatures" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::InsertSpliceSiteFeatures" stepLoadTypes="plugin">
      <paramValue name="configFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/config.txt</paramValue>
      <paramValue name="inputDir">$$dataDir$$</paramValue>
      <paramValue name="extDbRlsSpec">Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC|@@Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_VER@@</paramValue>
      <paramValue name="type">Splice Site</paramValue>
      <depends name="runBowtie_SRR094782"/>
      <depends name="runBowtie_SRR094783"/>
      <depends name="runBowtie_SRR094784"/>
      <depends name="runBowtie_SRR094785"/>
      <depends name="runBowtie_SRR094786"/>
      <depends name="runBowtie_SRR094787"/>
      <depends name="runBowtie_SRR094788"/>
      <depends name="runBowtie_SRR094789"/>
    </step>

    <step name="runTuningManager"
          stepClass="ApiCommonWorkflow::Main::WorkflowSteps::RunTuningManager">
      <depends name="insertSpliceSiteFeatures"/> 
  </step>

     <step name="extractSpliceSiteGenes" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::ExtractSpliceSiteGenes">
      <paramValue name="configFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/config.txt</paramValue>
      <paramValue name="outputUniqFile">$$dataDir$$/uniqProfile.out</paramValue>
      <paramValue name="outputNonUniqFile">$$dataDir$$/nonUniqProfile.out</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$genomeExtDbRlsSpec$$</paramValue>
      <depends name="runTuningManager"/>
    </step>

    <step name="makeMinPercentileProfile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakePercentileProfile">
      <paramValue name="inputFile">$$dataDir$$/uniqProfile.out</paramValue>
      <paramValue name="outputFile">$$dataDir$$/minPercents.txt</paramValue>
      <paramValue name="ties">min</paramValue>
      <paramValue name="hasHeader">true</paramValue>
      <depends name="extractSpliceSiteGenes"/>
    </step>

    <step name="insertExpressionProfile" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::InsertExpressionProfiles" stepLoadTypes="plugin">
      <paramValue name="configFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/profile_config.txt</paramValue>
      <paramValue name="inputDir">$$dataDir$$</paramValue>
      <paramValue name="extDbRlsSpec">Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC|@@Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_VER@@</paramValue>
      <depends name="makeMinPercentileProfile"/>
    </step>

<step name="copyExpressionProfilesToDownloadDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::CopyExpressionProfilesToDownloadDir">
      <paramValue name="copyFromDir">$$dataDir$$</paramValue>
      <paramValue name="configFile">$$dataDir$$/Tcruzi_RNASeq_Spliced_Leader_And_Poly_A_Sites_Nilsson_RSRC/final/profile_config.txt</paramValue>
      <paramValue name="copyToDir">$$downloadSiteTranscriptExpressionDataDir$$</paramValue>
      <depends name="insertExpressionProfile"/>
</step>

</workflowGraph>
