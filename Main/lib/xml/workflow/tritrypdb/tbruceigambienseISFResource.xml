<workflowGraph name="tbruceigambienseISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/tbrucei427ISFResource</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="Tbruceigambiense_chromosomes_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tbruceigambiense_chromosomes_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Tbruceigambiense_annotations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tbruceigambiense_annotations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Tbruceigambiense_chromosomes_RSRC"/>
  </subgraph>

</workflowGraph>
