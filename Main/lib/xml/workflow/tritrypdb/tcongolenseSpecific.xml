<workflowGraph name="tcongolenseSpecificAnalysis">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/TcongolenseSpecific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDir$$/Tcongolense</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/TcongolenseLoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/TcongolenseLoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/TcongolenseLoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/TcongolenseLoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="dbxRefsResources" xmlFile="tritrypdb/tcongolenseDbxRefsResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="euPathUniProtLinkDownloadFile">$$downloadSiteDataDir$$/TcongolenseEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name=""/>
  </subgraph>

    <subgraph name="GOAssociations_Tcongolense_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_Tcongolense_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GO_RSRC"/>
      <dependsGlobal name="GO_evidence_codes_RSRC"/>
    </subgraph>

    <subgraph name="Tcongolense_Comment_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tcongolense_Comment_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Tcongolense_GeneName_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tcongolense_GeneName_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Tcongolense_ECNumbers_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Tcongolense_ECNumbers_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDB_RSRC"/>
    </subgraph>


</workflowGraph>



