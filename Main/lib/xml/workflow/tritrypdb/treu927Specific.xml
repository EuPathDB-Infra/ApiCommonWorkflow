<workflowGraph name="treu927SpecificAnalysis">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/Treu927Specific</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/Treu927</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/Treu927LoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/Treu927LoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/Treu927LoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/Treu927LoadGenome/geneModelForSsa.txt</constant>


  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <subgraph name="GenbankBACEnds_loadAndAnalyze" xmlFile="loadDnaSeqAndAlignToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">tritrypdb/tbrucei.xml</paramValue>
      <paramValue name="organismName">Treu927</paramValue>
      <paramValue name="genomeExtDbRlsSpec">Treu927_chromosomes_RSRC|@@Treu927_chromosomes_VER@@</paramValue>
      <paramValue name="maxIntronSize">100</paramValue>
      <paramValue name="ncbiTaxonId">185431</paramValue>
      <paramValue name="BACEndsExtDbRlsSpec">Tbrucei_Genbank_BACEnds_RSRC|@@Tbrucei_Genbank_BACEnds_VER@@</paramValue>
      <paramValue name="BACEndsResourceName">Tbrucei_Genbank_BACEnds_RSRC</paramValue>
      <paramValue name="repeatMaskSpecies">-species Trypanosoma</paramValue>
      <paramValue name="genomicSeqsFile">$$parentDataDir$$/Treu927LoadGenome/genomicSeqs.fsa</paramValue>
      <paramValue name="genomicSeqsDir">$$parentDataDir$$/Treu927LoadGenome/genomicSeqs</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <subgraph name="miscResources" xmlFile="tritrypdb/treu927MiscResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="rnaSeqAndChipSeq" xmlFile="tritrypdb/treu927RnaSeqAndChipSeq.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="dbxRefsResources" xmlFile="tritrypdb/treu927DbxRefsResource.xml">
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="euPathUniProtLinkDownloadFile">$$downloadSiteDataDir$$/TbruceiEuPathUniProtLinks_$$projectName$$-$$projectVersion$$.txt</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <depends name="tbrucei_CorrectGeneAliases_RSRC"/>
  </subgraph>

    <subgraph name="arrayResources" xmlFile="tritrypdb/treu927ArrayStudies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
      <depends name=""/>
    </subgraph>

    <subgraph name="SageTagArrayDesign_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">SageTagArrayDesign_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

  <subgraph name="mapSageTags" xmlFile="mapSageTags.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="sageTagExtDbRlsSpec">SageTagArrayDesign_RSRC|@@SageTagArrayDesign_VER@@</paramValue>
    <paramValue name="studyName">Transcriptome analysis of two life stages of Trypanosoma brucei gambiense</paramValue>
    <paramValue name="paramValue">3952450</paramValue>  
    <paramValue name="nickName">mapSageTags</paramValue>   
    <paramValue name="calFreq">true</paramValue>
    <depends name="SageTagArrayDesign_RSRC"/>
  </subgraph>

    <subgraph name="SageTagFreqs_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">SageTagFreqs_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="SageTagArrayDesign_RSRC"/>
    </subgraph>

    <subgraph name="GOAssociations_Treu927_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">GOAssociations_Treu927_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="GO_RSRC"/>
      <dependsGlobal name="GO_evidence_codes_RSRC"/>
    </subgraph>

    <subgraph name="Treu927_Comments_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Treu927_Comments_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Treu927_GeneName_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Treu927_GeneName_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>

    <subgraph name="Treu927_ECNumbers_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">Treu927_ECNumbers_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
      <dependsGlobal name="enzymeDB_RSRC"/>
    </subgraph>

</workflowGraph>



