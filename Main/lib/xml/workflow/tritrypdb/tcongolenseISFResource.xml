<workflowGraph name="tcongolenseISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/tcongolenseISFResource</constant>

    <step name="makeDataDir" stepClass="ReFlow::StepClasses::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="Tcongolense_chromosomes_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tcongolense_chromosomes_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Tcongolense_annotations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tcongolense_annotations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Tcongolense_chromosomes_RSRC"/>
  </subgraph>

</workflowGraph>
