<workflowGraph name="lbraziliensisMassSpecResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/lbraziliensisMassSpecResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="Lbraziliensis_Proteomics_Promastigotes_Secreted_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Lbraziliensis_Proteomics_Promastigotes_Secreted_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>



</workflowGraph>
