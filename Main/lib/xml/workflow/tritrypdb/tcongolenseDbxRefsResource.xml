<workflowGraph name="tcongolenseDbxRefsResource">
      <param name="organismResourceXmlFile"/>
      <param name="parentDataDir"/>
      <param name="euPathUniProtLinkDownloadFile"/>
      <param name="projectName"/>

  <constant name="dataDir">$$parentDataDir$$/tcongolenseDbxRefsResources</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

  <subgraph name="Tcongolense_Entrez_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tcongolense_Entrez_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Tcongolense_Uniprot_Gene_dbxref_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tcongolense_Uniprot_Gene_dbxref_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>

</workflowGraph>
