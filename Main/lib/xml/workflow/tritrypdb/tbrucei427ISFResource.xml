<workflowGraph name="tbrucei427ISFResource">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/tbrucei427ISFResource</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <subgraph name="Tbrucei427_genome_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tbrucei427_genome_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <subgraph name="Tbrucei427_annotations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">Tbrucei427_annotations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="Tbrucei427_genome_RSRC"/>
  </subgraph>

</workflowGraph>
