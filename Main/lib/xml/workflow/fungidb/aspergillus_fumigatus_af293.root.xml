    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="dbESTExtDbRlsSpec">aspergillus_fumigatus_af293_dbest_RSRC|@@aspergillus_fumigatus_af293_DBEST_VER@@</paramValue>
