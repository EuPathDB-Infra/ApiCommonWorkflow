  <!-- ////////////////////////////////////////////////

         Aspergillus nidulans (A4)

       ////////////////////////////////////////////////  -->

  <subgraph name="aspergillus_nidulans_a4_LoadGenome" xmlFile="loadGenome.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/aspergillus_nidulans_a4.xml</paramValue>
    <paramValue name="organismName">aspergillus_nidulans_a4</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="genomeExtDbRlsSpec">aspergillus_nidulans_a4_genome_sequence_RSRC|@@aspergillus_nidulans_a4_GENOME_SEQUENCE_VER@@</paramValue>
    <paramValue name="organismSpecificISFReference">fungidb/aspergillus_nidulans_a4_workflow_ISF.xml</paramValue>
    <paramValue name="ncbiTaxonId">@@aspergillus_nidulans_a4_NCBI_TAXONOMY_ID@@</paramValue>
    <paramValue name="downloadSiteDataDir">downloadSite/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="extractProteins">true</paramValue>
    <depends name="common"/>
    <dependsGlobal name="taxonomy_RSRC"/> 
    <dependsGlobal name="SO_RSRC"/>
  </subgraph>

  <subgraph name="aspergillus_nidulans_a4_MakeAndMaskTopLevelGenome" xmlFile="makeAndMaskTopLevelGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">aspergillus_nidulans_a4</paramValue>
    <paramValue name="organismFullName">@@aspergillus_nidulans_a4_ORGANISM_FULL_NAME@@</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/aspergillus_nidulans_a4.xml</paramValue>
    <paramValue name="genomeExtDbRlsSpec">aspergillus_nidulans_a4_genome_sequence_RSRC|@@aspergillus_nidulans_a4_GENOME_SEQUENCE_VER@@</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFileResourceName"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFile">false</paramValue>
    <paramValue name="repeatMaskSpecies">-species Fungi</paramValue>
    <paramValue name="comparativeGenomicsFastaDir">$$comparativeGenomicsFastaDir$$</paramValue>
    <paramValue name="comparativeGenomicsGffDir">$$comparativeGenomicsGffDir$$</paramValue>
    <depends name="aspergillus_nidulans_a4_LoadGenome"/>
  </subgraph>

  <subgraph name="aspergillus_nidulans_a4_PostLoadGenome" xmlFile="postLoadGenome.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">aspergillus_nidulans_a4</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/aspergillus_nidulans_a4.xml</paramValue>
    <paramValue name="organismFullName">@@aspergillus_nidulans_a4_ORGANISM_FULL_NAME@@</paramValue>
    <paramValue name="organismTwoLetterAbbrev">@@aspergillus_nidulans_a4_ORGANISM_ABBREVIATION@@</paramValue>
    <paramValue name="ncbiTaxonId">@@aspergillus_nidulans_a4_NCBI_TAXONOMY_ID@@</paramValue>
    <paramValue name="parentNcbiTaxonId">@@aspergillus_nidulans_a4_NCBI_PARENT_TAXONOMY_ID@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">aspergillus_nidulans_a4_genome_sequence_RSRC|@@aspergillus_nidulans_a4_GENOME_SEQUENCE_VER@@</paramValue>
   
    <!-- FungiDB: skip ieDB -->
    <paramValue name="loadEiptope">false</paramValue>
    <paramValue name="iedbResourceName"></paramValue>
    <paramValue name="iedbVersion"></paramValue>
    <paramValue name="iedbDbRefResourceName"></paramValue>

    <!-- dbEST -->
    <paramValue name="dbESTResourceName">aspergillus_nidulans_a4_dbest_RSRC</paramValue>
    <paramValue name="dbESTVersion">@@aspergillus_nidulans_a4_DBEST_VER@@</paramValue>

    <paramValue name="usePredictedTranscripts">false</paramValue>
    <paramValue name="predictedTranscriptsExtDbRlsSpec"></paramValue>
    <paramValue name="runtRNAScan">true</paramValue>
    <paramValue name="runExportPred">false</paramValue>
    <paramValue name="maxIntronSize">20000</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">@@aspergillus_nidulans_a4_TAXON_HIERARCHY_FOR_BLASTX@@</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">NRDB_RSRC|@@NRDB_VER@@</paramValue>
    <paramValue name="repeatMaskSpecies">-species Fungi</paramValue>
    <paramValue name="genomeSize">@@aspergillus_nidulans_a4_GENOME_SIZE@@</paramValue>
    <paramValue name="skipProteomeAnalysis">false</paramValue>
    <paramValue name="skipGenomeAnalysis">false</paramValue>
    <paramValue name="mapEpitope">false</paramValue>
    <paramValue name="makeAssemblySeqs">false</paramValue>
    <paramValue name="makeAssemblies">false</paramValue>
    <paramValue name="mapAssemblySeqsAndAssemblies">false</paramValue>

    <!-- Organism-specific EST -->
    <paramValue name="loadOrganismSpecificESTResource">false</paramValue>
    <paramValue name="organismSpecificESTResourcesReference"></paramValue>
    <paramValue name="distanceBetweenStartsForAlignmentsInSameCluster">30000</paramValue>

    <!-- skip
    <paramValue name="loadOrganismSpecificESTResource">true</paramValue>
    <paramValue name="organismSpecificESTResourcesReference">fungidb/aspergillus_nidulans_a4_workflow_EST.xml</paramValue>
    -->

    <!-- Mass Spec -->
    <paramValue name="loadOrganismSpecificMassSpecResource">false</paramValue>
    <paramValue name="organismSpecificMassSpecResourcesReference">fungidb/aspergillus_nidulans_a4_MassSpecResource.xml</paramValue>

    <depends name="aspergillus_nidulans_a4_LoadGenome"/>
  </subgraph>

  <subgraph name="aspergillus_nidulans_a4_workflow" xmlFile="fungidb/aspergillus_nidulans_a4_workflow.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/aspergillus_nidulans_a4.xml</paramValue>
    <paramValue name="downloadSiteDataDir">downloadSite/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="webServicesDataDir">$$webServicesDataDir$$</paramValue>
    <depends name="aspergillus_nidulans_a4_LoadGenome"/>
  </subgraph>

  <subgraph name="aspergillus_nidulans_a4_MakeAndFormatDownloadFiles" xmlFile="makeAndFormatDownloadFiles.xml">
    <paramValue name="downloadSiteDataDir">downloadSite/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="organismName">aspergillus_nidulans_a4</paramValue>
    <paramValue name="organismFullName">@@aspergillus_nidulans_a4_ORGANISM_FULL_NAME@@</paramValue>
    <paramValue name="ncbiTaxonId">@@aspergillus_nidulans_a4_NCBI_TAXONOMY_ID@@</paramValue>
    <paramValue name="parentNcbiTaxonId">@@aspergillus_nidulans_a4_NCBI_PARENT_TAXONOMY_ID@@</paramValue>
    <paramValue name="genomeSource">@@aspergillus_nidulans_a4_GENOME_SOURCE@@</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="dbESTExtDbRlsSpec">aspergillus_nidulans_a4_dbest_RSRC|@@aspergillus_nidulans_a4_DBEST_VER@@</paramValue>
    <paramValue name="webServicesDataDir">$$webServicesDataDir$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">aspergillus_nidulans_a4_genome_sequence_RSRC|@@aspergillus_nidulans_a4_GENOME_SEQUENCE_VER@@</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="minOrfLength">50</paramValue>
    <paramValue name="deprecated">false</paramValue>
    <paramValue name="soTermIdsOrNames">chromosome</paramValue>
    <paramValue name="makeESTDownloadFile">true</paramValue>
    <paramValue name="makeProteomeDownloadFile">true</paramValue>
    <paramValue name="makeGenomicDownloadFile">true</paramValue>
    <depends name="aspergillus_nidulans_a4_PostLoadGenome"/>
    <depends name="aspergillus_nidulans_a4_MakeAndMaskTopLevelGenome"/>
  </subgraph>

