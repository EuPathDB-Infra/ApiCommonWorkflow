<!-- ////////////////////////////////////////////////

    Neurospora crassa (OR74A): organism specific workflow

     ////////////////////////////////////////////////  -->

<workflowGraph name="neurospora_crassa_or74a_workflow">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/neurospora_crassa_or74a_workflow</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/neurospora_crassa_or74a</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/neurospora_crassa_or74a_LoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/neurospora_crassa_or74a_LoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/neurospora_crassa_or74a_LoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/neurospora_crassa_or74a_LoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

  <!-- //////////////////////
             ALIASES
       ////////////////////// -->

       <!-- Primary Aliases -->
       <subgraph name="neurospora_crassa_or74a_primary_gene_aliases_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">neurospora_crassa_or74a_primary_gene_aliases_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
       </subgraph>

       <!-- Other Aliases -->
       <subgraph name="neurospora_crassa_or74a_gene_aliases_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">neurospora_crassa_or74a_gene_aliases_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
       </subgraph>

  <!-- //////////////////////
             DBXREFS
       ////////////////////// -->

       <!-- Uniprot -->
       <subgraph name="neurospora_crassa_or74a_uniprot_dbxrefs_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">neurospora_crassa_or74a_uniprot_dbxrefs_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
       </subgraph>

       <!-- EC IDs -->
       <subgraph name="neurospora_crassa_or74a_ec_ids_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">neurospora_crassa_or74a_ec_ids_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
            <dependsGlobal name="enzymeDB_RSRC"/>
        </subgraph>

        <!-- REFSEQ -->
        <subgraph name="neurospora_crassa_or74a_refseq_dbxrefs_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">neurospora_crassa_or74a_refseq_dbxrefs_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
        </subgraph>



<!-- Isloates: skip
    <subgraph name="mapIsolatesToGenome" xmlFile="fungidb/plasmoMapIsolatesToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismName">neurospora_crassa_or74a</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">neurospora_crassa_or74a_genome_sequence_RSRC|@@neurospora_crassa_or74a_GENOME_SEQUENCE_VER@@</paramValue>
