      <paramValue name="organismName">neurospora_crassa_or74a</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">neurospora_crassa_or74a_genome_sequence_RSRC|@@neurospora_crassa_or74a_GENOME_SEQUENCE_VER@@</paramValue>
      <depends name="mirrorToCluster"/>
      <dependsGlobal name="isolatesResource"/>
    </subgraph>
-->

