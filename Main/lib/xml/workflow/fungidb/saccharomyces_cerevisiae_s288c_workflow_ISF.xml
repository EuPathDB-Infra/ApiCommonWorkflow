<workflowGraph name="saccharomyces_cerevisiae_s288c_ISF">
      <param name="organismResourceXmlFile" />
      <param name="dataDir" />

      <!-- Where the workflow data will go -->
      <constant name="dataDir">
            $$dataDir$$/ISF
      </constant>

      <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
          <paramValue name="dataDir">$$dataDir$$</paramValue>
      </step>

      <!-- ISF fasta sequence -->
      <subgraph 
            name="saccharomyces_cerevisiae_s288c_genomic_sequence_RSRC" 
            xmlFile="loadResource.xml">

            <paramValue name="resourceName">
                saccharomyces_cerevisiae_s288c_genomic_sequence_RSRC
            </paramValue>
    
            <paramValue name="resourceXmlFileName">
                $$organismResourceXmlFile$$
            </paramValue>

            <paramValue name="parentDataDir">
                $$dataDir$$
            </paramValue>
      
            <depends name="makeDataDir"/>
      </subgraph>

      <!-- ISF genomic annotations -->
      <subgraph
            name="saccharomyces_cerevisiae_s288c_genomic_annotations_RSRC"
            xmlFile="loadResource.xml">
   
            <paramValue name="resourceName">
                 saccharomyces_cerevisiae_s288c_genomic_annotations_RSRC
            </paramValue>

            <paramValue name="resourceXmlFileName">
                 $$organismResourceXmlFile$$
            </paramValue>

            <paramValue name="parentDataDir">
                 $$dataDir$$
            </paramValue>
      
            <depends name="saccharomyces_cerevisiae_s288c_genomic_sequence_RSRC"/>
     </subgraph>

</workflowGraph>
