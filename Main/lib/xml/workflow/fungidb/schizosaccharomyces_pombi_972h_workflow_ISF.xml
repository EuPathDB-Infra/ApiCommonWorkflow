<!-- ////////////////////////////////////////////////

       Schizosachharomyces pombe (972h): ISF workflow

    ////////////////////////////////////////////////  -->

<workflowGraph name="schizosaccharomyces_pombi_972h_workflow_ISF">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

    <constant name="dataDir">$$dataDir$$/schizosaccharomyces_pombi_972h_ISF</constant>

    <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
      <paramValue name="dataDir">$$dataDir$$</paramValue>
    </step>

  <!-- genome sequence -->
  <subgraph name="schizosaccharomyces_pombi_972h_genome_sequence_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">schizosaccharomyces_pombi_972h_genome_sequence_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
  </subgraph>

  <!-- genome annotations -->
  <subgraph name="schizosaccharomyces_pombi_972h_genome_annotations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">schizosaccharomyces_pombi_972h_genome_annotations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="schizosaccharomyces_pombi_972h_genome_sequence_RSRC"/>
  </subgraph>

  <!-- Mito: skip
  <subgraph name="schizosaccharomyces_pombi_972h_mito_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">schizosaccharomyces_pombi_972h_mito_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="schizosaccharomyces_pombi_972h_genome_sequence_RSRC"/>
  </subgraph>
  -->

  <!-- Previous aliases: skip
  <subgraph name="schizosaccharomyces_pombi_972h_previous_aliases_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">schizosaccharomyces_pombi_972h_previous_aliases_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="schizosaccharomyces_pombi_972h_genome_annotations_RSRC"/>
      <depends name="schizosaccharomyces_pombi_972h_mito_RSRC"/>
  </subgraph>
  -->

</workflowGraph>
