<!-- ////////////////////////////////////////////////

    Rhizopus oryzae (99-880): organism specific workflow

     ////////////////////////////////////////////////  -->

<workflowGraph name="rhizopus_oryzae_99880_workflow">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/rhizopus_oryzae_99880_workflow</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/rhizopus_oryzae_99880</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/rhizopus_oryzae_99880_LoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/rhizopus_oryzae_99880_LoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/rhizopus_oryzae_99880_LoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/rhizopus_oryzae_99880_LoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

  <!-- //////////////////////
             ALIASES
       ////////////////////// -->

       <!-- Primary Aliases -->
       <subgraph name="rhizopus_oryzae_99880_primary_gene_aliases_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">rhizopus_oryzae_99880_primary_gene_aliases_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
       </subgraph>

       <!-- Other Aliases -->
       <subgraph name="rhizopus_oryzae_99880_gene_aliases_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">rhizopus_oryzae_99880_gene_aliases_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
       </subgraph>

  <!-- //////////////////////
             DBXREFS
       ////////////////////// -->

       <!-- Uniprot -->
       <subgraph name="rhizopus_oryzae_99880_uniprot_dbxrefs_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">rhizopus_oryzae_99880_uniprot_dbxrefs_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
       </subgraph>

       <!-- EC IDs -->
       <subgraph name="rhizopus_oryzae_99880_ec_ids_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">rhizopus_oryzae_99880_ec_ids_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
            <dependsGlobal name="enzymeDB_RSRC"/>
        </subgraph>

        <!-- REFSEQ -->
        <subgraph name="rhizopus_oryzae_99880_refseq_dbxrefs_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">rhizopus_oryzae_99880_refseq_dbxrefs_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
        </subgraph>



<!-- Isloates: skip
    <subgraph name="mapIsolatesToGenome" xmlFile="fungidb/plasmoMapIsolatesToGenome.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismName">rhizopus_oryzae_99880</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">rhizopus_oryzae_99880_genome_sequence_RSRC|@@rhizopus_oryzae_99880_GENOME_SEQUENCE_VER@@</paramValue>
