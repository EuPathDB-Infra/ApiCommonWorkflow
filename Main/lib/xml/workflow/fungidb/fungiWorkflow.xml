    <paramValue name="organismResourceXmlFile">fungidb/cryptococcus_neoformans_grubii_h99.xml</paramValue>
    <depends name="cryptococcus_neoformans_grubii_h99_LoadGenome"/>
  </subgraph>


</workflowGraph>

