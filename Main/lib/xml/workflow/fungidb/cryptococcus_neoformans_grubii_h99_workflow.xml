      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <paramValue name="genomeExtDbRlsSpec">cryptococcus_neoformans_grubii_h99_genome_sequence_RSRC|@@cryptococcus_neoformans_grubii_h99_GENOME_SEQUENCE_VER@@</paramValue>
