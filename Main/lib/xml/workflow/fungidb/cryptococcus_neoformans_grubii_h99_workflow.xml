<workflowGraph name="cryptococcus_neoformans_grubii_h99_workflow">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/cryptococcus_neoformans_grubii_h99_workflow</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/cryptococcus_neoformans_grubii_h99</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/cryptococcus_neoformans_grubii_h99_LoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/cryptococcus_neoformans_grubii_h99_LoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/cryptococcus_neoformans_grubii_h99_LoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/cryptococcus_neoformans_grubii_h99_LoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>

  <!-- //////////////////////
             ALIASES
       ////////////////////// -->

       <!-- Primary Aliases (FungiDB IDs to Original IDs) -->
       <subgraph name="cryptococcus_neoformans_grubii_h99_primary_gene_aliases_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">cryptococcus_neoformans_grubii_h99_primary_gene_aliases_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
       </subgraph>


<!-- Isolates: skip
