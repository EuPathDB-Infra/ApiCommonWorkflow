<workflowGraph name="coccidiodes_immitis_rs_ISF">
      <param name="organismResourceXmlFile"/>
      <param name="dataDir"/>

      <constant name="dataDir">
            $$dataDir$$/ISF
      </constant>

      <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
           <paramValue name="dataDir">$$dataDir$$</paramValue>
      </step>

      <!-- ISF fasta sequence -->
      <subgraph name="coccidiodes_immitis_rs_genomic_sequence_RSRC" xmlFile="loadResource.xml">
           <paramValue name="resourceName">
                 coccidiodes_immitis_rs_genomic_sequence_RSRC
           </paramValue>
           
           <paramValue name="resourceXmlFileName">
                 $$organismResourceXmlFile$$
           </paramValue>
           
           <paramValue name="parentDataDir">
                 $$dataDir$$
           </paramValue>
      
           <depends name="makeDataDir"/>
      </subgraph>

      <!-- ISF genomic annotations -->
      <subgraph name="coccidiodes_immitis_rs_genomic_annotations_RSRC" xmlFile="loadResource.xml">
          <paramValue name="resourceName">
               coccidiodes_immitis_rs_genomic_annotations_RSRC
          </paramValue>

          <paramValue name="resourceXmlFileName">
               $$organismResourceXmlFile$$
          </paramValue>
    
          <paramValue name="parentDataDir">
               $$dataDir$$
          </paramValue>
      
          <depends name="coccidiodes_immitis_rs_genomic_sequence_RSRC"/>
      </subgraph>

</workflowGraph>
