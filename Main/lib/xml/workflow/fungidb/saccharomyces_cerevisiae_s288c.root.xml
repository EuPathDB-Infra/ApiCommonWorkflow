  <!-- ////////////////////////////////////////////////

         Saccharomyces cerevisiae (S288C)

       ////////////////////////////////////////////////  -->

  <subgraph name="saccharomyces_cerevisiae_s288c_LoadGenome" xmlFile="loadGenome.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/saccharomyces_cerevisiae_s288c.xml</paramValue>
    <paramValue name="organismName">saccharomyces_cerevisiae_s288c</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="genomeExtDbRlsSpec">saccharomyces_cerevisiae_s288c_genome_sequence_RSRC|@@saccharomyces_cerevisiae_s288c_GENOME_SEQUENCE_VER@@</paramValue>
    <paramValue name="organismSpecificISFReference">fungidb/saccharomyces_cerevisiae_s288c_workflow_ISF.xml</paramValue>
    <paramValue name="ncbiTaxonId">@@saccharomyces_cerevisiae_s288c_NCBI_TAXONOMY_ID@@</paramValue>
    <paramValue name="downloadSiteDataDir">downloadSite/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="extractProteins">true</paramValue>
    <depends name="common"/>
    <dependsGlobal name="taxonomy_RSRC"/> 
    <dependsGlobal name="SO_RSRC"/>
  </subgraph>

  <subgraph name="saccharomyces_cerevisiae_s288c_MakeAndMaskTopLevelGenome" xmlFile="makeAndMaskTopLevelGenome.xml">
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">saccharomyces_cerevisiae_s288c</paramValue>
    <paramValue name="organismFullName">@@saccharomyces_cerevisiae_s288c_ORGANISM_FULL_NAME@@</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/saccharomyces_cerevisiae_s288c.xml</paramValue>
    <paramValue name="genomeExtDbRlsSpec">saccharomyces_cerevisiae_s288c_genome_sequence_RSRC|@@saccharomyces_cerevisiae_s288c_GENOME_SEQUENCE_VER@@</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFileResourceName"></paramValue>
    <paramValue name="insertVirtualSeqFromAgpFile">false</paramValue>
    <paramValue name="repeatMaskSpecies">-species Fungi</paramValue>
    <paramValue name="comparativeGenomicsFastaDir">$$comparativeGenomicsFastaDir$$</paramValue>
    <paramValue name="comparativeGenomicsGffDir">$$comparativeGenomicsGffDir$$</paramValue>
    <depends name="saccharomyces_cerevisiae_s288c_LoadGenome"/>
  </subgraph>

  <subgraph name="saccharomyces_cerevisiae_s288c_PostLoadGenome" xmlFile="postLoadGenome.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">saccharomyces_cerevisiae_s288c</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/saccharomyces_cerevisiae_s288c.xml</paramValue>
    <paramValue name="organismFullName">@@saccharomyces_cerevisiae_s288c_ORGANISM_FULL_NAME@@</paramValue>
    <paramValue name="organismTwoLetterAbbrev">@@saccharomyces_cerevisiae_s288c_ORGANISM_ABBREVIATION@@</paramValue>
    <paramValue name="ncbiTaxonId">@@saccharomyces_cerevisiae_s288c_NCBI_TAXONOMY_ID@@</paramValue>
    <paramValue name="parentNcbiTaxonId">@@saccharomyces_cerevisiae_s288c_NCBI_PARENT_TAXONOMY_ID@@</paramValue>
    <paramValue name="genomeExtDbRlsSpec">saccharomyces_cerevisiae_s288c_genome_sequence_RSRC|@@saccharomyces_cerevisiae_s288c_GENOME_SEQUENCE_VER@@</paramValue>
   
    <!-- FungiDB: skip ieDB -->
    <paramValue name="loadEiptope">false</paramValue>
    <paramValue name="iedbResourceName"></paramValue>
    <paramValue name="iedbVersion"></paramValue>
    <paramValue name="iedbDbRefResourceName"></paramValue>

    <!-- dbEST -->
    <paramValue name="dbESTResourceName">saccharomyces_cerevisiae_s288c_dbest_RSRC</paramValue>
    <paramValue name="dbESTVersion">@@saccharomyces_cerevisiae_s288c_DBEST_VER@@</paramValue>

    <paramValue name="usePredictedTranscripts">false</paramValue>
    <paramValue name="predictedTranscriptsExtDbRlsSpec"></paramValue>
    <paramValue name="runtRNAScan">true</paramValue>
    <paramValue name="runExportPred">false</paramValue>
    <paramValue name="maxIntronSize">20000</paramValue>
    <paramValue name="taxonHierarchyForBlastxFilter">@@saccharomyces_cerevisiae_s288c_TAXON_HIERARCHY_FOR_BLASTX@@</paramValue>
    <paramValue name="nrdbExtDbRlsSpec">NRDB_RSRC|@@NRDB_VER@@</paramValue>
    <paramValue name="repeatMaskSpecies">-species Fungi</paramValue>
    <paramValue name="genomeSize">@@saccharomyces_cerevisiae_s288c_GENOME_SIZE@@</paramValue>
    <paramValue name="skipProteomeAnalysis">false</paramValue>
    <paramValue name="skipGenomeAnalysis">false</paramValue>
    <paramValue name="mapEpitope">false</paramValue>
    <paramValue name="makeAssemblySeqs">false</paramValue>
    <paramValue name="makeAssemblies">false</paramValue>
    <paramValue name="mapAssemblySeqsAndAssemblies">false</paramValue>

    <!-- Organism-specific EST -->
    <paramValue name="loadOrganismSpecificESTResource">false</paramValue>
    <paramValue name="organismSpecificESTResourcesReference"></paramValue>
    <paramValue name="distanceBetweenStartsForAlignmentsInSameCluster">30000</paramValue>

    <!-- skip
    <paramValue name="loadOrganismSpecificESTResource">true</paramValue>
    <paramValue name="organismSpecificESTResourcesReference">fungidb/saccharomyces_cerevisiae_s288c_workflow_EST.xml</paramValue>
    -->

    <!-- Mass Spec -->
    <paramValue name="loadOrganismSpecificMassSpecResource">false</paramValue>
    <paramValue name="organismSpecificMassSpecResourcesReference">fungidb/saccharomyces_cerevisiae_s288c_MassSpecResource.xml</paramValue>

    <depends name="saccharomyces_cerevisiae_s288c_LoadGenome"/>
  </subgraph>

  <subgraph name="saccharomyces_cerevisiae_s288c_workflow" xmlFile="fungidb/saccharomyces_cerevisiae_s288c_workflow.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/saccharomyces_cerevisiae_s288c.xml</paramValue>
    <paramValue name="downloadSiteDataDir">downloadSite/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="webServicesDataDir">$$webServicesDataDir$$</paramValue>
    <depends name="saccharomyces_cerevisiae_s288c_LoadGenome"/>
  </subgraph>

  <subgraph name="saccharomyces_cerevisiae_s288c_MakeAndFormatDownloadFiles" xmlFile="makeAndFormatDownloadFiles.xml">
    <paramValue name="downloadSiteDataDir">downloadSite/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="organismName">saccharomyces_cerevisiae_s288c</paramValue>
    <paramValue name="organismFullName">@@saccharomyces_cerevisiae_s288c_ORGANISM_FULL_NAME@@</paramValue>
    <paramValue name="ncbiTaxonId">@@saccharomyces_cerevisiae_s288c_NCBI_TAXONOMY_ID@@</paramValue>
    <paramValue name="parentNcbiTaxonId">@@saccharomyces_cerevisiae_s288c_NCBI_PARENT_TAXONOMY_ID@@</paramValue>
    <paramValue name="genomeSource">@@saccharomyces_cerevisiae_s288c_GENOME_SOURCE@@</paramValue>
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="dbESTExtDbRlsSpec">saccharomyces_cerevisiae_s288c_dbest_RSRC|@@saccharomyces_cerevisiae_s288c_DBEST_VER@@</paramValue>
    <paramValue name="webServicesDataDir">$$webServicesDataDir$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">saccharomyces_cerevisiae_s288c_genome_sequence_RSRC|@@saccharomyces_cerevisiae_s288c_GENOME_SEQUENCE_VER@@</paramValue>
    <paramValue name="genomeVirtualSeqsExtDbRlsSpec"></paramValue>
    <paramValue name="minOrfLength">50</paramValue>
    <paramValue name="deprecated">false</paramValue>
    <paramValue name="soTermIdsOrNames">chromosome</paramValue>
    <paramValue name="makeESTDownloadFile">true</paramValue>
    <paramValue name="makeProteomeDownloadFile">true</paramValue>
    <paramValue name="makeGenomicDownloadFile">true</paramValue>
    <depends name="saccharomyces_cerevisiae_s288c_PostLoadGenome"/>
    <depends name="saccharomyces_cerevisiae_s288c_MakeAndMaskTopLevelGenome"/>
  </subgraph>

