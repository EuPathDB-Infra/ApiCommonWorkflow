    <paramValue name="downloadSiteDataDir">downloadSite/$$projectName$$/release-$$projectVersion$$</paramValue>
    <paramValue name="organismName">saccharomyces_cerevisiae_s288c_Mito</paramValue>
    <paramValue name="organismFullName">@@saccharomyces_cerevisiae_s288c_ORGANISM_FULL_NAME@@</paramValue>
