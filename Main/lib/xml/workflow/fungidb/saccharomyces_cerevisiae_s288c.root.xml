    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="dbESTExtDbRlsSpec">saccharomyces_cerevisiae_s288c_dbest_RSRC|@@saccharomyces_cerevisiae_s288c_DBEST_VER@@</paramValue>
    <paramValue name="webServicesDataDir">$$webServicesDataDir$$</paramValue>
    <paramValue name="genomeExtDbRlsSpec">saccharomyces_cerevisiae_s288c_genome_sequence_RSRC|@@saccharomyces_cerevisiae_s288c_GENOME_SEQUENCE_VER@@</paramValue>
