    </step>

    <step name="saccharomyces_cerevisiae_s288c_MitoMakeDownloadSiteTranscriptExpressionDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeApiSiteFilesDir">
      <paramValue name="apiSiteFilesDir">downloadSite/$$projectName$$/release-$$projectVersion$$/saccharomyces_cerevisiae_s288c_Mito/transcriptExpression</paramValue>
    </step>

