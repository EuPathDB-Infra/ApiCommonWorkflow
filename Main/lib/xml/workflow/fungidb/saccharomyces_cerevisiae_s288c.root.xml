    <depends name="saccharomyces_cerevisiae_s288c_PostLoadGenome"/>
    <depends name="saccharomyces_cerevisiae_s288c_MakeAndMaskTopLevelGenome"/>
    <depends name="saccharomyces_cerevisiae_s288c_MitoMakeDownloadSiteFastaDataDir"/>
    <depends name="saccharomyces_cerevisiae_s288c_MitoMakeDownloadSiteTabularDataDir"/>
    <depends name="saccharomyces_cerevisiae_s288c_MitoMakeDownloadSiteGffDataDir"/>
    <depends name="saccharomyces_cerevisiae_s288c_MitoMakeDownloadSiteTranscriptExpressionDataDir"/>
  </subgraph>


