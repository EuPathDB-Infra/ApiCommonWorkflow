<!-- ////////////////////////////////////////////////

    Saccharomyces cerevisiae (S288C): organism specific workflow

     ////////////////////////////////////////////////  -->

<workflowGraph name="saccharomyces_cerevisiae_s288c_workflow">
  <param name="projectName"/>
  <param name="projectVersion"/>
  <param name="parentDataDir"/>
  <param name="organismResourceXmlFile"/>
  <param name="downloadSiteDataDir"/>
  <param name="webServicesDataDir"/>

  <constant name="dataDir">$$parentDataDir$$/saccharomyces_cerevisiae_s288c_workflow</constant>
  <constant name="downloadSiteDataDir">$$downloadSiteDataDir$$/saccharomyces_cerevisiae_s288c</constant>
  <constant name="genomicSeqsFile">$$parentDataDir$$/saccharomyces_cerevisiae_s288c_LoadGenome/genomicSeqs.fsa</constant>
  <constant name="genomicSeqsFileForSsa">$$parentDataDir$$/saccharomyces_cerevisiae_s288c_LoadGenome/genomicSeqsForSsa.fsa</constant>
  <constant name="transcriptSeqsFileForSsa">$$parentDataDir$$/saccharomyces_cerevisiae_s288c_LoadGenome/transcriptSeqsForSsa.fsa</constant>
  <constant name="ssaGeneModelFile">$$parentDataDir$$/saccharomyces_cerevisiae_s288c_LoadGenome/geneModelForSsa.txt</constant>

  <step name="makeDataDir" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MakeDataDir">
    <paramValue name="dataDir">$$dataDir$$</paramValue>
  </step>

    <step name="mirrorToCluster" stepClass="ApiCommonWorkflow::Main::WorkflowSteps::MirrorToComputeCluster">
      <paramValue name="fileOrDirToMirror">$$dataDir$$</paramValue>
      <depends name="makeDataDir"/>
    </step>


  <!-- //////////////////////
             ALIASES
       ////////////////////// -->

       <!-- Primary Aliases -->
       <!--
       <subgraph name="saccharomyces_cerevisiae_s288c_primary_gene_aliases_RSRC" xmlFile="loadResource.xml">
            <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_primary_gene_aliases_RSRC</paramValue>
            <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
            <paramValue name="parentDataDir">$$dataDir$$</paramValue>
            <depends name="makeDataDir"/>
       </subgraph>
        -->


  <!-- //////////////////////
             DBXREFS
       ////////////////////// -->

       <!-- EC IDs -->
       <subgraph name="saccharomyces_cerevisiae_s288c_ec_ids_RSRC" xmlFile="loadResource.xml">
          <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_ec_ids_RSRC</paramValue>
          <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
          <paramValue name="parentDataDir">$$dataDir$$</paramValue>
          <depends name="makeDataDir"/>
          <dependsGlobal name="enzymeDB_RSRC"/>
      </subgraph>

      <!-- Uniprot -->
      <subgraph name="saccharomyces_cerevisiae_s288c_uniprot_dbxrefs_RSRC" xmlFile="loadResource.xml">
          <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_uniprot_dbxrefs_RSRC</paramValue>
          <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
          <paramValue name="parentDataDir">$$dataDir$$</paramValue>
          <depends name="makeDataDir"/>
      </subgraph>

      <!-- Candida -->
      <subgraph name="saccharomyces_cerevisiae_s288c_candida_dbxrefs_RSRC" xmlFile="loadResource.xml">
          <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_candida_dbxrefs_RSRC</paramValue>
          <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
          <paramValue name="parentDataDir">$$dataDir$$</paramValue>
          <depends name="makeDataDir"/>
      </subgraph>

      <!-- Refseq: Protein IDs: resources.xml probably needs to be updated to attach these to protein IDs -->
      <subgraph name="saccharomyces_cerevisiae_s288c_refseq_dbxrefs_RSRC" xmlFile="loadResource.xml">
          <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_refseq_dbxrefs_RSRC</paramValue>
          <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
          <paramValue name="parentDataDir">$$dataDir$$</paramValue>
          <depends name="makeDataDir"/>
      </subgraph>

      <!-- Refseq -->
      <subgraph name="saccharomyces_cerevisiae_s288c_refseq_na_dbxrefs_RSRC" xmlFile="loadResource.xml">
          <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_refseq_na_dbxrefs_RSRC</paramValue>
          <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
          <paramValue name="parentDataDir">$$dataDir$$</paramValue>
          <depends name="makeDataDir"/>
      </subgraph>

      <!-- Refseq Gene -->
      <subgraph name="saccharomyces_cerevisiae_s288c_refseq_gene_dbxrefs_RSRC" xmlFile="loadResource.xml">
          <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_refseq_gene_dbxrefs_RSRC</paramValue>
          <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
          <paramValue name="parentDataDir">$$dataDir$$</paramValue>
          <depends name="makeDataDir"/>
      </subgraph>

      <!-- Metacyc -->
      <subgraph name="saccharomyces_cerevisiae_s288c_metacyc_dbxrefs_RSRC" xmlFile="loadResource.xml">
          <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_metacyc_dbxrefs_RSRC</paramValue>
          <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
          <paramValue name="parentDataDir">$$dataDir$$</paramValue>
          <depends name="makeDataDir"/>
      </subgraph>


<!-- SNPs: skip
    <subgraph name="snps" xmlFile="fungidb/saccharomyces_cerevisiae_s288c_SNPs.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
      <depends name="makeDataDir"/>
    </subgraph>
-->

<!-- Correct gene aliases: skip
    <subgraph name="saccharomyces_cerevisiae_s288c_correct_gene_aliases_RSRC" xmlFile="loadResource.xml">
      <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_correct_gene_aliases_RSRC</paramValue>
      <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <depends name="saccharomyces_cerevisiae_s288c_genedb_ids_RSRC"/>
    </subgraph>
-->

<!-- Microarray: skip
    <subgraph name="arrayResources" xmlFile="fungidb/saccharomyces_cerevisiae_s288c_array_studies.xml">
      <paramValue name="parentDataDir">$$dataDir$$</paramValue>
      <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
      <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
      <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
      <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
      <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
      <depends name="saccharomyces_cerevisiae_s288c_correct_gene_aliases_RSRC"/>
    </subgraph>
-->

<!-- RNASeq/ChipSeq: skip for now
  <subgraph name="saccharomyces_cerevisiae_s288c_rna_seq_and_chip_seq" xmlFile="fungidb/saccharomyces_cerevisiae_s288c_rna_seq_and_chip_seq.xml">
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <paramValue name="organismResourceXmlFile">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="ssaGeneModelFile">$$ssaGeneModelFile$$</paramValue>
    <paramValue name="genomicSeqsFile">$$genomicSeqsFile$$</paramValue>
    <paramValue name="genomicSeqsFileForSsa">$$genomicSeqsFileForSsa$$</paramValue>
    <paramValue name="transcriptSeqsFileForSsa">$$transcriptSeqsFileForSsa$$</paramValue>
    <paramValue name="downloadSiteTranscriptExpressionDataDir">$$downloadSiteDataDir$$/transcriptExpression</paramValue>
    <depends name="makeDataDir"/>
  </subgraph>
-->


  <!-- Gene Ontology -->
  <subgraph name="saccharomyces_cerevisiae_s288c_go_associations_RSRC" xmlFile="loadResource.xml">
    <paramValue name="resourceName">saccharomyces_cerevisiae_s288c_go_associations_RSRC</paramValue>
    <paramValue name="resourceXmlFileName">$$organismResourceXmlFile$$</paramValue>
    <paramValue name="parentDataDir">$$dataDir$$</paramValue>
    <depends name="makeDataDir"/>
    <dependsGlobal name="GO_RSRC"/>
    <dependsGlobal name="GO_evidence_codes_RSRC"/>
  </subgraph>


