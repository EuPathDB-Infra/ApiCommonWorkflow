  <subgraph name="candida_albicans_sc5314_LoadGenome" xmlFile="loadGenome.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/candida_albicans_sc5314.xml</paramValue>
    <paramValue name="organismName">candida_albicans_sc5314</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="genomeExtDbRlsSpec">candida_albicans_sc5314_genome_sequence_RSRC|@@candida_albicans_sc5314_GENOME_SEQUENCE_VER@@</paramValue>
