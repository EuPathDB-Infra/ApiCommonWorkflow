  <subgraph name="phanerochaete_chrysosporium_rp-78_PostLoadGenome" xmlFile="postLoadGenome.xml">
    <paramValue name="projectName">$$projectName$$</paramValue>
    <paramValue name="projectVersion">$$projectVersion$$</paramValue>
    <paramValue name="parentDataDir"></paramValue>
    <paramValue name="organismName">phanerochaete_chrysosporium_rp-78</paramValue>
    <paramValue name="organismResourceXmlFile">fungidb/phanerochaete_chrysosporium_rp-78.xml</paramValue>
