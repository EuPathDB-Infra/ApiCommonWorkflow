<workflowGraph name="makeAndFormatDownloadFiles">
    <param name="relativeDownloadSiteDir"/>
    <param name="relativeWebServicesDir"/>
    <param name="organismAbbrev"/>
    <param name="genomeSource"/>
    <param name="projectName"/>
    <param name="projectVersion"/>
    <param name="genomeExtDbRlsSpec"/>
    <param name="minOrfLength"/>
    <param name="hasDeprecatedGenes"/>
    <param name="genomeTopLevelSoTerms"/>
    <param name="isAnnotatedGenome"/>
    <param name="skipGenomeAnalysis"/>
    <param name="hasMitochondrialGenomeSequence"/>
    <param name="hasPlastidGenomeSequence"/>


    <subgraph name="$$organismAbbrev$$_MakeNuclearWebsiteFiles" xmlFile="makeOrganismWebsiteFiles.xml">
      <paramValue name="relativeDownloadSiteDir">$$relativeDownloadSiteDir$$</paramValue>
      <paramValue name="relativeWebServicesDir">$$relativeWebServicesDir$$</paramValue>
      <paramValue name="organismAbbrev">$$organismAbbrev$$</paramValue>
      <paramValue name="genomeSource">$$genomeSource$$</paramValue>
      <paramValue name="projectName">$$projectName$$</paramValue>
      <paramValue name="projectVersion">$$projectVersion$$</paramValue>
      <paramValue name="dbESTExtDbRlsSpec">$$organismAbbrev$$_dbEST_RSRC|@@$$organismAbbrev$$_dbEST_VER@@</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$organismAbbrev$$_primary_genome_RSRC|$$genomeVersion$$</paramValue>
      <paramValue name="minOrfLength">50</paramValue>
      <paramValue name="cellularLocationSoTerms">chromosome,supercontig,contig</paramValue>
      <paramValue name="isAnnotatedGenome">$$isAnnotatedGenome$$</paramValue>
      <paramValue name="skipGenomeAnalysis">$$skipGenomeAnalysis$$</paramValue>
      <paramValue name="hasDeprecatedGenes">$$hasDeprecatedGenes$$</paramValue>
    </subgraph>

    <subgraph name="$$organismAbbrev$$_MakeMitoWebsiteFiles" xmlFile="makeOrganismWebsiteFiles.xml" includeIf="$$hasMitochondrialGenomeSequence$$">
      <paramValue name="relativeDownloadSiteDir">$$relativeDownloadSiteDir$$</paramValue>
      <paramValue name="relativeWebServicesDir">$$relativeWebServicesDir$$</paramValue>
      <paramValue name="organismAbbrev">$$organismAbbrev$$</paramValue>
      <paramValue name="genomeSource">$$genomeSource$$</paramValue>
      <paramValue name="projectName">$$projectName$$</paramValue>
      <paramValue name="projectVersion">$$projectVersion$$</paramValue>
      <paramValue name="dbESTExtDbRlsSpec">$$organismAbbrev$$_dbEST_RSRC|@@$$organismAbbrev$$_dbEST_VER@@</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$organismAbbrev$$_primary_genome_RSRC|$$genomeVersion$$</paramValue>
      <paramValue name="minOrfLength">50</paramValue>
      <paramValue name="cellularLocationSoTerms">mitochondrial_chromosome</paramValue>
      <paramValue name="isAnnotatedGenome">$$isAnnotatedGenome$$</paramValue>
      <paramValue name="skipGenomeAnalysis">$$skipGenomeAnalysis$$</paramValue>
      <paramValue name="hasDeprecatedGenes">$$hasDeprecatedGenes$$</paramValue>
    </subgraph>

    <subgraph name="$$organismAbbrev$$_MakePlastidWebsiteFiles" xmlFile="makeOrganismWebsiteFiles.xml" includeIf="$$hasPlastidGenomeSequence$$">
      <paramValue name="relativeDownloadSiteDir">$$relativeDownloadSiteDir$$</paramValue>
      <paramValue name="relativeWebServicesDir">$$relativeWebServicesDir$$</paramValue>
      <paramValue name="organismAbbrev">$$organismAbbrev$$</paramValue>
      <paramValue name="genomeSource">$$genomeSource$$</paramValue>
      <paramValue name="projectName">$$projectName$$</paramValue>
      <paramValue name="projectVersion">$$projectVersion$$</paramValue>
      <paramValue name="dbESTExtDbRlsSpec">$$organismAbbrev$$_dbEST_RSRC|@@$$organismAbbrev$$_dbEST_VER@@</paramValue>
      <paramValue name="genomeExtDbRlsSpec">$$organismAbbrev$$_primary_genome_RSRC|$$genomeVersion$$</paramValue>
      <paramValue name="minOrfLength">50</paramValue>
      <paramValue name="cellularLocationSoTerms">apicoplast_chromosome</paramValue>
      <paramValue name="isAnnotatedGenome">$$isAnnotatedGenome$$</paramValue>
      <paramValue name="skipGenomeAnalysis">$$skipGenomeAnalysis$$</paramValue>
      <paramValue name="hasDeprecatedGenes">$$hasDeprecatedGenes$$</paramValue>
    </subgraph>
</workflowGraph>
