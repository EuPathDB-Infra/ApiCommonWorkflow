  <datasetClass class="dbxref_gene2Entrez" graphPlanFile="dbXRefs.xml">
    <prop name="project"/>
    <prop name="orgAbbrev"/>
    <prop name="ncbiTaxId"/>
    <prop name="parentResource"/>

    <graphFile planFile="dbXRefs.xml" dir="${project}/${orgAbbrev}"/>

