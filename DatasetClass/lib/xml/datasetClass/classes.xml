  <!-- Category: genome -->

  <datasetClass class="genbank_primary_genome">
    <prop name="project"/>
    <prop name="orgAbbrev"/>
    <prop name="ncbiTaxId"/>
    <prop name="name"/>
    <prop name="version"/>
    <prop name="soTerm"/>
    <prop name="mapFile"/>

    <graphFile planFile="genomeSequenceAndFeatures.xml" dir="${project}/${orgAbbrev}"/>

    <resource resource="${orgAbbrev}_${soTerm}_${name}_RSRC"
              version="${version}"
	      plugin="GUS::Supported::Plugin::InsertSequenceFeatures">

      <manualGet fileOrDir="${project}/${orgAbbrev}/genome/${soTerm}_${name}_genbank/%RESOURCE_VERSION%/final/genome.gbf"/>

      <pluginArgs>--extDbName %RESOURCE_NAME% --extDbRlsVer %RESOURCE_VERSION% --mapFile @gusHome@/lib/xml/isf/${mapFile} --inputFileOrDir @@dataDir@@/genome.gbf --fileFormat genbank --soCvsVersion @@SO_VER@@ --organism ${ncbiTaxId} --seqSoTerm ${soTerm} --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    </resource>
  </datasetClass>

  <datasetClass class="genbank_secondary_genome">
    <prop name="project"/>
    <prop name="orgAbbrev"/>
    <prop name="ncbiTaxId"/>
    <prop name="name"/>
    <prop name="version"/>
    <prop name="soTerm"/>
    <prop name="mapFile"/>
    <prop name="parentDataset"/>

     <graphFile planFile="genomeSequenceAndFeatures.xml" dir="${project}/${orgAbbrev}"/>

    <resource resource="${orgAbbrev}_${soTerm}_${name}_RSRC"
              version="${version}"
	      plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
	      parentResource="${parentDataset}">

      <manualGet fileOrDir="${project}/${orgAbbrev}/genome/${soTerm}_${name}_genbank/%RESOURCE_VERSION%/final/genome.gbf"/>

      <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @gusHome@/lib/xml/isf/${mapFile} --inputFileOrDir @@dataDir@@/genome.gbf --fileFormat genbank --soCvsVersion @@SO_VER@@ --organism ${ncbiTaxId} --seqSoTerm ${soTerm} --bioperlTreeOutput @@dataDir@@/bioperlTree.out</pluginArgs>
    </resource>
  </datasetClass>

  <datasetClass class="GeneDB_GFF_genome_sequence">
    <prop name="project"/>
    <prop name="orgAbbrev"/>
    <prop name="ncbiTaxId"/>
    <prop name="version"/>
    <prop name="soTerm"/>
    <prop name="regex"/>

    <graphFile planFile="genomeSequenceAndFeatures.xml" dir="${project}/${orgAbbrev}"/>

    <resource resource="${orgAbbrev}_${soTerm}_GeneDB_RSRC"
              version="${version}"
	      plugin="GUS::Supported::Plugin::LoadFastaSequences">

      <manualGet fileOrDir="${project}/${orgAbbrev}/genome/GeneDB_GFF/%RESOURCE_VERSION%/final"/>

      <unpack>makeCustomFastaAndGffFromGff3 --input_dir @@dataDir@@/%RESOURCE_VERSION%/final --output_fasta @@dataDir@@/genome.fasta --output_gff @@dataDir@@/genome.gff3</unpack>

      <unpack>preprocessGFF3 --input_gff @@dataDir@@/genome.gff3 --output_gff @@dataDir@@/genome.gff</unpack>

      <pluginArgs>--externalDatabaseName %RESOURCE_NAME% --ncbiTaxId ${ncbiTaxId} --externalDatabaseVersion %RESOURCE_VERSION% --sequenceFile @@dataDir@@/genome.fasta --SOTermName '${soTerm}'  --regexSourceId  '${regex}' --tableName "DoTS::ExternalNASequence"</pluginArgs>
    </resource>
  </datasetClass>


  <datasetClass class="GeneDB_GFF_genome_features">
    <prop name="project"/>
    <prop name="orgAbbrev"/>
    <prop name="ncbiTaxId"/>
    <prop name="version"/>
    <prop name="soTerm"/>
    <prop name="parentResource"/>

    <graphFile planFile="genomeSequenceAndFeatures.xml" dir="${project}/${orgAbbrev}"/>

    <resource resource="${orgAbbrev}_${soTerm}_GeneDB_features_RSRC"
              version="${version}"
	      parentResource="${parentResource}"
	      plugin="GUS::Supported::Plugin::InsertSequenceFeatures"
	      internalDescrip="">

      <manualGet fileOrDir="global/empty"/>

      <pluginArgs>--extDbName %PARENT_RESOURCE_NAME% --extDbRlsVer %PARENT_RESOURCE_VERSION% --mapFile @@gusHome@@/lib/xml/isf/geneDBGFF2Gus.xml --inputFileOrDir @@dataDir@@/../${parentResource}/final/genome.gff --inputFileExtension "gff"  --fileFormat gff3 --soCvsVersion @@SO_VER@@ --defaultOrganism ${ncbiTaxId}  --seqSoTerm '${soTerm}' --validationLog @@dataDir@@/validation.log --bioperlTreeOutput @@dataDir@@/bioperlTree.out --seqIdColumn source_id --naSequenceSubclass ExternalNASequence</pluginArgs>
    </resource>
  </datasetClass>

  <datasetClass class="genome_virtual_sequence_agp">
    <prop name="project"/>
    <prop name="orgAbbrev"/>
    <prop name="ncbiTaxId"/>
    <prop name="version"/>
    <prop name="parentDataset"/>
    <prop name="virtualSeqSOTerm"/>
    <prop name="sourceIdPrefix"/>

    <resource resource="genome_virtual_sequence_agp_RSRC"
              version="${version}"
	      plugin="ApiCommonData::Load::Plugin::InsertVirtualSeqFromAgpFile"
	      parentResource="${parentDataset}">
      
      <manualGet fileOrDir="${project}/${orgAbbrev}/genome/${virtualSeqSOTerm}_assembly_agp/%RESOURCE_VERSION%/final/assembly.agp"/>

      <pluginArgs>--agpFile @@dataDir@@/assembly.agp --seqPieceExtDbName %PARENT_RESOURCE_NAME% --seqPieceExtDbRlsVer %PARENT_RESOURCE_VERSION% --virSeqExtDbName %RESOURCE_NAME% --virSeqExtDbRlsVer %RESOURCE_VERSION% --virtualSeqSOTerm ${virtualSeqSOTerm}  --ncbiTaxId ${ncbiTaxId} --soVer @@SO_VER@@ --sourceIdPrefix ${sourceIdPrefix}</pluginArgs>
    </resource>
